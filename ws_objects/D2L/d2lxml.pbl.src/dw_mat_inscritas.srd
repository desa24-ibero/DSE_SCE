 retrieve="  SELECT mat_inscritas.cuenta,   
         mat_inscritas.cve_mat,   
         mat_inscritas.gpo,   
         mat_inscritas.periodo,   
         mat_inscritas.anio,   
         mat_inscritas.calificacion,   
         mat_inscritas.cve_condicion,   
         mat_inscritas.acreditacion,   
         mat_inscritas.inscripcion, 
		'A' as tipo_grupo   
    FROM mat_inscritas   
WHERE mat_inscritas.periodo = :periodo
AND  mat_inscritas.anio  = :anio  
UNION
  SELECT mat_inscritas.no_folio_inter as cuenta,   
         mat_inscritas.cve_mat,   
         mat_inscritas.gpo,   
         mat_inscritas.periodo,   
         mat_inscritas.anio,   
         mat_inscritas.calificacion,   
         0 AS cve_condicion,   
         0 AS acreditacion,   
         '' AS inscripcion, 
		'I' as tipo_grupo     
    FROM sumuia_bd.dbo.dca_mat_inscritas mat_inscritas  
WHERE mat_inscritas.periodo = :periodo
AND  mat_inscritas.anio  = :anio  







" update="mat_inscritas" updatewhere=1 updatekeyinplace=no arguments=(("periodo", number),("anio", number)) )
