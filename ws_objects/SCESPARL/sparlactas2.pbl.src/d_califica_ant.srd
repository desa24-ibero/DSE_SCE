 retrieve="  SELECT dbo.alumnos.nombre_completo,   
         dbo.mat_inscritas.calificacion,   
         dbo.mat_inscritas.cuenta,   
         dbo.mat_inscritas.cve_mat,   
         dbo.mat_inscritas.gpo,   
         dbo.mat_inscritas.periodo,   
         dbo.mat_inscritas.anio,   
         dbo.materias.evaluacion  
    FROM dbo.mat_inscritas,   
         dbo.alumnos,   
         dbo.materias  
   WHERE ( dbo.mat_inscritas.cuenta = dbo.alumnos.cuenta ) and  
         ( dbo.mat_inscritas.cve_mat = dbo.materias.cve_mat ) and  
         ( ( dbo.mat_inscritas.anio = :anio ) AND  
         ( dbo.mat_inscritas.periodo = :periodo ) AND  
         ( dbo.mat_inscritas.cve_mat = :materia ) AND  
         ( dbo.mat_inscritas.gpo = :grupo )  AND  
         ( isnull(dbo.mat_inscritas.calificacion,~"~") <> ~"BA~") AND 
         ( dbo.mat_inscritas.cve_condicion IN (0,3) ) )
" update="dbo.mat_inscritas" updatewhere=1 updatekeyinplace=yes arguments=(("anio", number),("periodo", number),("materia", number),("grupo", string))  sort="alumnos_nombre_completo A " )
