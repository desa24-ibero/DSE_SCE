 column=(type=char(70) updatewhereclause=yes name=nombre_completo dbname="alumnos.nombre_completo" )
 column=(type=long updatewhereclause=yes name=cve_carrera dbname="academicos.cve_carrera" )
 column=(type=char(100) updatewhereclause=yes name=carrera dbname="carreras.carrera" )
 column=(type=long updatewhereclause=yes name=cve_plan dbname="academicos.cve_plan" )
 column=(type=char(20) updatewhereclause=yes name=nombre_plan dbname="nombre_plan.nombre_plan" )
 column=(type=long updatewhereclause=yes name=baja_laboratorio dbname="banderas.baja_laboratorio" )
 retrieve="  SELECT alumnos.cuenta,   
			sumuia_bd.dbo.v_sce_alumno_digito.digito,
         alumnos.nombre_completo,   
         academicos.cve_carrera,   
         carreras.carrera,   
         academicos.cve_plan,   
         nombre_plan.nombre_plan,   
         banderas.baja_laboratorio  
    FROM academicos,   
         alumnos,   
         banderas,   
         carreras,   
         nombre_plan,
			sumuia_bd.dbo.v_sce_alumno_digito, 
		plan_estudios
   WHERE ( alumnos.cuenta = academicos.cuenta ) and  
			( alumnos.cuenta = sumuia_bd.dbo.v_sce_alumno_digito.cuenta ) and  
         ( banderas.cuenta = alumnos.cuenta ) and  
         ( academicos.cve_carrera = carreras.cve_carrera ) and  
         ( academicos.cve_plan = nombre_plan.cve_plan ) and  
         ( ( banderas.baja_laboratorio = 1 ) )    and 
		academicos.cve_plan = plan_estudios.cve_plan and 
		academicos.cve_carrera = plan_estudios.cve_carrera and 
		plan_estudios.tipo_periodo = :tipo_periodo
	ORDER BY academicos.cve_carrera, alumnos.cuenta" arguments=(("tipo_periodo", string)) )
