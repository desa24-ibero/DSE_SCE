 column=(type=char(1) updatewhereclause=yes key=yes name=cve_indulto dbname="hist_indulto.cve_indulto" dbalias=".cve_indulto" values="3 Reprobadas	3/4 Inscripciones	4/Promedio	P/Inglés	I/" )
 column=(type=long updatewhereclause=yes key=yes name=periodo dbname="hist_indulto.periodo" dbalias=".periodo" values="Primavera	0/Verano	1/Otoño	2/" )
 column=(type=long updatewhereclause=yes key=yes name=anio dbname="hist_indulto.anio" dbalias=".anio" )
 retrieve="  SELECT hist_indulto.cuenta,   
         hist_indulto.cve_indulto,   
         hist_indulto.periodo,   
         hist_indulto.anio  
    FROM hist_indulto  
   WHERE ( hist_indulto.anio = :an_anio ) AND  
         (hist_indulto.periodo in ( :array_periodo) ) AND  
         ('9999' = :tipo_indulto)   AND 
		hist_indulto.cve_indulto <> 'I'
UNION 
  SELECT hist_indulto.cuenta,   
         hist_indulto.cve_indulto,   
         hist_indulto.periodo,   
         hist_indulto.anio  
    FROM hist_indulto   
   WHERE ( hist_indulto.anio = :an_anio ) AND  
         (hist_indulto.periodo in ( :array_periodo) ) AND  
         ('I' = hist_indulto.cve_indulto)  AND 
          ( EXISTS(SELECT * FROM historico 
         		WHERE historico.cuenta = hist_indulto.cuenta 
         		AND historico.cve_mat IN(SELECT cve_mat FROM materias_requisito WHERE id_prerrequisito = 'ING') 
				AND historico.calificacion NOT IN ('AC','6','7',~"8~",~"9~",~"10~", ~"MB~",~"B~",~"S~",~"E~",~"RE~"))
		OR 
		NOT EXISTS(SELECT * FROM historico 
         		WHERE historico.cuenta = hist_indulto.cuenta 
         		AND historico.cve_mat IN(SELECT cve_mat FROM materias_requisito WHERE id_prerrequisito = 'ING'))
		)
ORDER BY hist_indulto.anio ASC,   
         hist_indulto.periodo ASC,   
         hist_indulto.cuenta ASC,   
         hist_indulto.cve_indulto ASC   " update="hist_indulto" updatewhere=0 updatekeyinplace=no filter=" anio >=2009"arguments=(("an_anio", number),("array_periodo", numberlist),("tipo_indulto", string)) )
