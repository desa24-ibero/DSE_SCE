 retrieve="  SELECT ui_bicamnot.cuenta,   
         ui_bicamnot.cve_movimiento,   
         mov_bicamnot.movimiento,   
         ui_bicamnot.cve_mat,   
         ui_bicamnot.gpo,   
         ui_bicamnot.periodo,   
         ui_bicamnot.anio,   
         ui_bicamnot.calificacion,   
         ui_bicamnot.calificacion_ant,   
         ui_bicamnot.fecha_hora,   
         ui_bicamnot.usuario,   
         materias.materia,   
         coordinaciones.cve_coordinacion,   
         coordinaciones.coordinacion,
			alumnos.nombre_completo,
			academicos.cve_carrera
    FROM ui_bicamnot,   
         mov_bicamnot,   
         materias,   
         coordinaciones,
			alumnos,
			academicos  
   WHERE ( ui_bicamnot.cve_movimiento = mov_bicamnot.cve_movimiento ) and  
         ( ui_bicamnot.cve_mat = materias.cve_mat ) and  
         ( materias.cve_coordinacion = coordinaciones.cve_coordinacion ) AND
         ( ui_bicamnot.fecha_hora Between :Fecha_Ini And dateadd(day, 1,:Fecha_Fin)) And
         ( ui_bicamnot.cve_movimiento in (0)) and
         ( ui_bicamnot.cuenta = alumnos.cuenta ) and
         ( academicos.cuenta = alumnos.cuenta ) and
         ( ui_bicamnot.periodo = :ai_periodo or
           :ai_periodo = 99 ) and
         ( ui_bicamnot.anio = :ai_anio or
           :ai_anio = 9999 )
 ORDER BY ui_bicamnot.cuenta
" arguments=(("Fecha_Ini", datetime),("Fecha_Fin", datetime),("ai_periodo", number),("ai_anio", number))  sort="ui_bicamnot_anio A ui_bicamnot_periodo A coordinaciones_cve_coordinacion A ui_bicamnot_cve_mat A ui_bicamnot_gpo A " )
