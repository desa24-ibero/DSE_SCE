 retrieve="  SELECT mat_inscritas.cuenta,   
         mat_inscritas.cve_mat,   
         mat_inscritas.gpo,   
         mat_inscritas.periodo,   
         mat_inscritas.anio,   
         alumnos.nombre_completo,   
         coordinaciones.cve_coordinacion,   
         coordinaciones.coordinacion,   
         materias.materia,   
         mat_inscritas.calificacion,   
         actas_evaluacion.status  
    FROM alumnos,   
         coordinaciones,   
         mat_inscritas,   
         materias,   
         actas_evaluacion,   
         grupos  
   WHERE ( alumnos.cuenta = mat_inscritas.cuenta ) and  
         ( materias.cve_mat = mat_inscritas.cve_mat ) and  
         ( coordinaciones.cve_coordinacion = materias.cve_coordinacion ) and  
         ( actas_evaluacion.cve_mat = mat_inscritas.cve_mat ) and  
         ( actas_evaluacion.gpo = mat_inscritas.gpo ) and  
         ( grupos.cve_mat = mat_inscritas.cve_mat ) and  
         ( grupos.gpo = mat_inscritas.gpo ) and  
         ( ( mat_inscritas.periodo = :periodo ) AND  
         ( mat_inscritas.anio = :year ) AND  
			( grupos.periodo =:periodo ) AND
			( grupos.anio = :year ) AND
         ( mat_inscritas.cve_condicion = 0 )) AND  
         ( actas_evaluacion.status in ( :status )  ) AND  
         ( ( mat_inscritas.cve_mat in (:mats) ) OR  
         ( grupos.cve_mat_gpo in (:matsgpos) ) OR  
         (coordinaciones.cve_coordinacion in (:deptos) ) )    
" update="actas_evaluacion" updatewhere=0 updatekeyinplace=yes arguments=(("periodo", number),("year", number),("status", numberlist),("deptos", numberlist),("mats", numberlist),("matsgpos", stringlist))  sort="coordinaciones_cve_coordinacion A mat_inscritas_cve_mat A mat_inscritas_gpo A alumnos_nombre_completo A " )
