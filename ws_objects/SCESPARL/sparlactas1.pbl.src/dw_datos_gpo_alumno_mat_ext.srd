 retrieve="  SELECT alumnos.cuenta,   
         alumnos.nombre_completo,   
         coordinaciones.coordinacion,   
         materias.materia,   
         materias.nivel,   
         examen_extrao_titulo.cve_mat,   
         examen_extrao_titulo.gpo,   
         examen_extrao_titulo.periodo,   
         examen_extrao_titulo.anio,   
         ~"POR DESIGNAR~",   
         coordinaciones.cve_coordinacion,
			materias.evaluacion/*,		
			actas_evaluacion.no_acta*/ 
    FROM alumnos,   
         coordinaciones,   
         examen_extrao_titulo,   
         materias/*,
		   actas_evaluacion  */
   WHERE ( coordinaciones.cve_coordinacion = materias.cve_coordinacion ) and  
         ( materias.cve_mat = examen_extrao_titulo.cve_mat ) and  
         ( alumnos.cuenta = examen_extrao_titulo.cuenta ) and  
         /*( materias.cve_mat = actas_evaluacion.cve_mat) AND
			( grupos.gpo = actas_evaluacion.gpo ) AND*/
         ( ( examen_extrao_titulo.periodo = :periodo ) AND  
         ( examen_extrao_titulo.anio = :year ) AND  
         ( examen_extrao_titulo.tipo_examen = 2) AND  
         ( materias.nivel = :nivel ) ) AND
			(( materias.creditos > 0 ) OR 
			( materias.cve_mat NOT IN (SELECT cve_lab FROM teoria_lab))) 

" arguments=(("periodo", number),("year", number),("nivel", string))  sort="coordinaciones_cve_coordinacion A mat_inscritas_cve_mat A mat_inscritas_gpo A alumnos_nombre_completo A " )
