 column=(type=long updatewhereclause=yes name=cve_concepto dbname="v_pagos_inscripcion.cve_concepto" )
 column=(type=datetime updatewhereclause=yes name=fecha_vencimiento dbname="v_pagos_inscripcion.fecha_vencimiento" )
 column=(type=number updatewhereclause=yes name=saldo dbname="v_pagos_inscripcion.saldo" )
 retrieve="  SELECT dbo.v_pagos_inscripcion.cuenta,   
         dbo.v_pagos_inscripcion.periodo,   
         dbo.v_pagos_inscripcion.anio,   
         dbo.v_pagos_inscripcion.cve_concepto,   
         dbo.v_pagos_inscripcion.fecha_vencimiento,   
         dbo.v_pagos_inscripcion.saldo  
    FROM dbo.v_pagos_inscripcion  
   WHERE ( dbo.v_pagos_inscripcion.anio = :anio ) AND  
         ( dbo.v_pagos_inscripcion.periodo = :periodo ) AND  
         ( dbo.v_pagos_inscripcion.cve_concepto in (3,4,5,6,7,30) ) AND  
         ( dbo.v_pagos_inscripcion.saldo <= 0 )    
" arguments=(("anio", number),("periodo", number)) )
