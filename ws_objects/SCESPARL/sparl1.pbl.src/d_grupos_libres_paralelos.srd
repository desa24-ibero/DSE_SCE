 column=(type=long updatewhereclause=yes name=horario_cve_dia dbname="horario.cve_dia" values="DOMINGO	0/LUNES	1/MARTES	2/MIERCOLES	3/JUEVES	4/VIERNES	5/SABADO	6/DOMINGO	7/" )
 column=(type=long updatewhereclause=yes name=horario_hora_inicio dbname="horario.hora_inicio" )
 column=(type=long updatewhereclause=yes name=horario_hora_final dbname="horario.hora_final" )
 column=(type=long updatewhereclause=yes name=grupos_cupo dbname="grupos.cupo" )
 column=(type=long updatewhereclause=yes name=grupos_inscritos dbname="grupos.inscritos" )
 retrieve="  SELECT grupos.cve_mat,   
         materias.materia,   
         grupos.gpo,   
         horario.cve_dia,   
         horario.hora_inicio,   
         horario.hora_final,   
         grupos.cupo,   
         grupos.inscritos  
    FROM grupos,   
         horario,   
         materias  
   WHERE ((
			( grupos.cve_mat *= horario.cve_mat) and  
         ( grupos.gpo *= horario.gpo)) or
			(
			( grupos.cve_asimilada *= horario.cve_mat) and  
         ( grupos.gpo_asimilado *= horario.gpo))) and 
         ( grupos.cupo > grupos.inscritos ) and  
         ( grupos.cve_mat = materias.cve_mat ) and  
         ( ( grupos.anio = :year  ) AND  
         ( grupos.periodo = :Periodo ) AND  
         ( grupos.cve_mat = :mat ) AND  
         ( horario.anio = :year ) AND  
         ( horario.periodo = :Periodo ) ) AND
			( grupos.cond_gpo = 1 ) AND
         ( grupos.gpo <>~"ZZ~" ) 

   
" arguments=(("mat", number),("Periodo", number),("year", number))  sort="grupos_gpo A horario_cve_dia A horario_hora_inicio A horario_hora_final A " )
