 column=(type=long updatewhereclause=yes name=grupos_sim_cupo dbname="grupos_sim.cupo" )
 column=(type=long updatewhereclause=yes name=grupos_sim_inscritos dbname="grupos_sim.inscritos" )
 retrieve="  SELECT grupos_sim.cve_mat,   
         materias.materia,   
         grupos_sim.gpo,   
         horario_sim.cve_dia,   
         horario_sim.hora_inicio,   
         horario_sim.hora_final,   
         grupos_sim.cupo,   
         grupos_sim.inscritos  
    FROM grupos_sim,   
         horario_sim,   
         materias  
   WHERE ((
			( grupos_sim.cve_mat *= horario_sim.cve_mat) and  
         ( grupos_sim.gpo *= horario_sim.gpo)) or
			(
			( grupos_sim.cve_asimilada *= horario_sim.cve_mat) and  
         ( grupos_sim.gpo_asimilado *= horario_sim.gpo))) and 
         ( grupos_sim.cupo > grupos_sim.inscritos ) and  
         ( grupos_sim.cve_mat = materias.cve_mat ) and  
         ( ( grupos_sim.anio = :year  ) AND  
         ( grupos_sim.periodo = :Periodo ) AND  
         ( grupos_sim.cve_mat = :mat ) AND  
         ( horario_sim.anio = :year ) AND  
         ( horario_sim.periodo = :Periodo ) ) AND
			( grupos_sim.cond_gpo = 1 )
   
" arguments=(("mat", number),("Periodo", number),("year", number)) )
