 column=(type=char(42) updatewhereclause=yes name=materias_materia dbname="materias.materia" )
 column=(type=char(2) updatewhereclause=yes name=grupos_sim_gpo dbname="grupos_sim.gpo" )
 column=(type=long updatewhereclause=yes name=horario_sim_cve_dia dbname="horario_sim.cve_dia" )
 column=(type=long updatewhereclause=yes name=horario_sim_hora_inicio dbname="horario_sim.hora_inicio" )
