 retrieve="  SELECT grupos.cve_mat,   
         grupos.gpo,   
         grupos.cupo,   
         grupos.inscritos,   
         horario.cve_dia,   
         horario.hora_inicio,   
         horario.hora_final,   
         horario.cve_salon,   
         horario.cve_mat,   
         horario.gpo,   
         horario.periodo,   
         horario.anio,   
         dias.dia,   
         materias.materia,
			materias.nivel,   
         salon.cupo_max  
    FROM grupos,   
         horario,   
         dias,   
         materias,   
         salon  
   WHERE ( horario.cve_mat = grupos.cve_mat ) and  
         ( horario.gpo = grupos.gpo ) and  
         ( horario.periodo = grupos.periodo ) and  
         ( horario.anio = grupos.anio ) and  
         ( horario.cve_dia = dias.cve_dia ) and  
         ( materias.cve_mat = grupos.cve_mat ) and  
         ( horario.cve_salon *= salon.cve_salon ) and  
         ( ( grupos.anio = :a_anio ) AND  
         ( grupos.periodo = :a_periodo ) AND  
         ( horario.clase_aula = 0 ) AND  
         ( salon.clase_aula = ~"SALON~" ) AND
			( grupos.cve_mat = :a_cve_mat ) AND
			( grupos.gpo = :a_gpo ) )" update="horario" updatewhere=0 updatekeyinplace=yes arguments=(("a_anio", number),("a_periodo", number),("a_cve_mat", number),("a_gpo", string))  sort="diferencia D grupos_cve_mat A grupos_gpo A horario_cve_dia A horario_hora_inicio A " )
