table(column=(type=char(1) updatewhereclause=yes name=edificio dbname="edificio" )
 column=(type=char(1) update=yes updatewhereclause=yes name=nivel dbname="nivel" )
 column=(type=char(16) update=yes updatewhereclause=yes name=cve_salon dbname="cve_salon" )
 column=(type=decimal(6) update=yes updatewhereclause=yes name=cupo_max dbname="cupo_max" )
 column=(type=char(1) updatewhereclause=yes name=grado dbname="grado" )
 retrieve="
SELECT slns.edificio, slns.nivel, slns.cve_salon, cupo_max = ROUND((slns.cupo_max * (salones_edificio_conf.porcentaje_ocupa/100.00)), 0), slns.grado 
FROM ( SELECT edificio = LEFT(cve_salon, 1), cve_salon, cupo_max, dia = 0, hora = 0, fechahora = 0, estatus = '', nivel = substring(cve_salon,2,1), grado = salon.nivel
  FROM salon
 WHERE clase_aula = 'SALON' AND bloqueado = 0) slns, salones_edificio_conf  
WHERE slns.edificio = salones_edificio_conf.edificio 
AND slns.nivel = salones_edificio_conf.nivel 
" )
