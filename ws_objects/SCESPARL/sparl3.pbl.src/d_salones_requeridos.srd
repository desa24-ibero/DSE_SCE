table(column=(type=long updatewhereclause=yes name=grupos_cve_mat dbname="grupos.cve_mat" )
 column=(type=char(2) updatewhereclause=yes name=grupos_gpo dbname="grupos.gpo" )
 column=(type=long updatewhereclause=yes name=grupos_cupo dbname="grupos.cupo" )
 column=(type=long updatewhereclause=yes name=grupos_inscritos dbname="grupos.inscritos" )
 column=(type=char(16) updatewhereclause=yes name=salon_cve_salon dbname="salon.cve_salon" )
 column=(type=long updatewhereclause=yes name=salon_cupo_max dbname="salon.cupo_max" )
 retrieve="  SELECT grupos.cve_mat,   
         grupos.gpo,   
         grupos.cupo,   
         grupos.inscritos,
			salon.cve_salon,
			salon.cupo_max
    FROM horario,   
         grupos,   
         salon,   
         materias  
   WHERE ( grupos.cve_mat = horario.cve_mat ) and  
         ( grupos.gpo = horario.gpo ) and  
         ( grupos.periodo = horario.periodo ) and  
         ( grupos.anio = horario.anio ) and  
         ( horario.cve_salon = salon.cve_salon ) and  
         ( materias.cve_mat = grupos.cve_mat ) and 
			( horario.clase_aula = 0 ) and 
         ( ( horario.anio = :anio ) AND  
         ( horario.periodo = :periodo ) AND  
         ( materias.nivel LIKE :nivel ) AND  
         ( horario.cve_dia = :cve_dia ) AND  
         ( horario.hora_inicio = :hora_inicio ) AND  
         ( horario.hora_final = :hora_final ) AND  
         ( grupos.inscritos between :insc_min AND :insc_max ) AND  
         (grupos.inscritos + :Diferencia < salon.cupo_max) )  
UNION
  SELECT grupos.cve_mat,   
         grupos.gpo,   
         grupos.cupo,   
         grupos.inscritos,
			NULL,
			NULL
    FROM horario,   
         grupos,   
         materias  
   WHERE ( grupos.cve_mat = horario.cve_mat ) and  
         ( grupos.gpo = horario.gpo ) and  
         ( grupos.periodo = horario.periodo ) and  
         ( grupos.anio = horario.anio ) and  
         ( materias.cve_mat = grupos.cve_mat ) and 
			( horario.clase_aula = 0 ) and 
         ( ( horario.anio = :anio ) AND  
         ( horario.periodo = :periodo ) AND  
         ( materias.nivel LIKE :nivel ) AND  
         ( horario.cve_dia = :cve_dia ) AND  
         ( horario.hora_inicio = :hora_inicio ) AND  
         ( horario.hora_final = :hora_final ) AND  
         ( grupos.inscritos between :insc_min AND :insc_max ) AND  
         ( horario.cve_salon IS NULL) )  " arguments=(("anio", number),("periodo", number),("nivel", string),("cve_dia", number),("hora_inicio", number),("hora_final", number),("insc_min", number),("insc_max", number),("Diferencia", number)) )
