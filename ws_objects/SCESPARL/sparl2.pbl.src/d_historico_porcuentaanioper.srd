 retrieve="  SELECT dbo.historico.cuenta,   
         dbo.historico.cve_mat,   
         dbo.historico.gpo,   
         dbo.historico.periodo,   
         dbo.historico.anio,   
         dbo.historico.cve_carrera,   
         dbo.historico.cve_plan,   
         dbo.historico.calificacion,   
         dbo.historico.observacion,   
         dbo.materias.materia  
    FROM dbo.historico,   
         dbo.materias  
   WHERE ( dbo.historico.cve_mat = dbo.materias.cve_mat ) and  
         ( ( dbo.historico.cuenta = :cuenta ) and 
			  ( dbo.historico.anio = :anio ) and
			  ( dbo.historico.periodo = :periodo ) )    
" update="dbo.historico" updatewhere=0 updatekeyinplace=yes arguments=(("cuenta", number),("anio", number),("periodo", number)) )
