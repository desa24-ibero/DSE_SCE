 retrieve="  SELECT actas_evaluacion.cve_mat,   
         actas_evaluacion.gpo,   
         actas_evaluacion.no_acta,   
         actas_evaluacion.inscritos,   
         materias.materia,   
         coordinaciones.cve_coordinacion,   
         coordinaciones.coordinacion,   
         actas_evaluacion.status  
    FROM actas_evaluacion,   
         coordinaciones,   
         materias  
   WHERE ( materias.cve_coordinacion = coordinaciones.cve_coordinacion ) and  
		( materias.tipo_periodo = :tipo_periodo ) AND
         ( actas_evaluacion.cve_mat = materias.cve_mat ) and  
          ( actas_evaluacion.status = 0 ) 
" update="actas_evaluacion" updatewhere=0 updatekeyinplace=yes arguments=(("tipo_periodo", string))  sort="coordinaciones_cve_coordinacion A coordinaciones_coordinacion A actas_evaluacion_no_acta A " )
