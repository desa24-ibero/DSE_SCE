 retrieve="SELECT cuenta = ac.cuenta, nivel = ac.nivel, preincrito = 1, 
creditos_cursados = ac.creditos_cursados, sem_cursados = ac.sem_cursados, promedio = ac.promedio, nombre_completo = al.nombre_completo
FROM academicos ac, cuentas_cortes cc, alumnos al
WHERE ac.egresado = 0
AND ac.cuenta = al.cuenta
AND ac.cuenta in (select cuenta from preinsc)
AND ac.cuenta = cc.cuenta
UNION
SELECT cuenta = ac.cuenta, nivel = ac.nivel, preincrito = 0, 
creditos_cursados = ac.creditos_cursados, sem_cursados = ac.sem_cursados, promedio = ac.promedio, nombre_completo = al.nombre_completo 
FROM academicos ac, cuentas_cortes cc, alumnos al
WHERE ac.egresado = 0
AND ac.cuenta = al.cuenta
AND ac.cuenta not in (select cuenta from preinsc)
AND ac.cuenta = cc.cuenta
UNION
SELECT cuenta = hr.cuenta, nivel = ac.nivel, preincrito = 1, 
creditos_cursados = ac.creditos_cursados, sem_cursados = ac.sem_cursados, promedio = ac.promedio, nombre_completo = al.nombre_completo 
FROM hist_reingreso hr, academicos ac, alumnos al
WHERE hr.cuenta = ac.cuenta
AND ac.cuenta = al.cuenta
AND ac.egresado = 0
AND hr.periodo_ing = :an_periodo
AND hr.anio_ing = :an_anio
AND ac.cuenta IN (select cuenta from preinsc)
AND ac.cuenta NOT in (select cuenta from cuentas_cortes)
UNION
SELECT cuenta = hr.cuenta, nivel = ac.nivel, preincrito = 0, 
creditos_cursados = ac.creditos_cursados, sem_cursados = ac.sem_cursados, promedio = ac.promedio, nombre_completo = al.nombre_completo
FROM hist_reingreso hr, academicos ac, alumnos al
WHERE hr.cuenta = ac.cuenta
AND ac.cuenta = al.cuenta
AND ac.egresado = 0
AND hr.periodo_ing = :an_periodo
AND hr.anio_ing = :an_anio
AND ac.cuenta NOT in (select cuenta from preinsc)
AND ac.cuenta NOT in (select cuenta from cuentas_cortes)
UNION
SELECT cuenta = ar.cuenta, nivel = ac.nivel,  preincrito = 0, 
creditos_cursados = ac.creditos_cursados, sem_cursados = ac.sem_cursados, promedio = ac.promedio, nombre_completo = al.nombre_completo  
FROM aspirantes_revalidacion ar, academicos ac, alumnos al
WHERE ar.cuenta = ac.cuenta
AND ac.cuenta = al.cuenta
AND ac.egresado = 0
AND ar.periodo_ing = :an_periodo
AND ar.anio_ing = :an_anio
AND ac.cuenta NOT in (select cuenta from preinsc)
AND ac.cuenta NOT in (select cuenta from cuentas_cortes)
UNION
SELECT cuenta = ar.cuenta, nivel = ac.nivel,  preincrito = 1, 
creditos_cursados = ac.creditos_cursados, sem_cursados = ac.sem_cursados, promedio = ac.promedio, nombre_completo = al.nombre_completo  
FROM aspirantes_revalidacion ar, academicos ac, alumnos al
WHERE ar.cuenta = ac.cuenta
AND ac.cuenta = al.cuenta
AND ac.egresado = 0
AND ar.periodo_ing = :an_periodo
AND ar.anio_ing = :an_anio
AND ac.cuenta IN (select cuenta from preinsc)
AND ac.cuenta NOT in (select cuenta from cuentas_cortes) 
