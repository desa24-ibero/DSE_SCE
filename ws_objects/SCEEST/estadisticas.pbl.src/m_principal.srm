end type

on m_listados.create
this.Text="Listados"
this.m_alumnos=create m_alumnos
this.m_acadmicos=create m_acadmicos
this.m_preparatorias=create m_preparatorias
this.m_banderas=create m_banderas
this.m_documentos=create m_documentos
this.Item[]={this.m_alumnos, &
this.m_acadmicos, &
this.m_preparatorias, &
this.m_banderas, &
this.m_documentos}
end on

on m_listados.destroy
destroy(this.m_alumnos)
destroy(this.m_acadmicos)
destroy(this.m_preparatorias)
destroy(this.m_banderas)
destroy(this.m_documentos)
end on

