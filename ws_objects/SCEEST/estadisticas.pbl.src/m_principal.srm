global m_principal m_principal

type variables

end variables

on m_principal.create
m_principal=this
this.m_listados=create m_listados
this.m_especial=create m_especial
this.Item[]={this.m_listados, &
this.m_especial}
end on

on m_principal.destroy
destroy(this.m_listados)
destroy(this.m_especial)
end on

