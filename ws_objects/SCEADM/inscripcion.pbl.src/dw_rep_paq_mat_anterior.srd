 column=(type=char(100) updatewhereclause=yes name=carrera dbname="v_carreras.carrera" )
 column=(type=long updatewhereclause=yes name=cve_mat dbname="v_grupos.cve_mat" )
 column=(type=char(2) updatewhereclause=yes name=gpo dbname="v_grupos.gpo" )
 column=(type=long updatewhereclause=yes name=v_grupos_cupo dbname="v_grupos.cupo" )
 retrieve="  SELECT paquetes.num_paq,   
         v_carreras.cve_carrera,   
         v_carreras.carrera,   
         v_grupos.cve_mat,   
         v_grupos.gpo,
         v_grupos.cupo
    FROM paquetes,   
         v_carreras,   
         v_grupos,   
         v_materias  
   WHERE ( v_grupos.cve_mat = v_materias.cve_mat ) AND  
         ( v_grupos.primer_sem = paquetes.num_paq) AND
         ( paquetes.clv_carr = v_carreras.cve_carrera ) AND  
         ( ( v_grupos.periodo = :per ) AND  
         ( v_grupos.anio = :anio ) )   
ORDER BY paquetes.num_paq ASC,   
         v_carreras.cve_carrera ASC,   
         v_grupos.gpo ASC,   
         v_grupos.cve_mat ASC   
" arguments=(("per", number),("anio", number))  sort="num_paq A cve_carrera A gpo A cve_mat A " )
