 column=(type=long update=yes updatewhereclause=yes name=general_cuenta dbname="general.cuenta" )
 column=(type=char(50) updatewhereclause=yes name=general_apaterno dbname="general.apaterno" )
 column=(type=char(50) updatewhereclause=yes name=general_amaterno dbname="general.amaterno" )
 column=(type=char(50) updatewhereclause=yes name=general_nombre dbname="general.nombre" )
 column=(type=long updatewhereclause=yes key=yes name=general_folio dbname="general.folio" )
 column=(type=long updatewhereclause=yes key=yes name=general_clv_ver dbname="general.clv_ver" )
 column=(type=long updatewhereclause=yes key=yes name=general_clv_per dbname="general.clv_per" )
 column=(type=long updatewhereclause=yes key=yes name=general_anio dbname="general.anio" )
 column=(type=long updatewhereclause=yes name=aspiran_status dbname="aspiran.status" )
 retrieve="  SELECT aspiran.folio,   
         general.cuenta,   
         general.apaterno,   
         general.amaterno,   
         general.nombre,   
         general.folio,   
         general.clv_ver,   
         general.clv_per,   
         general.anio,
         aspiran.status
    FROM aspiran,   
         general  
   WHERE general.folio = aspiran.folio and  
         general.clv_ver = aspiran.clv_ver and  
         general.clv_per = aspiran.clv_per and  
         general.anio = aspiran.anio and  
         aspiran.clv_per = :per AND  
         aspiran.anio = :ani AND  
         general.ya_inscri = 0 AND  
         general.cuenta = 0 AND
         ( ( :opcion = 0 AND aspiran.status = 1 ) OR
           ( :opcion = 1 AND ( aspiran.status = 1 OR aspiran.status = 4 ) ) )

" update="general" updatewhere=1 updatekeyinplace=yes arguments=(("per", number),("ani", number),("opcion", number))  sort="general_apaterno A general_amaterno A general_nombre A " )
