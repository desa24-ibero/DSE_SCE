 column=(type=char(2) updatewhereclause=yes name=paquetes_materias_grupo dbname="paquetes_materias.grupo" )
 retrieve="  SELECT paquetes.num_paq,   
         v_carreras.cve_carrera,   
         v_carreras.carrera,   
         paquetes_materias.clv_mat,   
         paquetes_materias.grupo
    FROM paquetes,   
         v_carreras,   
         paquetes_materias,   
         v_materias  
   WHERE ( paquetes_materias.clv_mat = v_materias.cve_mat ) AND  
         ( paquetes_materias.num_paq = paquetes.num_paq) AND
         ( paquetes.clv_carr = v_carreras.cve_carrera ) 
ORDER BY paquetes.num_paq ASC,   
         v_carreras.cve_carrera ASC,   
         paquetes_materias.grupo ASC,   
         paquetes_materias.clv_mat ASC   
"  sort="num_paq A cve_carrera A " )
