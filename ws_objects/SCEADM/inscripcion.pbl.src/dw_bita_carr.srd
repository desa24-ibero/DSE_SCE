 column=(type=char(8) update=yes updatewhereclause=yes name=usuario dbname="bita_carr.usuario" )
 column=(type=long update=yes updatewhereclause=yes key=yes name=folio dbname="bita_carr.folio" )
 column=(type=long update=yes updatewhereclause=yes key=yes name=clv_ver dbname="bita_carr.clv_ver" )
 column=(type=long update=yes updatewhereclause=yes key=yes name=clv_per dbname="bita_carr.clv_per" )
 column=(type=long update=yes updatewhereclause=yes key=yes name=anio dbname="bita_carr.anio" )
 retrieve="  SELECT bita_carr.fecha,   
         bita_carr.carr_ant,   
         bita_carr.carr_act,   
         bita_carr.usuario,   
         bita_carr.folio,   
         bita_carr.clv_ver,   
         bita_carr.clv_per,   
         bita_carr.anio  
    FROM bita_carr  
   WHERE ( :fol = 1 OR  
         bita_carr.folio = :fol ) AND  
         ( :ver=99 or bita_carr.clv_ver = :ver ) AND  
         ( bita_carr.clv_per = :per ) AND  
         ( bita_carr.anio = :anio )    
" update="bita_carr" updatewhere=1 updatekeyinplace=yes arguments=(("fol", number),("ver", number),("per", number),("anio", number)) )
