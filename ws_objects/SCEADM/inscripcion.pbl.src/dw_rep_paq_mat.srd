 retrieve="SELECT  dbo.paquetes.num_paq ,
           dbo.v_carreras.cve_carrera ,
           dbo.v_carreras.carrera ,
           dbo.paquetes_materias.clv_mat ,
           dbo.paquetes_materias.grupo ,
           dbo.paquetes.cupo ,
           dbo.v_grupos.cupo ,     
           dbo.v_grupos.inscritos   
        FROM dbo.paquetes ,
           dbo.v_carreras ,
           dbo.paquetes_materias ,
           dbo.v_materias ,
           dbo.v_grupos     
        WHERE ( dbo.paquetes_materias.clv_mat = dbo.v_materias.cve_mat ) and          ( dbo.paquetes_materias.num_paq = dbo.paquetes.num_paq ) and          ( dbo.paquetes.clv_carr = dbo.v_carreras.cve_carrera ) and          ( dbo.v_grupos.cve_mat = dbo.paquetes_materias.clv_mat ) and          ( dbo.v_grupos.gpo = dbo.paquetes_materias.grupo ) and          ( dbo.v_grupos.periodo = dbo.paquetes_materias.clv_per ) and          ( dbo.v_grupos.anio = dbo.paquetes_materias.anio ) and          ( dbo.paquetes_materias.clv_per = :ai_periodo ) and          ( dbo.paquetes_materias.anio = :ai_anio )  
        ORDER BY dbo.paquetes.num_paq          ASC,
           dbo.v_carreras.cve_carrera          ASC,
           dbo.paquetes_materias.clv_mat          ASC,
           dbo.paquetes_materias.grupo          ASC  " arguments=(("ai_periodo", number),("ai_anio", number))  sort="num_paq A cve_carrera A paquetes_materias_clv_mat A " )
