 retrieve="  SELECT general.cuenta,   
         aspiran.clv_carr,   
         padres.nombre,   
         padres.apaterno,   
         padres.amaterno,   
         padres.calle,   
         padres.codigo_pos,   
         padres.colonia,   
         padres.estado  
    FROM aspiran,   
         general,   
         padres  
   WHERE ( aspiran.folio = general.folio ) and  
         ( aspiran.clv_ver = general.clv_ver ) and  
         ( aspiran.clv_per = general.clv_per ) and  
         ( aspiran.anio = general.anio ) and  
         ( aspiran.folio = padres.folio ) and  
         ( aspiran.clv_ver = padres.clv_ver ) and  
         ( aspiran.clv_per = padres.clv_per ) and  
         ( aspiran.anio = padres.anio ) and  
         (( aspiran.ing_per = :per ) AND  
         ( aspiran.ing_anio = :ani )) AND  
         (( :carr = 9999 ) OR  
         ( aspiran.clv_carr = :carr )) AND  
         aspiran.pago_insc = 1   
" arguments=(("ver", number),("per", number),("ani", number),("carr", number))  sort="general_cuenta A " )
