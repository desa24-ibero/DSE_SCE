 retrieve="  SELECT general.cuenta,   
         general.nombre,   
         general.apaterno,   
         general.amaterno,   
         aspiran.clv_carr,
         aspiran.status  
    FROM aspiran,   
         general  
   WHERE ( general.folio = aspiran.folio ) and  
         ( general.clv_ver = aspiran.clv_ver ) and  
         ( general.clv_per = aspiran.clv_per ) and  
         ( general.anio = aspiran.anio ) and  
         ( aspiran.ing_per = :per ) AND  
         ( aspiran.ing_anio = :ani ) AND  
         ( aspiran.status <> 0 and aspiran.status <> 3 )    
" arguments=(("per", number),("ani", number))  sort="general_cuenta A " )
