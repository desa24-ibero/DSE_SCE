 retrieve="  SELECT aspiran.folio,   
         general.cuenta,   
         general.apaterno,   
         general.amaterno,   
         general.nombre,   
         general.folio,   
         general.clv_ver,   
         general.clv_per,   
         general.anio,   
         aspiran.clv_carr  
    FROM aspiran,   
         general  
   WHERE ( general.folio = aspiran.folio ) and  
         ( general.clv_ver = aspiran.clv_ver ) and  
         ( general.clv_per = aspiran.clv_per ) and  
         ( general.anio = aspiran.anio ) and  
         ((aspiran.ing_per = :per ) AND  
         ( aspiran.ing_anio = :ani ) AND  
         ( aspiran.status <> 0 and aspiran.status <> 3 ))    
" arguments=(("ver", number),("per", number),("ani", number),("ord", number))  sort="if( ord=1,  general_cuenta ,  aspiran_clv_carr  ) A general_cuenta A " )
