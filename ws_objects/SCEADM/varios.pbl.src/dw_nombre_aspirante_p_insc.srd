 column=(type=long updatewhereclause=yes name=folio dbname="folio" )
 column=(type=long updatewhereclause=yes name=clv_ver dbname="clv_ver" )
 column=(type=long updatewhereclause=yes name=clv_per dbname="clv_per" )
 column=(type=long updatewhereclause=yes name=anio dbname="anio" )
 column=(type=long updatewhereclause=yes name=ing_per dbname="ing_per" values="PRIMAVERA	0/VERANO	1/OTOÑO	2/" )
 column=(type=long updatewhereclause=yes name=ing_anio dbname="ing_anio" )
 retrieve="  SELECT apaterno =general.apaterno,   
         amaterno =general.amaterno,   
         nombre = general.nombre,   
         folio = general.folio,   
         clv_ver = general.clv_ver,   
         clv_per = general.clv_per,   
         anio = general.anio,  
			ing_per = aspiran.ing_per,
			ing_anio = aspiran.ing_anio
    FROM general, aspiran  
   WHERE ( general.folio = :folio )  AND  
         ((general.clv_ver = :ver )  OR
          (:ver =99)               ) AND  
         ( general.clv_per = :per )  AND  
         ( general.anio = :ani )    AND
			( general.folio = aspiran.folio ) AND
			( general.clv_ver = aspiran.clv_ver) AND
			( general.clv_per = aspiran.clv_per) AND
			( general.anio = aspiran.anio) AND
			( aspiran.pago_insc = 1 )
	
" arguments=(("folio", number),("ver", number),("per", number),("ani", number)) )
