table(column=(type=char(50) updatewhereclause=yes name=nombre dbname="general.nombre" )
 column=(type=char(50) updatewhereclause=yes name=amaterno dbname="general.amaterno" )
 column=(type=char(50) updatewhereclause=yes name=apaterno dbname="general.apaterno" )
 column=(type=long updatewhereclause=yes name=folio dbname="general.folio" )
 column=(type=long updatewhereclause=yes name=clv_ver dbname="general.clv_ver" )
 column=(type=long updatewhereclause=yes name=clv_per dbname="general.clv_per" )
 column=(type=long updatewhereclause=yes name=anio dbname="general.anio" )
 retrieve="  SELECT general.nombre,   
         general.amaterno,   
         general.apaterno,   
         general.folio,   
         general.clv_ver,   
         general.clv_per,   
         general.anio  
    FROM general  
   WHERE ( ( (general.clv_ver = :ver) OR
             (:ver = 99)             ) AND  
         ( general.clv_per = :per ) AND  
         ( general.anio = :ani ) ) AND  
         ( ( general.nombre like :nombre ) OR  
         ( general.apaterno like :apat ) OR  
         ( general.amaterno like :amat )  )    
" arguments=(("nombre", string),("apat", string),("amat", string),("ver", number),("per", number),("ani", number)) )
