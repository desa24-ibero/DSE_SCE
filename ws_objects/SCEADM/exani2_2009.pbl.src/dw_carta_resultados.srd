 retrieve="  SELECT aspiran.puntaje,   
         aspiran.lugar_car,   
         aspiran.folio,   
         aspiran.clv_carr,   
         general.nombre,   
         general.apaterno,   
         general.amaterno,   
         aspiran.status,   
         general.cuenta, 
         aspiran.clv_ver,
         aspiran_promedio =isnull(aspiran.promedio,0),
			resultado_examen_modulo.area_1,
			resultado_examen_modulo.area_2,
			resultado_examen_modulo.area_3,
			resultado_examen_modulo.area_4,
			resultado_examen_modulo.area_5,
			parametros_area_examen.cve_area_examen_1,
			parametros_area_examen.cve_area_examen_2,
			parametros_area_examen.cve_area_examen_3,
			parametros_area_examen.cve_area_examen_4,
			parametros_area_examen.cve_area_examen_5,
			calificacion_global_examen = resultado_examen_modulo.calificacion_global_examen
    FROM aspiran,   
         general, 
			resultado_examen_modulo,
			parametros_area_examen
   WHERE ( aspiran.folio = general.folio ) and  
         ( aspiran.clv_ver = general.clv_ver ) and  
         ( aspiran.clv_per = general.clv_per ) and  
         ( aspiran.anio = general.anio ) and  
			( aspiran.folio = resultado_examen_modulo.folio ) and  
         ( aspiran.clv_ver = resultado_examen_modulo.clv_ver ) and  
         ( aspiran.clv_per = resultado_examen_modulo.clv_per ) and  
         ( aspiran.anio = resultado_examen_modulo.anio ) and  
         ( resultado_examen_modulo.cve_tipo_examen = 1 ) and  
         ( resultado_examen_modulo.cve_modulo_examen = 1 ) and  
         ( aspiran.clv_ver = parametros_area_examen.clv_ver ) and  
         ( aspiran.clv_per = parametros_area_examen.clv_per ) and  
         ( aspiran.anio = parametros_area_examen.anio ) and  
         ( resultado_examen_modulo.cve_tipo_examen =  parametros_area_examen.cve_tipo_examen) and  
         ( resultado_examen_modulo.cve_modulo_examen =  parametros_area_examen.cve_modulo_examen) and  
         ( aspiran.clv_per = :per ) AND  
         ( aspiran.anio = :ani ) AND  
         aspiran.pago_exam = 1 and
         ( ( :opcion = 0 AND   ((aspiran.clv_ver = :ver) OR (:ver = 99)) AND 
( aspiran.folio between :min AND :max) ) OR
           ( :opcion = 1 AND ( ( aspiran.status IN (3,4) ) ) OR 
									  ( ((aspiran.clv_ver = :ver) OR (:ver =99)) AND 
                               ( aspiran.status NOT IN (3,4) )  AND
                               ( aspiran.folio between :min AND :max) ) ) )
" arguments=(("ver", number),("per", number),("ani", number),("min", number),("max", number),("mensa_1", string),("mensa_2", string),("mensa_3", string),("mensa_4", string),("puntaje_min_plantel", number),("opcion", number))  sort="general_apaterno A general_amaterno A general_nombre A " )
