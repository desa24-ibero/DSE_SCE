 retrieve="  SELECT dbo.aspiran.puntaje,   
         dbo.aspiran.lugar_car,   
         dbo.aspiran.folio,   
         dbo.aspiran.clv_carr,   
         dbo.general.nombre,   
         dbo.general.apaterno,   
         dbo.general.amaterno,   
		dbo.general.sexo,   
         dbo.aspiran.status,   
         dbo.general.cuenta,   
         dbo.aspiran.clv_ver,
		dbo.aspiran.clv_carr
    FROM dbo.aspiran,   
         dbo.general
   WHERE ( dbo.aspiran.folio = dbo.general.folio ) and  
         ( dbo.general.clv_ver = dbo.aspiran.clv_ver ) and  
         ( dbo.aspiran.clv_per = dbo.general.clv_per ) and  
         ( dbo.general.anio = dbo.aspiran.anio ) and  
         ( dbo.aspiran.clv_per = :per ) AND  
         ( dbo.aspiran.anio = :ani ) AND  
         dbo.aspiran.pago_insc = 1    
" arguments=(("per", number),("ani", number),("mensa_1", string),("mensa_2", string),("mensa_3", string),("mensa_4", string))  sort="aspiran_clv_carr A general_apaterno A general_amaterno A general_nombre A " )
