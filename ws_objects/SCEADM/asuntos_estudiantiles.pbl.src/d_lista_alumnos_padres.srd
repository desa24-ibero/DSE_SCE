 column=(type=long updatewhereclause=yes name=aspiran_clv_carr dbname="aspiran.clv_carr" )
 retrieve="  SELECT dbo.general.cuenta,   
         dbo.general.nombre,   
         dbo.general.apaterno,   
         dbo.general.amaterno,   
         dbo.general.calle,   
         dbo.general.codigo_pos,   
         dbo.general.colonia,   
         dbo.general.estado,   
         dbo.general.telefono,   
         dbo.padres.nombre,   
         dbo.padres.apaterno,   
         dbo.padres.amaterno,   
         dbo.padres.calle,   
         dbo.padres.codigo_pos,   
         dbo.padres.colonia,   
         dbo.padres.estado,   
         dbo.padres.telefono,  
	   dbo.aspiran.clv_carr
    FROM dbo.aspiran,   
         dbo.general,   
         dbo.padres 
   WHERE ( dbo.aspiran.folio = dbo.general.folio ) and  
         ( dbo.aspiran.clv_ver = dbo.general.clv_ver ) and  
         ( dbo.aspiran.clv_per = dbo.general.clv_per ) and  
         ( dbo.aspiran.anio = dbo.general.anio ) and  
         ( dbo.aspiran.folio = dbo.padres.folio ) and  
         ( dbo.aspiran.clv_ver = dbo.padres.clv_ver ) and  
         ( dbo.aspiran.clv_per = dbo.padres.clv_per ) and 
         ( dbo.aspiran.anio = dbo.padres.anio ) and
         ( dbo.aspiran.clv_per = :per ) AND  
         ( dbo.aspiran.anio = :ani ) AND  
/*         (( :carr = 9999 ) OR  
         ( dbo.aspiran.clv_carr = :carr )) AND  
*/
         ( dbo.aspiran.pago_insc = 1)   
	  


" arguments=(("per", number),("ani", number),("carr", number))  sort="aspiran_clv_carr A padres_apaterno A padres_amaterno A padres_nombre A " )
