 column=(type=long update=yes updatewhereclause=yes name=cve_dia_rem dbname="aspiran_remediales_inscritas.cve_dia_rem" values="LUNES	1/MARTES	2/MIERCOLES	3/JUEVES	4/VIERNES	5/SABADO	6/	0/" )
 column=(type=long update=yes updatewhereclause=yes key=yes name=cve_dia dbname="aspiran_remediales_inscritas.cve_dia" values="LUNES	1/MARTES	2/MIERCOLES	3/JUEVES	4/VIERNES	5/SABADO	6/	0/" )
 retrieve="    SELECT a.folio AS folio_group,
							CONVERT(VARCHAR,c.cuenta) + '-' + CONVERT(VARCHAR,c.digito) AS cuenta,
							d.carrera,
							a.clv_ver,
							a.clv_per,
							a.anio,
							GETDATE() AS fecha,
							a.clv_carr,
							a.num_paq,
							NULL AS cve_mat_rem,
							NULL AS hora_inicio_rem,
							NULL AS hora_fin_rem,
							f.cve_mat,
							(SELECT MIN(hora_inicio) FROM controlescolar_bd.dbo.horario 
							 WHERE cve_mat = f.cve_mat AND gpo = f.gpo AND periodo = a.ing_per AND anio = a.ing_anio) AS hora_inicio,
							(SELECT MAX(hora_final) FROM controlescolar_bd.dbo.horario 
							 WHERE cve_mat = f.cve_mat AND gpo = f.gpo AND periodo = a.ing_per AND anio = a.ing_anio) AS hora_fin,
							1 AS mat_insertada,
							NULL AS comentarios,
							b.nombre AS general_nombre,   
							b.apaterno AS general_apaterno,   
							b.amaterno AS general_amaterno,   
							NULL AS materia_remedial,
							(SELECT materia FROM v_materias WHERE cve_mat = f.cve_mat) AS materia_inscrita,
							 NULL AS gpo_rem,
							 f.gpo,
							 NULL AS cve_dia_rem,
							 (SELECT MAX(cve_dia) FROM controlescolar_bd.dbo.horario 
							 WHERE cve_mat = f.cve_mat AND gpo = f.gpo AND periodo = a.ing_per AND anio = a.ing_anio) AS cve_dia
				FROM aspiran a
				INNER JOIN general b ON b.folio = a.folio
				AND b.clv_ver = a.clv_ver
				AND b.clv_per = a.clv_per
				AND b.anio = a.anio
				INNER JOIN v_sce_alumno_digito c ON c.cuenta = b.cuenta
				INNER JOIN v_carreras d ON d.cve_carrera = a.clv_carr
				INNER JOIN paquetes e ON e.num_paq = a.num_paq
				INNER JOIN controlescolar_bd.dbo.mat_inscritas f ON f.cuenta = b.cuenta
				INNER JOIN controlescolar_bd.dbo.prerrequisitos_esp g ON g.cve_carrera = e.clv_carr
				AND g.cve_plan = e.clv_plan
				AND g.cve_prerreq = f.cve_mat
				WHERE b.cuenta <> 0 
				AND b.cuenta IN (SELECT cuenta FROM controlescolar_bd.dbo.alumnos)
				AND a.pago_insc = 1 
				AND a.status IN (1,2,4)
				AND (a.clv_ver = :an_clv_ver OR 99 = :an_clv_ver)
				AND a.ing_per = :an_clv_per
				AND a.ing_anio = :an_anio
			UNION
			SELECT a.folio AS folio_group,
						CONVERT(VARCHAR,c.cuenta) + '-' + CONVERT(VARCHAR,c.digito) AS cuenta,
						d.carrera,
						a.clv_ver,
						a.clv_per,
						a.anio,
						h.fecha,
						a.clv_carr,
						a.num_paq,
						h.cve_mat_prop AS cve_mat_rem,
						h.hora_inicio_prop AS hora_inicio_rem,
						h.hora_fin_prop AS hora_fin_rem,
						h.cve_mat,
						h.hora_inicio,
						h.hora_fin,
						0 AS mat_insertada,
						h.comentarios,
						b.nombre AS general_nombre,   
						b.apaterno AS general_apaterno,   
						b.amaterno AS general_amaterno,   
						h.materia_prop AS materia_remedial,
						h.materia AS materia_inscrita,
						h.gpo_prop AS gpo_rem,
						h.gpo AS gpo,
						h.cve_dia_prop AS cve_dia_rem,
						h.cve_dia
				FROM aspiran a
				INNER JOIN general b ON b.folio = a.folio
				AND b.clv_ver = a.clv_ver
				AND b.clv_per = a.clv_per
				AND b.anio = a.anio
				INNER JOIN v_sce_alumno_digito c ON c.cuenta = b.cuenta
				INNER JOIN v_carreras d ON d.cve_carrera = a.clv_carr
				INNER JOIN paquetes e ON e.num_paq = a.num_paq
				INNER JOIN prop_materias_rechazadas h ON h.folio = a.folio
				AND h.clv_ver = a.clv_ver
				AND h.clv_per = a.clv_per
				AND h.anio = a.anio
				AND CONVERT(VARCHAR(8),h.fecha, 112) = CONVERT(VARCHAR(8), GETDATE(), 112)
				WHERE b.cuenta <> 0 
				AND b.cuenta IN (SELECT cuenta FROM controlescolar_bd.dbo.alumnos)
				AND a.pago_insc = 1 
				AND a.status IN (1,2,4)
				AND (a.clv_ver = :an_clv_ver OR 99 = :an_clv_ver)
				AND a.ing_per = :an_clv_per
				AND a.ing_anio = :an_anio
" update="aspiran_remediales_inscritas" updatewhere=1 updatekeyinplace=no arguments=(("an_clv_ver", number),("an_clv_per", number),("an_anio", number)) )
