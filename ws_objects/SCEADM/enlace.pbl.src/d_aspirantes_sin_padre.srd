 column=(type=char(9) updatewhereclause=yes name=periodo_examen dbname="periodo_examen" )
 column=(type=char(9) updatewhereclause=yes name=periodo_ingreso dbname="periodo_ingreso" )
 retrieve="  SELECT g.cuenta,   
         a.folio,   
         a.clv_ver,   
         a.clv_per,   
         a.anio,   
         a.ing_per,   
         a.ing_anio,
			periodo_examen = CASE a.clv_per WHEN 0 THEN ~"Primavera~"
																WHEN 1 THEN ~"Verano~"
																WHEN 2 THEN ~"Otoño~" END,
			periodo_ingreso = CASE a.ing_per WHEN 0 THEN ~"Primavera~"
																WHEN 1 THEN ~"Verano~"
																WHEN 2 THEN ~"Otoño~" END 
    FROM aspiran a,   
         general g
   WHERE ( a.folio = g.folio ) and  
         ( a.clv_ver = g.clv_ver ) and  
         ( a.clv_per = g.clv_per ) and  
         ( a.anio = g.anio ) and  
         ( a.status in (1,4) ) AND  
			(( a.clv_ver = :ai_version) OR
          (:ai_version = 99        ) ) AND
			( a.clv_per = :ai_periodo) AND
			( a.anio = :ai_anio) AND
		   NOT EXISTS (SELECT * FROM padres p 
												WHERE a.folio = p.folio
												AND a.clv_ver = p.clv_ver
												AND a.clv_per = p.clv_per
												AND a.anio = p.anio)
" arguments=(("ai_version", number),("ai_periodo", number),("ai_anio", number)) )
