 column=(type=long updatewhereclause=yes name=horario_periodo dbname="horario.periodo" )
 column=(type=long updatewhereclause=yes name=horario_anio dbname="horario.anio" )
 column=(type=long updatewhereclause=yes name=horario_cve_dia dbname="horario.cve_dia" )
 column=(type=char(16) updatewhereclause=yes name=horario_cve_salon dbname="horario.cve_salon" )
 column=(type=long updatewhereclause=yes name=horario_hora_inicio dbname="horario.hora_inicio" )
 column=(type=long updatewhereclause=yes name=horario_hora_final dbname="horario.hora_final" )
 column=(type=long updatewhereclause=yes name=horario_clase_aula dbname="horario.clase_aula" )
 retrieve="SELECT  a.cve_mat,
			a.gpo,
			a.periodo,
			a.anio,
			a.cve_dia,
			a.cve_salon,
			a.hora_inicio,
			a.hora_final,
			a.clase_aula 
FROM controlescolar_bd.dbo.horario a
INNER JOIN controlescolar_bd.dbo.grupos b ON b.cve_mat = a.cve_mat
AND b.gpo = a.gpo
AND b.periodo = a.periodo
AND b.anio = a.anio
WHERE a.periodo = :periodo
AND a.anio = :anio
AND a.cve_mat = :cve_mat_prop
AND ((a.hora_inicio <> :hora_inicio_prop AND a.hora_final <> :hora_final_prop AND a.gpo <>:gpo_prop)
OR (a.hora_inicio <> :hora_inicio AND a.hora_final <> :hora_final AND a.gpo <> :gpo))
AND b.inscritos < b.cupo" arguments=(("periodo", number),("anio", number),("cve_mat_prop", number),("hora_inicio_prop", number),("hora_final_prop", number),("gpo_prop", string),("hora_inicio", number),("hora_final", number),("gpo", string)) )
