table(column=(type=long updatewhereclause=yes name=aspiran_folio dbname="aspiran.folio" )
 retrieve="  SELECT DISTINCT aspiran.folio  
    FROM aspiran,   
         sec_peso  
   WHERE ( sec_peso.clv_carr = aspiran.clv_carr ) and  
         ( ( :ver=99 or aspiran.clv_ver = :ver ) AND  
         ( aspiran.clv_per = :per ) AND  
         ( aspiran.anio = :ani ) AND  
         ( sec_peso.clv_sec not in (  SELECT cali_sec.clv_sec                                            FROM cali_sec                                           WHERE ( cali_sec.folio = aspiran.folio ) AND                                                 ( cali_sec.clv_ver = :ver ) AND                                                 ( cali_sec.clv_per = :per ) AND                                                 ( cali_sec.anio = :ani ) AND                                                 ( cali_sec.clv_area = sec_peso.clv_area )  )) ) AND  
         aspiran.pago_exam = 1   
ORDER BY aspiran.folio ASC   
" arguments=(("ver", number),("per", number),("ani", number))  sort="aspiran_folio A " )
compute(band=detail alignment="2" expression=" aspiran_folio"border="0" color="0" x="5" y="4" height="65" width="330" format="[general]"  name=fol  font.face="Arial" font.height="-10" font.weight="400"  font.family="2" font.pitch="2" font.charset="0" background.mode="1" background.color="536870912" )
