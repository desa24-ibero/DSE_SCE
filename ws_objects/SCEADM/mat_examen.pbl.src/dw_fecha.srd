 retrieve="  SELECT fecha.clv_ver,   
         fecha.clv_per,   
         fecha.anio,   
         fecha.clv_actividad,   
         fecha.fecha_ini,   
         fecha.fecha_fin  
    FROM fecha  
   WHERE ( (fecha.clv_ver = :ver) OR
           (:ver =99)            ) AND  
         ( fecha.clv_per = :per ) AND  
         ( fecha.anio = :anio )    
" update="fecha" updatewhere=1 updatekeyinplace=no arguments=(("ver", number),("per", number),("anio", number)) )
