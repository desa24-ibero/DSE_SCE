 column=(type=long updatewhereclause=yes name=aspiran_clv_ver dbname="aspiran.clv_ver" )
 column=(type=long updatewhereclause=yes name=aspiran_clv_per dbname="aspiran.clv_per" )
 retrieve="  SELECT aspiran.folio,   
         aspiran.anio,   
         padres.nombre,   
         padres.apaterno,   
         padres.amaterno,   
         padres.calle,   
         padres.codigo_pos,   
         padres.colonia,   
         padres.estado,   
         padres.telefono,   
         aspiran.clv_carr,   
         aspiran.clv_ver,   
         aspiran.clv_per  
    FROM aspiran,   
         padres  
   WHERE ( aspiran.folio = padres.folio ) and  
         ( aspiran.anio = padres.anio ) and  
         ( aspiran.clv_ver = padres.clv_ver ) and  
         ( aspiran.clv_per = padres.clv_per ) and  
         (( aspiran.ing_per = :per ) AND  
         ( aspiran.ing_anio = :ani )) AND  
         (( :carr = 9999 ) OR  
         ( aspiran.clv_carr = :carr )) AND  
         ( :ai_pago_insc = 99 OR
            aspiran.pago_insc = :ai_pago_insc)
    
" arguments=(("ver", number),("per", number),("ani", number),("carr", number),("ai_pago_insc", number)) )
