 retrieve="  SELECT aspiran.folio,   
         aspiran.anio,   
         aspiran.salon,   
         aspiran.clv_ver,   
         aspiran.clv_per,   
         aspiran.clv_carr,   
         general.nombre,   
         general.apaterno,   
         general.amaterno  
    FROM aspiran,   
         general  
   WHERE ( aspiran.folio = general.folio ) and  
         ( aspiran.clv_ver = general.clv_ver ) and  
         ( aspiran.clv_per = general.clv_per ) and  
         ( aspiran.anio = general.anio ) and  
         ( (aspiran.clv_ver = :ver) OR 
           (:ver = 99)             ) AND  
         ( aspiran.clv_per = :per ) AND  
         ( aspiran.anio = :ani ) AND  
         aspiran.salon = :salon AND  
         aspiran.pago_exam = 1    
" arguments=(("ver", number),("per", number),("ani", number),("salon", string))  sort="salon D nombre A " )
