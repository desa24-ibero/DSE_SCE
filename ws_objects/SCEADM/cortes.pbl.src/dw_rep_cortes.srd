 retrieve="  SELECT aspiran.promedio,   
         aspiran.puntaje,   
         aspiran.lugar_gen,   
         aspiran.lugar_car,   
         aspiran.folio,   
         aspiran.clv_carr,   
         aspiran.clv_ver,
         aspiran.status  
    FROM aspiran  
   WHERE ( aspiran.clv_per = :per ) AND  
         ( aspiran.anio = :ani ) AND  
         ( aspiran.pago_exam = 1 ) AND  
         ( ( :opcion = 0 AND aspiran.status = 0 AND  (aspiran.clv_ver = :ver)  ) OR
         ( :opcion = 1 AND (( aspiran.status = 0 AND (aspiran.clv_ver = :ver)  ) OR
                            aspiran.status = 3 ) ) )" arguments=(("ver", number),("per", number),("ani", number),("opcion", number))  sort="clv_carr A puntaje D " )
