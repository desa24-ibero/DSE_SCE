 column=(type=datetime updatewhereclause=yes name=fecha dbname="bita_bachi.fecha" )
 column=(type=real updatewhereclause=yes name=nuevo dbname="bita_bachi.nuevo" )
 column=(type=real updatewhereclause=yes name=anterior dbname="bita_bachi.anterior" )
 column=(type=char(8) updatewhereclause=yes name=usuario dbname="bita_bachi.usuario" )
 column=(type=long updatewhereclause=yes name=clv_ver dbname="bita_bachi.clv_ver" )
 column=(type=long updatewhereclause=yes name=clv_per dbname="bita_bachi.clv_per" )
 column=(type=long updatewhereclause=yes name=anio dbname="bita_bachi.anio" )
 retrieve="  SELECT bita_bachi.folio,   
         bita_bachi.fecha,   
         bita_bachi.nuevo,   
         bita_bachi.anterior,   
         bita_bachi.usuario,   
         bita_bachi.clv_ver,   
         bita_bachi.clv_per,   
         bita_bachi.anio  
    FROM bita_bachi,   
         aspiran  
   WHERE ( aspiran.folio = bita_bachi.folio ) and  
         ( aspiran.clv_ver = bita_bachi.clv_ver ) and  
         ( aspiran.clv_per = bita_bachi.clv_per ) and  
         ( aspiran.anio = bita_bachi.anio ) and  
         ( (( bita_bachi.clv_ver = :ver ) OR 
            ( :ver = 99)                 ) AND  
         ( bita_bachi.clv_per = :per ) AND  
         ( bita_bachi.anio = :ani ) AND  
         ( aspiran.pago_exam = 1 ) )   
ORDER BY bita_bachi.folio ASC,   
         bita_bachi.fecha ASC   
" arguments=(("ver", number),("per", number),("ani", number)) )
