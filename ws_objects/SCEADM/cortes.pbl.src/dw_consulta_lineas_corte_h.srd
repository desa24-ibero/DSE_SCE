 column=(type=long updatewhereclause=yes name=lineas_corte_hist_anio dbname="lineas_corte_hist.anio" )
 retrieve="  SELECT lch.clv_carr,   
         lch.puntaje_acep,   
         lch.puntaje_dif,   
         c.carrera,
         lch.clv_ver,
         lch.clv_per,
         lch.anio         
    FROM lineas_corte_hist lch,   
         v_carreras  c
   WHERE (lch.clv_carr = c.cve_carrera ) AND
			((lch.clv_ver = :ver) OR
          (:ver = 99)         ) AND
         (lch.clv_per = :per) AND
         (lch.anio = :ani)
  ORDER BY lch.anio, lch.clv_per, lch.clv_ver, lch.clv_carr

" arguments=(("ver", number),("per", number),("ani", number)) )
