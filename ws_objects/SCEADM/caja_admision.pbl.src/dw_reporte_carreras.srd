table(column=(type=long updatewhereclause=yes name=aspiran_clv_carr dbname="aspiran.clv_carr" )
 retrieve="  SELECT aspiran.clv_carr  
    FROM aspiran,   
         general  
   WHERE ( aspiran.folio = general.folio ) and  
         ( aspiran.clv_ver = general.clv_ver ) and  
         ( aspiran.clv_per = general.clv_per ) and  
         ( aspiran.anio = general.anio ) and  
         ( :ver = 99 OR  
         general.clv_ver = :ver  ) AND  
         ( general.clv_per = :per ) AND  
         ( general.anio = :ani ) AND  
         (( :concep = 0 AND  
         aspiran.pago_exam = :pago ) OR  
         ( :concep = 1 AND  
         aspiran.pago_insc = :pago AND  
         aspiran.status <> 0 and aspiran.status <> 3))    
" arguments=(("ver", number),("per", number),("ani", number),("pago", number),("concep", number))  sort="aspiran_clv_carr A " )
