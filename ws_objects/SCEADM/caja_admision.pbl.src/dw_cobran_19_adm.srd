table(column=(type=long updatewhereclause=yes name=general_cuenta dbname="general.cuenta" )
 column=(type=long updatewhereclause=yes name=aspiran_folio dbname="aspiran.folio" )
 retrieve="	SELECT general.cuenta,aspiran.folio
    FROM   aspiran,general
	WHERE    ( aspiran.folio = general.folio ) and  
         ( general.clv_ver = aspiran.clv_ver ) and  
         ( aspiran.clv_per = general.clv_per ) and  
         ( general.anio = aspiran.anio ) and  
         ( aspiran.ing_per = :periodo ) AND  
         ( aspiran.ing_anio = :anio ) AND  
   		 ( aspiran.status IN (1,4,2)) AND
		(  general.cuenta <> 0 ) and
		( aspiran.pago_insc = 0 )

" arguments=(("periodo", number),("anio", number))  sort="general_cuenta A " )
