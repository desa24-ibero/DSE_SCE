table(column=(type=long updatewhereclause=yes name=folio dbname="folio" )
 column=(type=long updatewhereclause=yes name=anio dbname="anio" )
 column=(type=long updatewhereclause=yes name=clv_ver dbname="clv_ver" )
 column=(type=long updatewhereclause=yes name=clv_per dbname="clv_per" )
 column=(type=long updatewhereclause=yes name=clv_carr dbname="clv_carr" )
 column=(type=char(50) updatewhereclause=yes name=general_nombre dbname="nombre" )
 column=(type=char(50) updatewhereclause=yes name=general_apaterno dbname="apaterno" )
 column=(type=char(50) updatewhereclause=yes name=general_amaterno dbname="amaterno" )
 column=(type=long updatewhereclause=yes name=general_cuenta dbname="cuenta" )
 column=(type=char(8) updatewhereclause=yes name=bita_bachi_usuario dbname="usuario" )
 column=(type=long updatewhereclause=yes name=aspiran_status dbname="status" )
 retrieve="  SELECT aspiran.folio,   
         aspiran.anio,   
         aspiran.clv_ver,   
         aspiran.clv_per,   
         aspiran.clv_carr,   
         general.nombre,   
         general.apaterno,   
         general.amaterno,   
         general.cuenta,   
         bita_bachi.usuario,
         aspiran.status     
    FROM aspiran,   
         general,   
         bita_bachi  
   WHERE ( aspiran.folio = general.folio ) and  
         ( aspiran.clv_ver = general.clv_ver ) and  
         ( aspiran.clv_per = general.clv_per ) and  
         ( aspiran.anio = general.anio ) and  
         ( general.folio = bita_bachi.folio ) and  
         ( general.clv_ver = bita_bachi.clv_ver ) and  
         ( bita_bachi.clv_per = general.clv_per ) and  
         ( general.anio = bita_bachi.anio ) and  
         ( :ver = 99 OR  
         general.clv_ver = :ver  ) AND  
         ( general.clv_per = :per ) AND  
         ( general.anio = :ani ) AND  
         (( :concep = 0 AND  
         aspiran.pago_exam = :pago ) OR  
         ( :concep = 1 AND  
         aspiran.pago_insc = :pago AND  
         aspiran.status <> 0 and aspiran.status <> 3 )) AND  
         bita_bachi.anterior = NULL    
UNION ALL
  SELECT aspiran.folio,   
         aspiran.anio,   
         aspiran.clv_ver,   
         aspiran.clv_per,   
         aspiran.clv_carr,   
         general.nombre,   
         general.apaterno,   
         general.amaterno,   
         general.cuenta,   
         bita_bachi_usuario= ~"~",
         aspiran.status     
    FROM aspiran,   
         general
   WHERE ( aspiran.folio = general.folio ) and  
         ( aspiran.clv_ver = general.clv_ver ) and  
         ( aspiran.clv_per = general.clv_per ) and  
         ( aspiran.anio = general.anio ) and  
         ( :ver = 99 OR  
         general.clv_ver = :ver  ) AND  
         ( general.clv_per = :per ) AND  
         ( general.anio = :ani ) AND  
         (( :concep = 0 AND  
         aspiran.pago_exam = :pago ) OR  
         ( :concep = 1 AND  
         aspiran.pago_insc = :pago AND  
         aspiran.status <> 0 and aspiran.status <> 3 )) AND  
	NOT EXISTS (SELECT * FROM general g2,  bita_bachi 
	WHERE ( general.folio = g2.folio ) and  
         ( general.clv_ver = g2.clv_ver ) and  
         ( general.clv_per = g2.clv_per ) and  
         ( general.anio = g2.anio ) and  
         ( general.folio = bita_bachi.folio ) and  
         ( general.clv_ver = bita_bachi.clv_ver ) and  
         ( bita_bachi.clv_per = general.clv_per ) and  
         ( general.anio = bita_bachi.anio ) ) 
" arguments=(("ver", number),("per", number),("ani", number),("pago", number),("concep", number)) )
