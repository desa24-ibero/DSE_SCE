 column=(type=char(50) updatewhereclause=yes name=general_apaterno dbname="apaterno" )
 column=(type=char(50) updatewhereclause=yes name=general_amaterno dbname="amaterno" )
 column=(type=long updatewhereclause=yes name=aspiran_clv_per dbname="clv_per" )
 column=(type=long updatewhereclause=yes name=aspiran_anio dbname="anio" )
 retrieve="  SELECT  aspiran.folio ,
           aspiran.clv_carr ,
           general.cuenta ,
           general.nombre ,
           general.apaterno ,
           general.amaterno ,
           aspiran.clv_per ,
           aspiran.anio     
        FROM aspiran ,
           general     
        WHERE ( aspiran.folio = general.folio ) and
          ( general.clv_ver = aspiran.clv_ver ) and
          ( aspiran.clv_per = general.clv_per ) and
          ( general.anio = aspiran.anio ) and
          ( aspiran.clv_per = :per ) and
          ( aspiran.anio = :ani ) and
          ( aspiran.folio between :min AND :max ) and
          ( ( :opcion = 0 AND aspiran.status = 1 AND aspiran.clv_ver = :ver ) OR
          ( :opcion = 1 AND (( aspiran.status = 1 AND aspiran.clv_ver = :ver ) OR
                             aspiran.status = 4 ) ) )
   ORDER BY general.apaterno ASC,   
         general.amaterno ASC,   
         general.nombre ASC   
" arguments=(("ver", number),("per", number),("ani", number),("min", number),("max", number),("mensa_1", number),("mensa_2", number),("mensa_3", number),("mensa_4", number),("abiertas", string),("fecha_banco", date),("fecha_uia", date),("fecha_hoy", date),("abono", number),("opcion", number)) )
