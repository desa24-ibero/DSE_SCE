 column=(type=char(30) updatewhereclause=yes name=apellidos dbname="apellidos" )
 column=(type=char(30) updatewhereclause=yes name=alumnos_nombre dbname="nombre" )
 column=(type=char(30) updatewhereclause=yes name=carrera dbname="carrera" )
 column=(type=char(1) updatewhereclause=yes name=grabacion_causa dbname="grabacion_causa" )
 column=(type=char(2) updatewhereclause=yes name=tipo_de_grupo dbname="tipo_de_grupo" )
 column=(type=char(12) updatewhereclause=yes name=nombre_de_la_imagen dbname="nombre_de_la_imagen" )
 retrieve="  SELECT matricula = academicos.cuenta,   
         apellidos = convert(varchar(30),alumnos.apaterno +~" ~"+alumnos.amaterno),   
         nombre = convert(varchar(30),alumnos.nombre),   
         carrera = convert(varchar(30),carreras.carrera),   
			grabacion_causa = convert(varchar(1),~"N~"),
         tipo_de_grupo = CASE academicos.nivel  WHEN ~"L~" THEN ~"01~" 
                           WHEN ~"P~" THEN ~"02~"
									ELSE ~" ~" END,
			nombre_de_la_imagen = convert(varchar(12),~"A~")
    FROM academicos,   
         alumnos,   
         carreras
   WHERE ( academicos.cuenta = alumnos.cuenta ) and  
         ( academicos.cve_carrera = carreras.cve_carrera ) and
			( academicos.cuenta in (select cuenta from prueba2))
	ORDER BY academicos.cuenta
" )
