table(column=(type=datetime updatewhereclause=yes name=general_fecha_nac dbname="fecha_nac" )
 retrieve="  SELECT general.fecha_nac  
    FROM aspiran,   
         general  
   WHERE ( general.folio = aspiran.folio ) and  
         ( general.clv_ver = aspiran.clv_ver ) and  
         ( general.clv_per = aspiran.clv_per ) and  
         ( general.anio = aspiran.anio ) and  
         ( :ver=99 or aspiran.clv_ver = :ver ) AND  
         ( aspiran.clv_per = :per ) AND  
         ( aspiran.anio = :ani ) AND  
         ( :carr = 9999 OR  
         aspiran.clv_carr = :carr ) AND  
         :status like ~"%~"+convert(varchar(1),aspiran.status)+~"%~" AND  
         aspiran.pago_exam = 1    
" arguments=(("ver", number),("per", number),("ani", number),("carr", number),("status", string))  sort="int((year(today())-year( general_fecha_nac ))*2/10) A " )
group(level=1 header.height=1 trailer.height=69 by=("int((year(today())-year( general_fecha_nac ))*2/10)" ) header.color="536870912" trailer.color="536870912" )
