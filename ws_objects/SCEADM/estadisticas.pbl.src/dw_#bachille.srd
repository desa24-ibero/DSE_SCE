table(column=(type=real updatewhereclause=yes name=promedio dbname="promedio" )
 retrieve="  SELECT aspiran.promedio  
    FROM aspiran  
   WHERE ( :ver=99 or aspiran.clv_ver = :ver ) AND  
         ( aspiran.clv_per = :per ) AND  
         ( aspiran.anio = :ani ) AND  
         ( :carr = 9999 OR  
         aspiran.clv_carr = :carr ) AND  
         :status like ~"%~"+convert(varchar(1),aspiran.status)+~"%~" AND  
         aspiran.pago_exam = 1    
" arguments=(("ver", number),("per", number),("ani", number),("carr", number),("status", string)) )
