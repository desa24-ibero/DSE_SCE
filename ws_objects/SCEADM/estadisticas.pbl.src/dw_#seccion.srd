table(column=(type=number updatewhereclause=yes name=cali_sec_calif dbname="calif" )
 column=(type=char(25) updatewhereclause=yes name=sec_nom_nombre dbname="nombre" )
 retrieve="  SELECT cali_sec.calif,   
         sec_nom.nombre  
    FROM aspiran,   
         cali_sec,   
         sec_nom  
   WHERE ( aspiran.folio = cali_sec.folio ) and  
         ( aspiran.clv_ver = cali_sec.clv_ver ) and  
         ( aspiran.clv_per = cali_sec.clv_per ) and  
         ( aspiran.anio = cali_sec.anio ) and  
         ( sec_nom.clv_area = cali_sec.clv_area ) and  
         ( sec_nom.clv_sec = cali_sec.clv_sec ) and  
         ( :ver=99 or aspiran.clv_ver = :ver ) AND  
         ( aspiran.clv_per = :per ) AND  
         ( aspiran.anio = :ani ) AND  
         ( :carr = 9999 OR  
         aspiran.clv_carr = :carr ) AND  
         :status like ~"%~"+convert(varchar(1),aspiran.status)+~"%~" AND  
         ( cali_sec.clv_area = :area ) AND  
         ( cali_sec.clv_sec = :secc ) AND  
         aspiran.pago_exam = 1    
" arguments=(("ver", number),("per", number),("ani", number),("carr", number),("area", number),("secc", number),("status", string)) )
