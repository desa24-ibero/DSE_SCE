table(column=(type=long updatewhereclause=yes name=general_trabajo dbname="trabajo" )
 column=(type=long updatewhereclause=yes name=general_trab_hor dbname="trab_hor" )
 retrieve="  SELECT general.trabajo,   
         general.trab_hor  
    FROM aspiran,   
         general  
   WHERE ( general.folio = aspiran.folio ) and  
         ( general.clv_ver = aspiran.clv_ver ) and  
         ( general.clv_per = aspiran.clv_per ) and  
         ( general.anio = aspiran.anio ) and  
         ( :ver=99 or aspiran.clv_ver = :ver ) AND  
         ( aspiran.clv_per = :per ) AND  
         ( aspiran.anio = :ani ) AND  
         ( :carr = 9999 OR  
         aspiran.clv_carr = :carr ) AND  
         :status like ~"%~"+convert(char(1),aspiran.status)+~"%~" AND  
         aspiran.pago_exam = 1    
" arguments=(("ver", number),("per", number),("ani", number),("carr", number),("status", string))  sort="if(  general_trabajo =0, 0, 1 ) A " )
group(level=1 header.height=1 trailer.height=85 by=("if(  general_trabajo =0, 0, 1 )" ) header.color="536870912" trailer.color="536870912" )
