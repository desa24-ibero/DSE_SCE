table(column=(type=real updatewhereclause=yes name=cali_are_calif dbname="calif" )
 column=(type=long updatewhereclause=yes name=cali_are_clv_area dbname="clv_area" )
 retrieve="  SELECT cali_are.calif,   
         cali_are.clv_area  
    FROM aspiran,   
         cali_are  
   WHERE ( aspiran.folio = cali_are.folio ) and  
         ( cali_are.clv_ver = aspiran.clv_ver ) and  
         ( cali_are.clv_per = aspiran.clv_per ) and  
         ( cali_are.anio = aspiran.anio ) and  
         ( :ver=99 or aspiran.clv_ver = :ver ) AND  
         ( aspiran.clv_per = :per ) AND  
         ( aspiran.anio = :ani ) AND  
         ( :carr = 9999 OR  
         aspiran.clv_carr = :carr ) AND  
         :status like ~"%~"+convert(varchar(1),aspiran.status)+~"%~" AND  
         ( cali_are.clv_area = :area ) AND  
         aspiran.pago_exam = 1    
" arguments=(("ver", number),("per", number),("ani", number),("carr", number),("area", number),("status", string)) )
