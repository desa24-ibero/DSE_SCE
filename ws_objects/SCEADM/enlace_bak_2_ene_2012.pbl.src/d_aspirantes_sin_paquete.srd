 column=(type=char(9) updatewhereclause=yes name=periodo_examen dbname="periodo_examen" )
 column=(type=char(9) updatewhereclause=yes name=periodo_ingreso dbname="periodo_ingreso" )
 column=(type=long updatewhereclause=yes name=aspiran_pago_insc dbname="aspiran.pago_insc" values="NO	0/SI	1/" )
 column=(type=long updatewhereclause=yes name=aspiran_num_paq dbname="aspiran.num_paq" )
 retrieve="  SELECT general.cuenta,   
         aspiran.folio,   
         aspiran.clv_ver,   
         aspiran.clv_per,   
         aspiran.anio,   
         aspiran.ing_per,   
         aspiran.ing_anio,
			periodo_examen = CASE aspiran.clv_per WHEN 0 THEN ~"Primavera~"
																WHEN 1 THEN ~"Verano~"
																WHEN 2 THEN ~"Otoño~" END,
			periodo_ingreso = CASE aspiran.ing_per WHEN 0 THEN ~"Primavera~"
																WHEN 1 THEN ~"Verano~"
																WHEN 2 THEN ~"Otoño~" END, 
         aspiran.pago_insc,
			aspiran.num_paq
    FROM aspiran,   
         general,   
         padres 
   WHERE ( aspiran.folio = general.folio ) and  
         ( general.folio = padres.folio ) and  
         ( aspiran.clv_ver = general.clv_ver ) and  
         ( aspiran.clv_ver = padres.clv_ver ) and  
         ( aspiran.clv_per = general.clv_per ) and  
         ( aspiran.clv_per = padres.clv_per ) and  
         ( aspiran.anio = general.anio ) and  
         ( aspiran.anio = padres.anio ) and  
         ( aspiran.status in (1,4) ) AND  
         ( isnull(aspiran.num_paq,0) = 0 ) AND  
         ( (aspiran.clv_ver = :ai_version) OR
           (:ai_version = 99) ) AND  
         ( aspiran.clv_per = :ai_periodo ) AND  
         ( aspiran.anio = :ai_anio ) AND
         ( aspiran.pago_exam = 1 ) 

			
" arguments=(("ai_version", number),("ai_periodo", number),("ai_anio", number)) )
