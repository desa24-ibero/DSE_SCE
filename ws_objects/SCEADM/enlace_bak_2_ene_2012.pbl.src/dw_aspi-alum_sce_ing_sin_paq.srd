 retrieve="  SELECT dbo.aspiran.promedio,   
         dbo.general.nombre,   
         dbo.general.apaterno,   
         dbo.general.amaterno,   
         dbo.general.calle,   
         dbo.general.codigo_pos,   
         dbo.general.colonia,   
         dbo.general.estado,   
         dbo.general.telefono,   
         dbo.general.fecha_nac,   
         dbo.general.lugar_nac,   
         dbo.general.nacional,   
         dbo.general.sexo,   
         dbo.general.edo_civil,   
         dbo.general.religion,   
         dbo.general.bachillera,   
         dbo.general.trabajo,   
         dbo.general.trab_hor,   
         dbo.general.ya_inscri,   
         dbo.general.cuenta,   
         dbo.general.transporte,   
         dbo.padres.nombre,   
         dbo.padres.apaterno,   
         dbo.padres.amaterno,   
         dbo.padres.calle,   
         dbo.padres.codigo_pos,   
         dbo.padres.colonia,   
         dbo.padres.estado,   
         dbo.padres.telefono,   
         dbo.aspiran.clv_carr,
			dbo.aspiran.clv_ver,
			dbo.aspiran.clv_per,
			dbo.aspiran.folio,
			dbo.aspiran.anio  
    FROM dbo.aspiran,   
         dbo.general,   
         dbo.padres
   WHERE ( dbo.aspiran.folio = dbo.padres.folio ) and  
         ( dbo.aspiran.clv_ver = dbo.padres.clv_ver ) and  
         ( dbo.aspiran.clv_per = dbo.padres.clv_per ) and  
         ( dbo.aspiran.anio = dbo.padres.anio ) and  
         ( dbo.aspiran.folio = dbo.general.folio ) and  
         ( dbo.general.clv_ver = dbo.aspiran.clv_ver ) and  
         ( dbo.aspiran.clv_per = dbo.general.clv_per ) and  
         ( dbo.general.anio = dbo.aspiran.anio ) and  
         ( (dbo.aspiran.clv_ver = :ver) OR
           (:ver = 99) ) AND  
         ( dbo.aspiran.ing_per = :per ) AND  
         ( dbo.aspiran.ing_anio = :ani ) AND  
         dbo.general.cuenta <> 0 AND  
         dbo.aspiran.pago_insc = 1 and
         ( dbo.aspiran.clv_per = :per ) AND  
         ( dbo.aspiran.anio = :ani )   AND
			( dbo.aspiran.num_paq in (0, null))
ORDER BY dbo.aspiran.folio" arguments=(("ver", number),("per", number),("ani", number))  sort="aspiran_anio A aspiran_clv_per A aspiran_clv_ver A general_cuenta A " )
