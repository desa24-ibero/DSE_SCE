 column=(type=char(2) updatewhereclause=yes name=horario_gpo dbname="horario.gpo" dbalias=".gpo" )
 column=(type=long updatewhereclause=yes name=horario_cve_dia dbname="horario.cve_dia" dbalias=".cve_dia" )
 column=(type=long updatewhereclause=yes name=horario_hora_inicio dbname="horario.hora_inicio" dbalias=".hora_inicio" )
 column=(type=long updatewhereclause=yes name=horario_hora_final dbname="horario.hora_final" dbalias=".hora_final" )
 column=(type=char(83) updatewhereclause=yes name=corario_hora_final dbname="corario_hora_final" )
 column=(type=char(42) updatewhereclause=yes name=materias_materia dbname="materias.materia" dbalias=".materia" )
 column=(type=char(10) updatewhereclause=yes name=profesor dbname="profesor" )
 column=(type=char(12) updatewhereclause=yes name=tipo_grupod dbname="tipo_grupod" )
 retrieve="  SELECT horario.cve_mat,   
         horario.gpo,   
         horario.cve_dia,   
         horario.hora_inicio,   
         horario.hora_final, 
		CONVERT(varchar, horario.cve_mat) + ' - ' + materias.materia + ' Gpo. ' + horario.gpo as corario_hora_final, materias.materia, 
		'Profesor: '  as profesor, 
		'Tipo grupo: ' AS tipo_grupod 
    FROM horario, materias 
WHERE  horario.periodo = :ai_periodo 
AND horario.anio = :ai_anio
AND horario.cve_mat = :al_cve_materia 
AND horario.gpo = :as_gpo 
AND horario.cve_mat = materias.cve_mat


" arguments=(("ai_periodo", number),("ai_anio", number),("al_cve_materia", number),("as_gpo", string)) )
