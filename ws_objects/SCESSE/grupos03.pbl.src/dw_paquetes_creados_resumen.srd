table(column=(type=long updatewhereclause=yes name=solicitados dbname="solicitados" )
 column=(type=long updatewhereclause=yes name=faltantes dbname="faltantes" )
 column=(type=long updatewhereclause=yes name=creados dbname="creados" )
 column=(type=long updatewhereclause=yes name=paquetes_materias_clv_mat dbname="paquetes_materias.clv_mat" )
 column=(type=char(42) updatewhereclause=yes name=materias_materia dbname="materias.materia" )
 retrieve="
SELECT 0 AS solicitados, 0 AS faltantes, SUM(pa.cupo) as creados, pm.clv_mat, m.materia
FROM paquetes pa, 
	paquetes_materias pm, 
	prerrequisitos_esp pe, 
	materias m, 
	carreras c     
WHERE pa.periodo = :a_periodo 
AND pa.anio = :a_anio  
AND pa.num_paq = pm.num_paq 
AND pm.clv_mat = pe.cve_prerreq 
AND pm.clv_per = pa.periodo 
AND pm.anio = pa.anio
AND pe.cve_carrera = pa.clv_carr 
AND pe.cve_plan = pa.clv_plan
AND m.cve_mat = pm.clv_mat
AND c.cve_carrera = pa.clv_carr 
AND c.cve_coordinacion = :a_coordinacion
GROUP BY  pm.clv_mat, m.materia


/*SELECT 0 AS solicitados, 0 AS faltantes, SUM(pa.cupo) as creados, pm.clv_mat, m.materia
