 column=(type=long updatewhereclause=yes name=compute_0016 dbname="compute_0016" )
 column=(type=long updatewhereclause=yes name=materias_cve_coordinacion dbname="materias.cve_coordinacion" )
 column=(type=char(70) updatewhereclause=yes name=coordinaciones_coordinacion dbname="coordinaciones.coordinacion" )
 column=(type=long updatewhereclause=yes name=grupos_cond_gpo dbname="grupos.cond_gpo" )
 retrieve="  SELECT DISTINCT materias.cve_mat,   
         materias.materia,   
         grupos.gpo,   
         grupos.cupo,   
         grupos.inscritos,   
         materias.sigla,   
         profesor.cve_profesor,   
         profesor.nombre,   
         profesor.apaterno,   
         profesor.amaterno,   
         profesor.nombre_completo,   
         grupos.tipo,   
         tipo_grupo.nombre_tipo,   
         grupos.cve_asimilada,   
         grupos.gpo_asimilado,   
         0,   
         materias.cve_coordinacion,   
         coordinaciones.coordinacion,   
         grupos.cond_gpo  
    FROM profesor,   
         materias,   
         horario,   
         grupos,   
         tipo_grupo,   
         coordinaciones  
   WHERE ( grupos.cve_mat = horario.cve_mat ) AND 
         ( grupos.gpo = horario.gpo ) AND    
         ( grupos.cve_mat = materias.cve_mat ) and  
         ( grupos.cve_profesor = profesor.cve_profesor ) and  
         ( horario.periodo = :periodo ) and  
         ( horario.anio = :anio ) and  
         ( grupos.tipo = tipo_grupo.tipo ) and  
         ( materias.cve_coordinacion = coordinaciones.cve_coordinacion ) and  
         ( ( materias.cve_coordinacion in ( :coordinaciones ) ) AND  
         ( horario.clase_aula in ( :tipo_salon ) ) AND  
         ( grupos.periodo = :periodo ) AND  
         ( grupos.anio = :anio ) AND  
         ( materias.nivel in ( :nivel ) )  AND  
         ( horario.cve_salon IS NULL ) ) 
" arguments=(("orden", number),("coordinaciones", numberlist),("tipo_salon", numberlist),("periodo", number),("anio", number),("nivel", stringlist))  sort="materias_cve_coordinacion A materias_cve_mat A grupos_gpo A profesor_cve_profesor A " )
