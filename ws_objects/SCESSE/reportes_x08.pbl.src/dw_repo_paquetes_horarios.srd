table(column=(type=long updatewhereclause=yes name=grupos_periodo dbname="grupos.periodo" )
 column=(type=long updatewhereclause=yes name=grupos_anio dbname="grupos.anio" )
 column=(type=long updatewhereclause=yes name=v_paquetes_materias_num_paq dbname="v_paquetes_materias.num_paq" )
 column=(type=long updatewhereclause=yes name=v_paquetes_materias_clv_mat dbname="v_paquetes_materias.clv_mat" )
 column=(type=char(42) updatewhereclause=yes name=materias_materia dbname="materias.materia" )
 column=(type=char(6) updatewhereclause=yes name=materias_sigla dbname="materias.sigla" )
 column=(type=char(2) updatewhereclause=yes name=v_paquetes_materias_grupo dbname="v_paquetes_materias.grupo" )
 column=(type=long updatewhereclause=yes name=horario_cve_dia dbname="horario.cve_dia" )
 column=(type=char(9) updatewhereclause=yes name=dias_dia dbname="dias.dia" )
 column=(type=char(16) updatewhereclause=yes name=horario_cve_salon dbname="horario.cve_salon" )
 column=(type=long updatewhereclause=yes name=horario_hora_inicio dbname="horario.hora_inicio" )
 column=(type=long updatewhereclause=yes name=horario_hora_final dbname="horario.hora_final" )
 column=(type=long updatewhereclause=yes name=materias_cve_coordinacion dbname="materias.cve_coordinacion" )
 column=(type=long updatewhereclause=yes name=carreras_cve_carrera dbname="carreras.cve_carrera" )
 column=(type=char(100) updatewhereclause=yes name=carreras_carrera dbname="carreras.carrera" )
 column=(type=long updatewhereclause=yes name=grupos_forma_imparte dbname="grupos.forma_imparte" )
 column=(type=char(1) updatewhereclause=yes name=grupos_sesionado dbname="grupos.sesionado" )
 column=(type=char(15) updatewhereclause=yes name=fecha_duracion dbname="fecha_duracion" )
 retrieve="SELECT 	grupos.periodo,   
			grupos.anio,   
			v_paquetes_materias.num_paq,   
			v_paquetes_materias.clv_mat,   
			materias.materia,   
			materias.sigla,   
			v_paquetes_materias.grupo,   
			horario.cve_dia,   
			dias.dia,   
			horario.cve_salon,   
			horario.hora_inicio,   
			horario.hora_final,
			materias.cve_coordinacion,
			carreras.cve_carrera,
			carreras.carrera,
			grupos.forma_imparte,
			grupos.sesionado,
			(CASE WHEN CONVERT(VARCHAR(3), grupos.fecha_inicio, 107) = 'Jan' THEN 'Ene' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_inicio, 106) + ' a '
					WHEN CONVERT(VARCHAR(3), grupos.fecha_inicio, 107) = 'Apr' THEN 'Abr' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_inicio, 106) + ' a '
					WHEN CONVERT(VARCHAR(3), grupos.fecha_inicio, 107) = 'Aug' THEN 'Ago' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_inicio, 106) + ' a '
					WHEN CONVERT(VARCHAR(3), grupos.fecha_inicio, 107) = 'Dec' THEN 'Dic' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_inicio, 106) + ' a '
					ELSE CONVERT(VARCHAR(6), grupos.fecha_inicio, 107) + ' a '
			END) + 
			(CASE WHEN CONVERT(VARCHAR(3), grupos.fecha_fin, 107) = 'Jan' THEN 'Ene' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_fin, 106)
				  WHEN CONVERT(VARCHAR(3), grupos.fecha_fin, 107) = 'Apr' THEN 'Abr' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_fin, 106)
				  WHEN CONVERT(VARCHAR(3), grupos.fecha_fin, 107) = 'Aug' THEN 'Ago' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_fin, 106)
				  WHEN CONVERT(VARCHAR(3), grupos.fecha_fin, 107) = 'Dec' THEN 'Dic' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_fin, 106)
				  ELSE CONVERT(VARCHAR(6), grupos.fecha_fin, 107)
			END) AS fecha_duracion
FROM grupos
INNER JOIN horario ON horario.cve_mat = grupos.cve_mat
AND ((grupos.tipo <>  2 AND horario.cve_mat = grupos.cve_mat AND horario.gpo = grupos.gpo) OR  
 	    (grupos.tipo = 2 AND horario.cve_mat = grupos.cve_asimilada AND horario.gpo = grupos.gpo_asimilado))
INNER JOIN materias ON materias.cve_mat = horario.cve_mat
INNER JOIN dias ON dias.cve_dia = horario.cve_dia
INNER JOIN v_paquetes_materias ON v_paquetes_materias.grupo = grupos.gpo
AND v_paquetes_materias.clv_mat = grupos.cve_mat
AND v_paquetes_materias.clv_per = grupos.periodo
AND v_paquetes_materias.anio = grupos.anio
INNER JOIN v_paquetes ON v_paquetes.num_paq = v_paquetes_materias.num_paq
INNER JOIN carreras ON carreras.cve_carrera = v_paquetes.clv_carr
WHERE grupos.forma_imparte =  :tipo_imp
AND (v_paquetes.clv_carr = :a_carrera OR :a_carrera = 9999) 
AND v_paquetes_materias.clv_per IN ( :a_periodo )
AND v_paquetes_materias.anio = :a_anio
AND v_paquetes.num_paq IN (SELECT v_paquetes_materias.num_paq 
						   			   FROM v_paquetes_materias 
									   WHERE (v_paquetes_materias.clv_mat = :a_materia OR :a_materia = 9999) 
									   AND (v_paquetes_materias.grupo = :a_gpo OR :a_gpo = ~"%~"))" arguments=(("a_periodo", numberlist),("a_anio", number),("a_materia", number),("a_gpo", string),("a_carrera", number),("tipo_imp", number)) )
