 column=(type=long updatewhereclause=yes name=materias_cve_coordinacion dbname="materias.cve_coordinacion" )
 column=(type=char(70) updatewhereclause=yes name=coordinaciones_coordinacion dbname="coordinaciones.coordinacion" )
 column=(type=long updatewhereclause=yes name=grupos_cond_gpo dbname="grupos.cond_gpo" )
 column=(type=long updatewhereclause=yes name=grupos_periodo dbname="grupos.periodo" )
 column=(type=long updatewhereclause=yes name=grupos_forma_imparte dbname="grupos.forma_imparte" )
 column=(type=char(1) updatewhereclause=yes name=grupos_sesionado dbname="grupos.sesionado" )
 column=(type=char(15) updatewhereclause=yes name=fecha_duracion dbname="fecha_duracion" )
 retrieve="SELECT DISTINCT materias.cve_mat,   
						materias.materia,   
						grupos.gpo,   
						grupos.cupo,   
						grupos.inscritos,   
						materias.sigla,   
						profesor.cve_profesor,   
						profesor.nombre,   
						profesor.apaterno,   
						profesor.amaterno,   
						profesor.nombre_completo,   
						grupos.tipo,   
						tipo_grupo.nombre_tipo,   
						grupos.cve_asimilada,   
						grupos.gpo_asimilado,   
						0 as compute_0016,   
						materias.cve_coordinacion,   
						coordinaciones.coordinacion,   
						grupos.cond_gpo, 
						grupos.periodo,
						grupos.forma_imparte,
						grupos.sesionado,
						(CASE WHEN CONVERT(VARCHAR(3), grupos.fecha_inicio, 107) = 'Jan' THEN 'Ene' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_inicio, 106) + ' a '
								WHEN CONVERT(VARCHAR(3), grupos.fecha_inicio, 107) = 'Apr' THEN 'Abr' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_inicio, 106) + ' a '
								WHEN CONVERT(VARCHAR(3), grupos.fecha_inicio, 107) = 'Aug' THEN 'Ago' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_inicio, 106) + ' a '
								WHEN CONVERT(VARCHAR(3), grupos.fecha_inicio, 107) = 'Dec' THEN 'Dic' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_inicio, 106) + ' a '
								ELSE CONVERT(VARCHAR(6), grupos.fecha_inicio, 107) + ' a '
						 END) + 
						(CASE WHEN CONVERT(VARCHAR(3), grupos.fecha_fin, 107) = 'Jan' THEN 'Ene' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_fin, 106)
								 WHEN CONVERT(VARCHAR(3), grupos.fecha_fin, 107) = 'Apr' THEN 'Abr' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_fin, 106)
								 WHEN CONVERT(VARCHAR(3), grupos.fecha_fin, 107) = 'Aug' THEN 'Ago' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_fin, 106)
								 WHEN CONVERT(VARCHAR(3), grupos.fecha_fin, 107) = 'Dec' THEN 'Dic' + ' ' + CONVERT(VARCHAR(2), grupos.fecha_fin, 106)
								 ELSE CONVERT(VARCHAR(6), grupos.fecha_fin, 107)
						 END) AS fecha_duracion
FROM profesor
INNER JOIN grupos ON grupos.cve_profesor = profesor.cve_profesor
INNER JOIN materias ON materias.cve_mat = grupos.cve_mat
LEFT JOIN horario ON horario.cve_mat = grupos.cve_mat
AND horario.gpo = grupos.gpo
AND horario.cve_mat = grupos.cve_asimilada
AND horario.gpo = grupos.gpo_asimilado
AND horario.periodo = grupos.periodo
AND horario.anio = grupos.anio
AND horario.clase_aula IN ( :tipo_salon )
INNER JOIN tipo_grupo ON tipo_grupo.tipo = grupos.tipo
INNER JOIN coordinaciones ON coordinaciones.cve_coordinacion = materias.cve_coordinacion
WHERE grupos.periodo IN (:periodo)
AND grupos.anio = :anio
AND grupos.cond_gpo = 1
AND grupos.gpo NOT LIKE ~"Z%~"
AND grupos.forma_imparte =  :tipo_imp
AND grupos.inscritos <= :inscritos_min
AND materias.cve_coordinacion IN ( :coordinaciones )
AND materias.nivel IN ( :nivel )
" arguments=(("orden", number),("coordinaciones", numberlist),("tipo_salon", numberlist),("periodo", numberlist),("anio", number),("nivel", stringlist),("inscritos_min", number),("tipo_imp", number))  sort="materias_cve_coordinacion A materias_cve_mat A grupos_gpo A profesor_cve_profesor A " )
