 retrieve="  SELECT profesor_cotitular.cve_mat,   
         profesor_cotitular.gpo,   
         profesor_cotitular.periodo,   
         profesor_cotitular.anio,   
         profesor_cotitular.cve_profesor,   
         profesor_cotitular.titularidad,   
         profesor_cotitular.fecha_inicio,   
         profesor_cotitular.fecha_fin,   
         profesor.nombre,   
         profesor.apaterno,   
         profesor.amaterno, 
		profesor_cotitular.horas_totales_grupo as horas_profesor, 
		profesor_cotitular.tipo_profesor, 
		profesor_cotitular.autorizado, 
		profesor_cotitular.evalua   
    FROM profesor_cotitular,   
         profesor  
   WHERE profesor_cotitular.cve_profesor = profesor.cve_profesor
AND profesor_cotitular.cve_mat = :cve_mat 
AND profesor_cotitular.gpo = :gpo 
AND profesor_cotitular.periodo = :periodo
AND profesor_cotitular.anio = :anio  
UNION 
   SELECT grupos.cve_mat as cve_mat,   
         grupos.gpo as gpo,   
         grupos.periodo as periodo,   
         grupos.anio as anio,   
         grupos.cve_profesor as cve_profesor,   
         1 as titularidad,   
         grupos.fecha_inicio as fecha_inicio,   
         grupos.fecha_fin as fecha_fin,   
         profesor.nombre as profesor_nombre,   
         profesor.apaterno as profesor_apaterno,   
         profesor.amaterno as profesor_amaterno, 
		 0 as horas_profesor, 
		1 as tipo_profesor, 
		0 as autorizado, 
		'S' as evalua     
    FROM grupos,   
         profesor  
   WHERE grupos.cve_profesor = profesor.cve_profesor
AND grupos.cve_mat = :cve_mat 
AND grupos.gpo = :gpo 
AND grupos.periodo = :periodo
AND grupos.anio = :anio  
AND NOT EXISTS(SELECT * FROM profesor_cotitular 
				WHERE profesor_cotitular.cve_mat = grupos.cve_mat
					AND profesor_cotitular.gpo = grupos.gpo 
					AND profesor_cotitular.periodo = grupos.periodo 
					AND profesor_cotitular.anio = grupos.anio)   




  /*SELECT profesor_cotitular.cve_mat,   
         profesor_cotitular.gpo,   
         profesor_cotitular.periodo,   
         profesor_cotitular.anio,   
         profesor_cotitular.cve_profesor,   
         profesor_cotitular.titularidad,   
         profesor_cotitular.fecha_inicio,   
         profesor_cotitular.fecha_fin,   
         profesor.nombre,   
         profesor.apaterno,   
         profesor.amaterno, 
		(SELECT SUM(horario_profesor_grupo.horas_asignadas) 
			FROM horario_profesor_grupo 
			WHERE horario_profesor_grupo.cve_mat = profesor_cotitular.cve_mat 
			AND horario_profesor_grupo.gpo = profesor_cotitular.gpo 
			AND horario_profesor_grupo.periodo = profesor_cotitular.periodo 
			AND horario_profesor_grupo.anio = profesor_cotitular.anio 
			AND horario_profesor_grupo.cve_profesor = profesor_cotitular.cve_profesor) as horas_profesor   
    FROM profesor_cotitular,   
         profesor  
   WHERE profesor_cotitular.cve_profesor = profesor.cve_profesor
AND profesor_cotitular.cve_mat = :cve_mat 
AND profesor_cotitular.gpo = :gpo 
AND profesor_cotitular.periodo = :periodo
AND profesor_cotitular.anio = :anio  */
   
   " arguments=(("cve_mat", number),("gpo", string),("periodo", number),("anio", number))  sort="titularidad D cve_profesor D " )
