 retrieve="  SELECT horario_modular.cve_mat,   
         horario_modular.gpo,   
         horario_modular.periodo,   
         horario_modular.anio,   
         horario_modular.cve_dia,   
         horario_modular.cve_salon,   
         horario_modular.hora_inicio,   
         horario_modular.hora_final,   
         horario_modular.fecha_inicio,   
         horario_modular.fecha_fin,   
         materias.materia,   
         horario_modular.orden  
    FROM horario_modular,   
         materias  
   WHERE ( horario_modular.cve_mat = materias.cve_mat )    
AND  horario_modular.cve_mat = :al_cve_mat
AND horario_modular.gpo = :as_gpo
AND horario_modular.periodo = :ai_periodo
AND horario_modular.anio = :ai_anio
" arguments=(("al_cve_mat", number),("as_gpo", string),("ai_periodo", number),("ai_anio", number)) )
