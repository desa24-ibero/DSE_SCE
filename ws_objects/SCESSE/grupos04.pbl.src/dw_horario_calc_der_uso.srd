 column=(type=char(2) updatewhereclause=yes name=horario_gpo dbname="horario.gpo" dbalias=".gpo" )
 column=(type=long updatewhereclause=yes name=horario_cve_dia dbname="horario.cve_dia" dbalias=".cve_dia" )
 column=(type=long updatewhereclause=yes name=horario_hora_inicio dbname="horario.hora_inicio" dbalias=".hora_inicio" )
 column=(type=long updatewhereclause=yes name=horario_hora_final dbname="horario.hora_final" dbalias=".hora_final" )
 column=(type=long updatewhereclause=yes name=horario_clase_aula dbname="horario.clase_aula" dbalias=".clase_aula" )
 column=(type=datetime updatewhereclause=yes name=grupos_fecha_inicio dbname="grupos.fecha_inicio" dbalias=".fecha_inicio" )
 column=(type=datetime updatewhereclause=yes name=grupos_fecha_fin dbname="grupos.fecha_fin" dbalias=".fecha_fin" )
 retrieve="  SELECT horario.cve_mat,   
         horario.gpo,   
         horario.cve_dia,   
         horario.hora_inicio,   
         horario.hora_final,   
         horario.clase_aula,   
         grupos.fecha_inicio,   
         grupos.fecha_fin  
FROM grupos , materias, horario   
WHERE grupos.periodo = :ai_periodo
AND grupos.anio = :ai_anio
AND grupos.cve_mat = materias.cve_mat
AND materias.cve_coordinacion = :al_coordinacion 
AND grupos.cve_mat = horario.cve_mat
AND grupos.gpo = horario.gpo 
AND grupos.periodo = horario.periodo 
AND grupos.anio = horario.anio 
AND grupos.tipo IN(SELECT tipo FROM tipo_grupo WHERE requiere_horario = 1) 
AND horario.clase_aula = 0 
AND NOT(grupos.cve_mat = :al_cve_mat AND grupos.gpo = :as_gpo) 









" arguments=(("ai_periodo", number),("ai_anio", number),("al_coordinacion", number),("al_cve_mat", number),("as_gpo", string)) )
