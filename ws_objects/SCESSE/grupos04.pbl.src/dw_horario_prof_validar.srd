 column=(type=long updatewhereclause=yes name=horario_profesor_grupo_hora_final dbname="horario_profesor_grupo.hora_final" dbalias="h.hora_final" )
 column=(type=long updatewhereclause=yes name=grupos_cve_profesor dbname="grupos_cve_profesor" )
 column=(type=long updatewhereclause=yes name=materias_cve_coordinacion dbname="materias.cve_coordinacion" dbalias="m.cve_coordinacion" )
 retrieve="SELECT g.cve_mat as cve_mat, 
		  g.gpo as gpo, 
		  h.fecha_inicio as fecha_inicio, 
		  h.fecha_fin as fecha_fin, 
		  h.cve_dia as cve_dia, 
		  h.hora_inicio as hora_inicio , 
		  h.hora_final as hora_final, 
		  grupos_cve_profesor = h.cve_profesor, 
		  m.cve_coordinacion as cve_coordinacion 
FROM grupos g, horario_profesor_grupo h, materias m    
WHERE h.cve_profesor = :a_profesor
AND g.periodo = :a_periodo
AND g.anio = :a_anio
AND g.cve_mat = h.cve_mat 
AND g.gpo = h.gpo 
AND g.periodo = h.periodo 
AND g.anio = h.anio 
AND m.cve_mat = g.cve_mat
AND NOT (g.cve_mat = :a_cve_materia AND g.gpo = :a_gpo)
ORDER BY g.cve_profesor, g.cve_mat_gpo   
/*AND g.cve_profesor = h.cve_profesor */ 


" arguments=(("a_profesor", number),("a_periodo", number),("a_anio", number),("a_cve_materia", number),("a_gpo", string)) )
