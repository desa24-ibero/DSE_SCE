 column=(type=long updatewhereclause=yes name=academicos_cve_carrera dbname="academicos.cve_carrera" )
 column=(type=char(100) updatewhereclause=yes name=carreras_carrera dbname="carreras.carrera" )
 column=(type=char(50) updatewhereclause=yes name=alumnos_apaterno dbname="alumnos.apaterno" )
 column=(type=char(50) updatewhereclause=yes name=alumnos_amaterno dbname="alumnos.amaterno" )
 column=(type=char(50) updatewhereclause=yes name=alumnos_nombre dbname="alumnos.nombre" )
 column=(type=char(15) updatewhereclause=yes name=flag_documentos_desc_flag_documento dbname="flag_documentos.desc_flag_documento" )
 column=(type=char(60) updatewhereclause=yes name=tipo_documento_documento dbname="tipo_documento.documento" )
 column=(type=long updatewhereclause=yes name=compute_0009 dbname="compute_0009" )
 column=(type=char(70) updatewhereclause=yes name=alumnos_nombre_completo dbname="alumnos.nombre_completo" )
 retrieve="  SELECT academicos.cuenta,   
         academicos.cve_carrera,
			carreras.carrera,   
         alumnos.apaterno,   
         alumnos.amaterno,   
         alumnos.nombre,   
         flag_documentos.desc_flag_documento,   
         tipo_documento.documento,   
         0,   
         alumnos.nombre_completo  
    FROM academicos,   
         alumnos,   
         banderas,   
         documentos,   
         flag_documentos,   
         tipo_documento,   
         banderas_inscrito,
			carreras  
   WHERE ( academicos.cuenta = alumnos.cuenta ) and  
         ( academicos.cuenta = banderas_inscrito.cuenta ) and  
         ( academicos.cuenta = banderas.cuenta ) and  
         ( academicos.cuenta = documentos.cuenta ) and
			( academicos.cve_carrera = carreras.cve_carrera ) and  
         ( documentos.cve_flag_documento = flag_documentos.cve_flag_documento ) and  
         ( documentos.cve_documento = tipo_documento.cve_documento ) and  
         ( banderas.baja_documentos IN (0,5)) AND  
         ( documentos.cve_flag_documento = 0 ) AND  
         ( academicos.cve_plan in ( :plan ) ) AND  
         ( academicos.cve_carrera in ( :carrera ) )    
" arguments=(("orden", number),("plan", numberlist),("carrera", numberlist))  sort="academicos_cve_carrera A alumnos_nombre_completo A " )
