 column=(type=long updatewhereclause=yes name=materias_cve_coordinacion dbname="materias.cve_coordinacion" )
 column=(type=char(70) updatewhereclause=yes name=coordinaciones_coordinacion dbname="coordinaciones.coordinacion" )
 column=(type=long updatewhereclause=yes name=profesor_auxiliar_cve_profesor dbname="profesor_auxiliar.cve_profesor" )
 retrieve="SELECT horas_tot_coord = sum(p.horas), 
       m.cve_coordinacion, 
		 c.coordinacion,
		 p.cve_profesor
FROM profesor_auxiliar p, materias m, coordinaciones c, grupos g
WHERE p.cve_mat = m.cve_mat
AND	p.cve_profesor = :al_cve_profesor
AND	m.cve_coordinacion = c.cve_coordinacion
AND   p.periodo = :ai_periodo
AND   p.anio = :ai_anio
AND 	p.cve_mat = g.cve_mat
AND	p.gpo = g.gpo
AND	p.periodo = g.periodo
AND	p.anio = g.anio
AND	g.cond_gpo = 1
AND NOT (p.cve_mat = :al_cve_mat
		   AND p.gpo = :as_gpo)
GROUP BY m.cve_coordinacion, 
   		c.coordinacion, 
         p.cve_profesor
ORDER BY p.cve_profesor" arguments=(("al_cve_profesor", number),("al_cve_mat", number),("as_gpo", string),("ai_periodo", number),("ai_anio", number)) )
