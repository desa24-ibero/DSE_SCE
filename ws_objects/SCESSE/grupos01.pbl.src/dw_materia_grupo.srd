 retrieve="  SELECT grupos.cve_mat_gpo,
	    grupos.cve_mat, 
         grupos.gpo,   
         grupos.periodo,   
         grupos.anio,   
         grupos.cond_gpo,   
         grupos.cupo,   
         grupos.tipo,   
         grupos.inscritos,   
         grupos.insc_desp_bajas,   
         grupos.cve_asimilada,   
         grupos.gpo_asimilado,   
         grupos.cve_profesor,   
         grupos.prom_gpo,   
         grupos.porc_asis,   
         grupos.ema4,
         grupos.primer_sem  
    FROM grupos   
    WHERE (grupos.cve_mat = :materia) AND
          (grupos.gpo     = :grupo) AND
          (grupos.periodo = :periodo) AND
          (grupos.anio    = :ano)

" update="grupos" updatewhere=1 updatekeyinplace=yes arguments=(("materia", number),("grupo", string),("periodo", number),("ano", number)) )
