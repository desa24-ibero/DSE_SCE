table(column=(type=long updatewhereclause=yes name=horas_tot_coord dbname="horas_tot_coord" )
 column=(type=long updatewhereclause=yes name=materias_cve_coordinacion dbname="materias.cve_coordinacion" dbalias="m.cve_coordinacion" )
 column=(type=char(70) updatewhereclause=yes name=coordinaciones_coordinacion dbname="coordinaciones.coordinacion" dbalias="c.coordinacion" )
 column=(type=long updatewhereclause=yes name=grupos_cve_profesor dbname="grupos.cve_profesor" dbalias="g.cve_profesor" )
 retrieve="SELECT horas_tot_coord = sum(horas_reales), 
       m.cve_coordinacion, 
		 c.coordinacion,
		 g.cve_profesor
FROM grupos g, materias m, coordinaciones c
WHERE g.cve_mat = m.cve_mat
AND	g.cve_profesor = :al_cve_profesor
AND	m.cve_coordinacion = c.cve_coordinacion
AND	g.tipo not in (1, 2)
AND   g.periodo = :ai_periodo
AND   g.anio = :ai_anio
AND NOT (g.cve_mat = :al_cve_mat
		   AND g.gpo = :as_gpo)
AND	g.cond_gpo = 1
GROUP BY m.cve_coordinacion, 
   		c.coordinacion, 
         g.cve_profesor
UNION 
SELECT horas_tot_coord = sum(horas_reales), 
       m.cve_coordinacion, 
		 c.coordinacion,
		 g.cve_profesor
FROM grupos g, materias m, coordinaciones c
WHERE g.cve_mat = m.cve_mat
AND	g.cve_profesor = :al_cve_profesor
AND	m.cve_coordinacion = c.cve_coordinacion
AND	g.tipo not in (1, 2)
AND   g.periodo IN(SELECT periodo from periodos_por_procesos where cve_proceso = 4 and tipo_periodo <> (SELECT tipo FROM periodo WHERE periodo = :ai_periodo))
AND   g.anio = :ai_anio
AND NOT (g.cve_mat = :al_cve_mat
		   AND g.gpo = :as_gpo)
AND	g.cond_gpo = 1
GROUP BY m.cve_coordinacion, 
   		c.coordinacion, 
         g.cve_profesor
ORDER BY g.cve_profesor
" arguments=(("al_cve_profesor", number),("al_cve_mat", number),("as_gpo", string),("ai_periodo", number),("ai_anio", number)) )
