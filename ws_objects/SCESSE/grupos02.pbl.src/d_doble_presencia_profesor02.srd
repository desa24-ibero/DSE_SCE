 column=(type=char(2) update=yes updatewhereclause=yes name=gpo dbname="horario.gpo" )
 column=(type=long update=yes updatewhereclause=yes name=periodo dbname="horario.periodo" )
 column=(type=long update=yes updatewhereclause=yes name=anio dbname="horario.anio" )
 column=(type=long update=yes updatewhereclause=yes name=cve_dia dbname="horario.cve_dia" )
 column=(type=long update=yes updatewhereclause=yes name=hora_inicio dbname="horario.hora_inicio" )
 column=(type=long update=yes updatewhereclause=yes name=hora_final dbname="horario.hora_final" )
 retrieve="  SELECT horario.cve_mat,   
         horario.gpo,   
         horario.periodo,   
         horario.anio,   
         horario.cve_dia,   
         horario.hora_inicio,   
         horario.hora_final  
    FROM horario,   
         grupos  
   WHERE ( grupos.cve_mat = horario.cve_mat ) and  
         ( grupos.gpo = horario.gpo ) and  
         ( grupos.periodo = horario.periodo ) and  
         ( grupos.anio = horario.anio ) and  
         ( grupos.periodo = :ai_periodo ) and  
         ( grupos.anio = :ai_anio ) and  
         not( ( horario.cve_mat = :al_cve_mat ) AND  
              ( horario.gpo = :as_gpo ) AND  
              ( horario.periodo = :ai_periodo ) AND  
              ( horario.anio = :ai_anio )) AND  
            ((( :ai_hora_inicio <= horario.hora_inicio ) AND  
              ( horario.hora_inicio <= :ai_hora_final  )) OR  
             (( :ai_hora_final <= horario.hora_final - 1  ) AND  
              ( horario.hora_final - 1 <= :ai_hora_final   ))) AND  
             ( horario.cve_dia = :ai_cve_dia ) AND  
             ( grupos.cve_profesor = :al_cve_profesor )    
 " arguments=(("al_cve_mat", number),("as_gpo", string),("ai_periodo", number),("ai_anio", number),("al_cve_profesor", number),("ai_cve_dia", number),("ai_hora_inicio", number),("ai_hora_final", number)) )
