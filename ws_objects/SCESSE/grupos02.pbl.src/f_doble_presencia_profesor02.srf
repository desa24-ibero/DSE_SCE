global function boolean f_doble_presencia_profesor02 (long al_cve_mat, string as_gpo, integer ai_periodo, integer ai_anio, long al_cve_profesor, integer ai_cve_dia, integer ai_hora_inicio, integer ai_hora_final, ref string as_grupos_encimados)
