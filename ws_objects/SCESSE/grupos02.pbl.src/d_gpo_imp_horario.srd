 retrieve="  SELECT horario.cve_mat,   
         horario.gpo,   
         horario.periodo,   
         horario.anio,   
         horario.cve_dia,   
         horario.cve_salon,   
         horario.hora_inicio,   
         horario.hora_final,   
         horario.clase_aula, 
		horario.comentario
    FROM horario
   WHERE (horario.cve_mat = :cvmat) AND  
         (horario.gpo     = :gpo) AND
         (horario.periodo = :Periodo) AND
         (horario.anio    = :ano)   






" update="horario" updatewhere=0 updatekeyinplace=no arguments=(("cvmat", number),("gpo", string),("periodo", number),("ano", number))  sort="cve_dia A hora_inicio A " )
