 column=(type=long updatewhereclause=yes name=anio dbname="horario.anio" )
 column=(type=long updatewhereclause=yes name=no_semana dbname="profesor_lista_asistencia.no_semana" )
 column=(type=long updatewhereclause=yes name=cve_dia dbname="horario.cve_dia" )
 retrieve=" SELECT 	horario.hora_inicio,
			horario.hora_final,
			horario.cve_mat,
			horario.gpo,
			materias.materia,
			profesor.nombre_completo,
			grupos.cve_profesor,
			horario.cve_salon, 
			materias.cve_coordinacion,
			dias.dia,
			coordinaciones.coordinacion,
			profesor_lista_asistencia.cve_asistencia,
			profesor_lista_asistencia.periodo,
			profesor_lista_asistencia.anio,
			profesor_lista_asistencia.no_semana,
			profesor_lista_asistencia.cve_dia
    FROM horario,   
         grupos,   
         profesor,   
         dias,   
         materias,
			coordinaciones,
			profesor_lista_asistencia
   WHERE ( horario.cve_mat = grupos.cve_mat ) and  
         ( grupos.cve_profesor = profesor.cve_profesor ) and  
         ( horario.cve_dia = dias.cve_dia ) and  
         ( horario.gpo = grupos.gpo ) and  
         ( grupos.cve_mat = materias.cve_mat ) and  
			( grupos.anio = horario.anio) and
			( grupos.periodo = horario.periodo) and
			( grupos.anio = :a_anio ) and
			( grupos.periodo = :a_periodo ) and
			( profesor_lista_asistencia.cve_mat = grupos.cve_mat ) and
			( profesor_lista_asistencia.gpo = grupos.gpo ) and
			( profesor_lista_asistencia.periodo = horario.periodo) and
			( profesor_lista_asistencia.anio = horario.anio ) and
			( profesor_lista_asistencia.no_semana = :a_no_semana ) and
			( profesor_lista_asistencia.cve_dia = horario.cve_dia ) and
			( profesor_lista_asistencia.hora_inicio = horario.hora_inicio ) and
			( materias.cve_coordinacion = coordinaciones.cve_coordinacion ) and
			(( horario.cve_dia IN (:a_dia) ) AND
         ( materias.cve_coordinacion = :a_coordinacion OR :a_coordinacion = 9999 ))
ORDER BY coordinaciones.cve_coordinacion ASC,horario.cve_dia ASC,horario.hora_inicio ASC,horario.cve_mat ASC" arguments=(("a_periodo", number),("a_anio", number),("a_coordinacion", number),("a_dia", numberlist),("a_no_semana", number)) )
