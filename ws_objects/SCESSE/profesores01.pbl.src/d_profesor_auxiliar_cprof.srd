 retrieve="  SELECT profesor_auxiliar.cve_profesor,   
         profesor_auxiliar.cve_mat,   
         profesor_auxiliar.gpo,   
         profesor_auxiliar.periodo,   
         profesor_auxiliar.anio,   
         profesor_auxiliar.cve_categoria_auxiliar,   
         profesor_auxiliar.horas,   
			profesor_auxiliar.cve_dia,  
			profesor_auxiliar.hora_inicio,  
			profesor_auxiliar.hora_final,  
         nombre_completo = convert(varchar(75), ~"~")  
    FROM profesor_auxiliar  
   WHERE ( profesor_auxiliar.cve_mat = :ai_cve_mat ) AND  
         ( profesor_auxiliar.gpo = :as_gpo ) AND  
         ( profesor_auxiliar.periodo = :ai_periodo ) AND  
         ( profesor_auxiliar.anio = :ai_anio )    
" update="profesor_auxiliar" updatewhere=1 updatekeyinplace=yes arguments=(("ai_cve_mat", number),("as_gpo", string),("ai_periodo", number),("ai_anio", number)) )
