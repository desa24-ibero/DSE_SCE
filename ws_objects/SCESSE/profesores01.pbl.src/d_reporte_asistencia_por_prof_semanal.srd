 column=(type=long updatewhereclause=yes name=materias_cve_coordinacion dbname="materias.cve_coordinacion" )
 column=(type=long updatewhereclause=yes name=profesor_lista_asistencia_no_semana dbname="profesor_lista_asistencia.no_semana" )
 retrieve="  SELECT dbo.profesor_lista_asistencia.cve_mat,   
         dbo.profesor_lista_asistencia.gpo,   
         dbo.profesor.cve_profesor,   
         dbo.profesor.nombre_completo,   
         dbo.profesor_lista_asistencia.cve_asistencia,   
         dbo.materias.cve_coordinacion,  
         dbo.profesor_lista_asistencia.no_semana
    FROM dbo.profesor_lista_asistencia,   
         dbo.materias,   
         dbo.grupos,   
         dbo.profesor  
   WHERE ( dbo.profesor_lista_asistencia.cve_mat = dbo.materias.cve_mat ) and  
         ( dbo.profesor_lista_asistencia.cve_mat = dbo.grupos.cve_mat ) and  
         ( dbo.profesor_lista_asistencia.gpo = dbo.grupos.gpo ) and  
         ( dbo.profesor_lista_asistencia.periodo = dbo.grupos.periodo ) and  
         ( dbo.profesor_lista_asistencia.anio = dbo.grupos.anio ) and  
         ( dbo.grupos.cve_profesor = dbo.profesor.cve_profesor ) and  
         ( ( dbo.profesor_lista_asistencia.periodo = :a_periodo ) AND  
         ( dbo.profesor_lista_asistencia.anio = :a_anio ) AND  
         ( dbo.profesor_lista_asistencia.no_semana = :ai_num_semana ) AND  
         ( ( dbo.materias.cve_coordinacion = :a_coordinacion ) OR ( :a_coordinacion = 9999 ) ) )    
" arguments=(("a_periodo", number),("a_anio", number),("a_coordinacion", number),("ai_num_semana", number))  sort="materias_cve_coordinacion A profesor_cve_profesor A profesor_lista_asistencia_cve_mat A profesor_lista_asistencia_gpo A profesor_lista_asistencia_cve_asistencia A " )
