 column=(type=char(152) updatewhereclause=yes name=nombre_alumno dbname="nombre_alumno" )
 column=(type=long updatewhereclause=yes name=cve_carrera dbname="no_academicos_hist.cve_carrera" initial="0" )
 column=(type=char(100) updatewhereclause=yes name=desc_carrera dbname="desc_carrera" )
 column=(type=long updatewhereclause=yes name=cve_plan dbname="no_academicos_hist.cve_plan" initial="0" )
 column=(type=char(20) updatewhereclause=yes name=desc_plan dbname="desc_plan" )
 column=(type=long updatewhereclause=yes name=cve_subsistema dbname="no_academicos_hist.cve_subsistema" initial="0" )
 column=(type=char(40) updatewhereclause=yes name=desc_sub dbname="desc_sub" )
 column=(type=char(1) updatewhereclause=yes name=nivel dbname="no_academicos_hist.nivel" initial="L" )
 column=(type=char(15) updatewhereclause=yes name=desc_nivel dbname="desc_nivel" )
 column=(type=long updatewhereclause=yes name=cuenta_2 dbname="no_academicos_hist.cuenta_2" )
 retrieve="  SELECT distinct rtrim(convert(char(8),no_academicos_hist.cuenta)) + '-' + (select digito from v_sce_alumno_digito
																	where cuenta = no_academicos_hist.cuenta ) cuenta,  
		(select nombre + ' ' + apaterno + ' ' + amaterno from alumnos where cuenta = no_academicos_hist.cuenta) as nombre_alumno ,
         no_academicos_hist.cve_carrera,   
		(select carrera from carreras a where a.cve_carrera = no_academicos_hist.cve_carrera) as desc_carrera,
         no_academicos_hist.cve_plan,   
		(  SELECT nombre_plan FROM nombre_plan
			WHERE ( nombre_plan.cve_plan =  no_academicos_hist.cve_plan ) ) as desc_plan,
         no_academicos_hist.cve_subsistema,   
		( SELECT distinct subsistema.subsistema  FROM subsistema  WHERE ( subsistema.cve_carrera =  no_academicos_hist.cve_carrera ) AND   ( subsistema.cve_plan = no_academicos_hist.cve_plan  ) AND ( subsistema.cve_subsistema = no_academicos_hist.cve_subsistema )) as desc_sub,
         no_academicos_hist.nivel,   
		(   SELECT upper(nivel.desc_corta)  FROM nivel WHERE nivel.cve_nivel   = no_academicos_hist.nivel ) as desc_nivel,
		no_academicos_hist.cuenta as cuenta_2
    FROM no_academicos_hist  
   WHERE no_academicos_hist.en_aca_hist = 0
	AND no_academicos_hist.usuario_modif is null
order by no_academicos_hist.cuenta,no_academicos_hist.cve_carrera,no_academicos_hist.cve_plan,no_academicos_hist.cve_subsistema"  sort="cuenta_2 A " )
