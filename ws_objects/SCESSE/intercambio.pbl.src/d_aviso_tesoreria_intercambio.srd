 retrieve="SELECT 
cuenta =d.cuenta, 
digito =d.digito,
nombre_completo = al.nombre_completo,
cve_carrera = ac.cve_carrera,
c.carrera,
cve_plan = ac.cve_plan,
nombre_plan = np.nombre_plan,
periodo_ing = ac.periodo_ing,
periodo_ing_descripcion = p2.descripcion,
anio_ing = ac.anio_ing,
cve_mat_base = pi.cve_mat_base,   
materia_base = m2.materia,  
creditos =  m2.creditos,
horas_reales =  m2.horas_reales,
calificacion = hbf.calificacion,
calificacion_anterior = hbf.calificacion_anterior,

periodo_base = hbf.periodo,
periodo_base_descripcion = p3.descripcion,
anio_base = hbf.anio,

cve_mat = pi.cve_posrreq, 
materia = m.materia, 
creditos_ser =  m.creditos,
horas_reales_ser =  m.horas_reales,
gpo = mi.gpo, 
anio = mi.anio, 
periodo = mi.periodo, 
periodo_descripcion = p.descripcion,
fecha_hoy = CASE datepart(mm, getdate()) 
                             WHEN 1 THEN convert(varchar(2),datepart(dd, getdate())) +~" de Enero de ~"+convert(varchar(4),datepart(yy, getdate()))
                             WHEN 2 THEN convert(varchar(2),datepart(dd, getdate())) +~" de Febrero de ~"+convert(varchar(4),datepart(yy, getdate()))
                             WHEN 3 THEN convert(varchar(2),datepart(dd, getdate())) +~" de Marzo de ~"+convert(varchar(4),datepart(yy, getdate()))
                             WHEN 4 THEN convert(varchar(2),datepart(dd, getdate())) +~" de Abril de ~"+convert(varchar(4),datepart(yy, getdate()))
                             WHEN 5 THEN convert(varchar(2),datepart(dd, getdate())) +~" de Mayo de ~"+convert(varchar(4),datepart(yy, getdate()))
                             WHEN 6 THEN convert(varchar(2),datepart(dd, getdate())) +~" de Junio de ~"+convert(varchar(4),datepart(yy, getdate()))
                             WHEN 7 THEN convert(varchar(2),datepart(dd, getdate())) +~" de Julio de ~"+convert(varchar(4),datepart(yy, getdate()))
                             WHEN 8 THEN convert(varchar(2),datepart(dd, getdate())) +~" de Agosto de ~"+convert(varchar(4),datepart(yy, getdate()))
                             WHEN 9 THEN convert(varchar(2),datepart(dd, getdate())) +~" de Septiembre de ~"+convert(varchar(4),datepart(yy, getdate()))
                             WHEN 10 THEN convert(varchar(2),datepart(dd, getdate())) +~" de Octubre de ~"+convert(varchar(4),datepart(yy, getdate()))
                             WHEN 11 THEN convert(varchar(2),datepart(dd, getdate())) +~" de Noviembre de ~"+convert(varchar(4),datepart(yy, getdate()))
                             WHEN 12 THEN convert(varchar(2),datepart(dd, getdate())) +~" de Diciembre de ~"+convert(varchar(4),datepart(yy, getdate()))
                             ELSE ~"~" END,
hora = convert(varchar(10), getdate(), 108)
FROM v_sce_alumno_digito d, posrrequisitos_intercambio pi, materias m, mat_inscritas mi, materias m2, periodo p, alumnos al, academicos ac, carreras c, historico_temp_intercambio hbf, nombre_plan np, periodo p2, periodo p3
WHERE d.cuenta = mi.cuenta
AND  d.cuenta = pi.cuenta
AND d.cuenta = al.cuenta
AND d.cuenta = ac.cuenta
AND d.cuenta = hbf.cuenta
AND ac.cve_carrera = c.cve_carrera
AND ac.cve_plan = np.cve_plan
AND ac.periodo_ing = p2.periodo
AND pi.cve_posrreq = m.cve_mat
AND pi.cve_posrreq = mi.cve_mat
AND pi.cve_mat_base = hbf.cve_mat
AND hbf.periodo = p3.periodo
AND d.cuenta  = :al_cuenta
AND  pi.cve_mat_base = m2.cve_mat
and  mi.periodo = p.periodo
" arguments=(("al_cuenta", number)) )
