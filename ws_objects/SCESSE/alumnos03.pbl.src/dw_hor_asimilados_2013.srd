 column=(type=long updatewhereclause=yes name=horario_cve_dia dbname="horario.cve_dia" dbalias=".cve_dia" values="DOMINGO	0/LUNES	1/MARTES	2/MIERCOLES	3/JUEVES	4/VIERNES	5/SABADO	6/" )
 column=(type=char(16) updatewhereclause=yes name=horario_cve_salon dbname="horario.cve_salon" dbalias=".cve_salon" )
 retrieve="  SELECT 	mat_inscritas.cve_mat,   
				(select materias.materia from materias where materias.cve_mat = grupos.cve_asimilada) materia ,
         			mat_inscritas.gpo,   
         			grupos.cve_asimilada,   
         			grupos.gpo_asimilado,   
         			horario.hora_inicio,   
         			horario.hora_final,   
         			horario.cve_dia,   
         			horario.cve_salon  
    FROM mat_inscritas,   
         		grupos,   
         		horario 
   WHERE  ((mat_inscritas.cuenta = :cuenta ) and   
				 (mat_inscritas.cve_mat = :materia ) and
                    (mat_inscritas.cve_mat = grupos.cve_mat )) and  
                    ((mat_inscritas.gpo = grupos.gpo ) and
                    (grupos.periodo = mat_inscritas.periodo) and
                    (grupos.anio = mat_inscritas.anio)) and                
         		    ((grupos.cve_asimilada = horario.cve_mat ) and  
         		     (grupos.gpo_asimilado = horario.gpo ) and  
                      (horario.periodo = mat_inscritas.periodo) and
                     (horario.anio = mat_inscritas.anio))" arguments=(("cuenta", number),("materia", number)) )
