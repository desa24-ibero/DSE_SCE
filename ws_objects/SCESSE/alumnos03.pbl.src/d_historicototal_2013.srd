 column=(type=char(2) updatewhereclause=yes name=historico_calificacion dbname="historico.calificacion" )
 column=(type=long updatewhereclause=yes name=historico_observacion dbname="historico.observacion" )
 column=(type=long updatewhereclause=yes name=historico_cuenta dbname="historico.cuenta" )
 retrieve="  SELECT  academicos.cve_carrera,   
         academicos.cve_plan,   
         historico.cve_mat,   
         materias.materia,   
         historico.gpo,   
         historico.periodo,
		periodo.descripcion_corta,   
         historico.anio,   
         historico.cve_carrera,   
         historico.cve_plan,   
         historico.calificacion,   
         historico.observacion,   
         historico.cuenta  
    FROM historico,   
         v_sce_carreras_cursadas academicos,   
         materias,
		periodo
   WHERE ( historico.cuenta = academicos.cuenta ) and  
         ( historico.periodo = periodo.periodo ) and  
         ( historico.cve_mat = materias.cve_mat ) and  
         ( ( historico.cuenta = :cuenta ) )   
ORDER BY academicos.vigente desc,
		historico.cve_carrera ASC, 
         historico.anio ASC,   
         historico.periodo ASC, 
         historico.cve_plan ASC,   
         historico.cve_mat ASC   


" arguments=(("cuenta", number)) )
