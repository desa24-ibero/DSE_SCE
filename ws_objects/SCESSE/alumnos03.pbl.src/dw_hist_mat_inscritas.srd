table(column=(type=long updatewhereclause=yes name=hist_mat_inscritas_cve_mat dbname="hist_mat_inscritas.cve_mat" )
 column=(type=char(6) updatewhereclause=yes name=materias_sigla dbname="materias.sigla" )
 column=(type=char(42) updatewhereclause=yes name=materias_materia dbname="materias.materia" )
 column=(type=decimal(3) updatewhereclause=yes name=materias_creditos dbname="materias.creditos" )
 column=(type=char(2) updatewhereclause=yes name=hist_mat_inscritas_gpo dbname="hist_mat_inscritas.gpo" )
 column=(type=long updatewhereclause=yes name=hist_mat_inscritas_periodo dbname="hist_mat_inscritas.periodo" )
 column=(type=long updatewhereclause=yes name=hist_mat_inscritas_anio dbname="hist_mat_inscritas.anio" )
 column=(type=char(2) updatewhereclause=yes name=hist_mat_inscritas_calificacion dbname="hist_mat_inscritas.calificacion" )
 column=(type=long updatewhereclause=yes name=hist_mat_inscritas_cve_condicion dbname="hist_mat_inscritas.cve_condicion" values="NORMAL	0/BAJA ACADEMICA	1/BAJA TOTAL	2/" )
 column=(type=long updatewhereclause=yes name=hist_mat_inscritas_cve_acreditacion dbname="hist_mat_inscritas.cve_acreditacion" values="NO ACREDITADA	0/ACREDITADA	1/EXTRAORDINARIO	2/ORDINARIO	3/PRERR. HISTORICO	4/REVALIDADA	5/TIT. SUFICIENCIA	6/" )
 column=(type=char(1) updatewhereclause=yes name=hist_mat_inscritas_inscripcion dbname="hist_mat_inscritas.inscripcion" values="INSCRIP.	I/ALTA	A/" )
 column=(type=char(6) updatewhereclause=yes name=materias_sigla_anterior dbname="materias.sigla_anterior" )
 retrieve="  SELECT hist_mat_inscritas.cve_mat,   
         materias.sigla,   
         materias.materia,   
         materias.creditos,   
         hist_mat_inscritas.gpo,   
         hist_mat_inscritas.periodo,   
         hist_mat_inscritas.anio,   
         hist_mat_inscritas.calificacion,   
         hist_mat_inscritas.cve_condicion,   
         hist_mat_inscritas.cve_acreditacion,   
         hist_mat_inscritas.inscripcion,  
         materias.sigla_anterior
    FROM hist_mat_inscritas,   
         materias  
   WHERE ( ( hist_mat_inscritas.cuenta = :cuenta ) )    AND
           ( hist_mat_inscritas.cve_mat = materias.cve_mat )
   ORDER BY  hist_mat_inscritas.anio, hist_mat_inscritas.periodo
         
" arguments=(("cuenta", number)) )
