table(column=(type=long updatewhereclause=yes name=mat_inscritas_cve_mat dbname="mat_inscritas.cve_mat" dbalias=".cve_mat" )
 column=(type=char(2) updatewhereclause=yes name=mat_inscritas_gpo dbname="mat_inscritas.gpo" dbalias=".gpo" )
 column=(type=long updatewhereclause=yes name=grupos_tipo dbname="grupos.tipo" dbalias=".tipo" )
 column=(type=char(42) updatewhereclause=yes name=materias_materia dbname="materias.materia" dbalias=".materia" )
 column=(type=long updatewhereclause=yes name=materias_creditos dbname="materias.creditos" dbalias=".creditos" )
 column=(type=long updatewhereclause=yes name=mat_inscritas_cve_condicion dbname="mat_inscritas.cve_condicion" dbalias=".cve_condicion" values="NORMAL	0/BAJA ACADEMICA	1/BAJA TOTAL	2/" )
 column=(type=long updatewhereclause=yes name=academicos_cve_carrera dbname="academicos.cve_carrera" dbalias=".cve_carrera" )
 column=(type=char(100) updatewhereclause=yes name=carreras_carrera dbname="carreras.carrera" dbalias=".carrera" )
 column=(type=long updatewhereclause=yes name=horario_hora_inicio dbname="horario.hora_inicio" dbalias=".hora_inicio" )
 column=(type=long updatewhereclause=yes name=horario_hora_final dbname="horario.hora_final" dbalias=".hora_final" )
 column=(type=long updatewhereclause=yes name=horario_cve_dia dbname="horario.cve_dia" dbalias=".cve_dia" )
 column=(type=char(16) updatewhereclause=yes name=horario_cve_salon dbname="horario.cve_salon" dbalias=".cve_salon" )
 retrieve="SELECT mat_inscritas.cve_mat,   
       mat_inscritas.gpo,  
	   grupos.tipo, 
       materias.materia,   
	   materias.creditos,
	   mat_inscritas.cve_condicion,   
	   academicos.cve_carrera,   
	   carreras.carrera, 
	   horario.hora_inicio,   
	   horario.hora_final,   
	   horario.cve_dia,   
	   horario.cve_salon
FROM mat_inscritas 
LEFT JOIN horario ON ((mat_inscritas.cve_mat = horario.cve_mat)
					   AND (mat_inscritas.gpo = horario.gpo)
					   AND (mat_inscritas.periodo = horario.periodo)
					   AND (mat_inscritas.anio = horario.anio)),   
materias  ,
academicos,   
carreras,
grupos 
WHERE (mat_inscritas.cuenta = :cuenta) 
AND (mat_inscritas.cve_mat = materias.cve_mat)	
AND (mat_inscritas.cuenta = academicos.cuenta)
AND (academicos.cve_carrera = carreras.cve_carrera)
AND (mat_inscritas.cve_mat = grupos.cve_mat)
AND (mat_inscritas.gpo = grupos.gpo) 
AND (mat_inscritas.anio = grupos.anio) 
AND (mat_inscritas.periodo = grupos.periodo)   
UNION 
SELECT mat_inscritas.cve_mat,   
       mat_inscritas.gpo,
	   grupos.tipo,   
	   (SELECT materias.materia FROM materias WHERE materias.cve_mat = grupos.cve_asimilada) materia,
	   (SELECT materias.creditos FROM materias WHERE materias.cve_mat = grupos.cve_asimilada) creditos,
	   mat_inscritas.cve_condicion, 
	   academicos.cve_carrera,   
       carreras.carrera, 
	   horario.hora_inicio,   
	   horario.hora_final,   
	   horario.cve_dia,   
	   horario.cve_salon
FROM mat_inscritas,   
grupos LEFT JOIN horario on (( grupos.cve_asimilada = horario.cve_mat) 
											AND ( grupos.gpo_asimilado = horario.gpo) 
											AND (grupos.periodo = horario.periodo) 
											AND (grupos.anio = horario.anio)),
academicos,   
carreras 
WHERE ((mat_inscritas.cuenta = :cuenta)
AND (mat_inscritas.cve_mat = grupos.cve_mat))
AND ((mat_inscritas.gpo = grupos.gpo)
AND (grupos.periodo = mat_inscritas.periodo)
AND (grupos.anio = mat_inscritas.anio))
AND ((grupos.cve_asimilada = horario.cve_mat)
AND (grupos.gpo_asimilado = horario.gpo)
AND (horario.periodo = mat_inscritas.periodo)
AND (horario.anio = mat_inscritas.anio))
AND (mat_inscritas.cuenta = academicos.cuenta)
AND (academicos.cve_carrera = carreras.cve_carrera)   
ORDER BY mat_inscritas.cve_mat ASC, horario.cve_dia ASC, horario.hora_inicio ASC  " arguments=(("cuenta", number)) )
