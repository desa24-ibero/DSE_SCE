 column=(type=char(70) updatewhereclause=yes name=padre_nombre_completo dbname="padre.nombre_completo" )
 retrieve="  SELECT academicos.cuenta,   
         academicos.cve_carrera,   
         carreras.carrera,   
         padre.apaterno,   
         padre.amaterno,   
         padre.nombre,   
         padre.calle,   
         padre.colonia,   
         padre.cod_postal,   
         padre.telefono,   
         0,   
         academicos.promedio,   
         academicos.creditos_cursados,   
         padre.nombre_completo  
    FROM academicos,   
         padre,   
         carreras  
   WHERE ( academicos.cuenta = padre.cuenta ) and  
         ( academicos.cve_carrera = carreras.cve_carrera ) and  
         (  academicos.cuenta in ( :cuenta ) )    
" arguments=(("cuenta", numberlist),("orden", number),("prom_on", number),("cred_on", number),("dir_on", number))  sort="academicos_cve_carrera A padre_nombre_completo A " )
