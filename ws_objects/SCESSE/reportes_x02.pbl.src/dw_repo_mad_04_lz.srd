 retrieve="  SELECT academicos.cuenta,   
         academicos.cve_carrera,   
         carreras.carrera,   
         padre.apaterno,   
         padre.amaterno,   
         padre.nombre,   
         padre.calle,   
         padre.colonia,   
         padre.cod_postal,   
         padre.telefono,   
         0,   
         academicos.promedio,   
         academicos.creditos_cursados,   
         domicilio.calle,   
         domicilio.colonia,   
         domicilio.cod_postal,   
         domicilio.telefono,   
         alumnos.amaterno,   
         alumnos.apaterno,   
         alumnos.nombre,   
         alumnos.nombre_completo  
    FROM academicos,   
         padre,   
         alumnos,   
         domicilio,   
         carreras  
   WHERE ( academicos.cuenta = padre.cuenta ) and  
         ( academicos.cuenta = alumnos.cuenta ) and  
         ( academicos.cuenta = domicilio.cuenta ) and  
         ( academicos.cve_carrera = carreras.cve_carrera ) and  
         ( academicos.cuenta in ( :cuenta ) )    
" arguments=(("cuenta", numberlist),("orden", number),("prom_on", number),("cred_on", number),("dir_on", number))  sort="academicos_cve_carrera A " )
