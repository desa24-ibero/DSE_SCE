 column=(type=long updatewhereclause=yes name=grupos_cond_gpo dbname="grupos.cond_gpo" )
 column=(type=long updatewhereclause=yes name=horario_cve_dia dbname="horario.cve_dia" values="Domingo	0/Lunes	1/Martes	2/Miércoles	3/Jueves	4/Viernes	5/Sábado	6/" )
 column=(type=char(16) updatewhereclause=yes name=horario_cve_salon dbname="horario.cve_salon" )
 column=(type=long updatewhereclause=yes name=horario_hora_inicio dbname="horario.hora_inicio" )
 column=(type=long updatewhereclause=yes name=horario_hora_final dbname="horario.hora_final" )
 column=(type=long updatewhereclause=yes name=horario_clase_aula dbname="horario.clase_aula" values="Salón	0/Taller	1/Laboratorio	2/Otro	3/" )
 retrieve="  SELECT materias.cve_mat,   
         materias.materia,   
         grupos.gpo,   
         grupos.cupo,   
         grupos.inscritos,   
         materias.sigla,   
         profesor.cve_profesor,   
         profesor.nombre,   
         profesor.apaterno,   
         profesor.amaterno,   
         profesor.nombre_completo,   
         grupos.tipo,   
         tipo_grupo.nombre_tipo,   
         grupos.cve_asimilada,   
         grupos.gpo_asimilado,   
         0,   
         materias.cve_coordinacion,   
         coordinaciones.coordinacion,   
         grupos.cond_gpo,   
         horario.cve_dia,   
         horario.cve_salon,   
         horario.hora_inicio,   
         horario.hora_final,   
         horario.clase_aula  
    FROM profesor,   
         materias,   
         horario,   
         grupos,   
         tipo_grupo,   
         coordinaciones  
   WHERE  ( (( grupos.cve_mat *= horario.cve_mat ) AND 
            ( grupos.gpo *= horario.gpo ))  OR
			  (( grupos.cve_asimilada *= horario.cve_mat ) AND 
            ( grupos.gpo_asimilado *= horario.gpo)) ) and  
         ( grupos.cve_mat = materias.cve_mat ) and  
         ( grupos.cve_profesor = profesor.cve_profesor ) and  
         ( horario.periodo = :periodo) and  
         ( horario.anio = :anio ) and  
         ( grupos.tipo = tipo_grupo.tipo ) and  
         ( materias.cve_coordinacion = coordinaciones.cve_coordinacion ) and  
         ( ( materias.cve_coordinacion in ( :coordinaciones ) ) AND  
         ( horario.clase_aula in ( :tipo_salon ) ) AND  
         ( grupos.periodo = :periodo ) AND  
         ( grupos.anio = :anio ) AND  
         ( materias.nivel in ( :nivel ) ) )    
" arguments=(("orden", number),("coordinaciones", numberlist),("tipo_salon", numberlist),("periodo", number),("anio", number),("nivel", stringlist))  sort="materias_cve_coordinacion A materias_cve_mat A grupos_gpo A profesor_cve_profesor A " )
