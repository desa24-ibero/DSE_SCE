 retrieve="  SELECT area_mat.cve_area,   
         materias.cve_mat,   
         materias.materia,   
         materias.creditos,   
         teoria_lab.cve_lab,   
         prerrequisitos.cve_prerreq,   
         prerrequisitos.enlace,   
         materias.sigla,   
         materias.horas_teoria,   
         materias.horas_practica,   
         mat_prerreq_pos.semestre_ideal,   
         prerrequisitos.orden,   
         mat_prerreq_pos.cursativa,  
         materias.sigla_anterior   ,
         materias.horas_individuales,   
         materias.horas_aula 
    FROM area_mat,   
         materias,   
         prerrequisitos,   
         teoria_lab,   
         mat_prerreq_pos  
   WHERE ( area_mat.cve_mat *= prerrequisitos.cve_mat) and  
         ( materias.cve_mat *= teoria_lab.cve_teoria) and  
         ( materias.cve_mat *= mat_prerreq_pos.cve_mat) and  
         ( area_mat.cve_mat = materias.cve_mat ) and  
         ( area_mat.cve_area = :cve_area ) AND  
         ( prerrequisitos.cve_carrera = :carrera ) AND  
         ( prerrequisitos.cve_plan = :plan ) AND  
         ( mat_prerreq_pos.cve_carrera = :carrera ) AND  
         ( mat_prerreq_pos.cve_plan = :plan )    
" arguments=(("cve_area", number),("carrera", number),("plan", number)) )
