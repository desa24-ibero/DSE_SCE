 retrieve="  SELECT hist_reingreso.cuenta,  
         alumnos.cuenta,
         alumnos.nombre,   
         alumnos.apaterno,   
         alumnos.amaterno,   
         academicos.cve_carrera,   
         hist_reingreso.cve_formaingreso,   
         hist_indulto.cve_indulto,   
         hist_reingreso.periodo_ing,   
         hist_reingreso.anio_ing  
    FROM hist_reingreso, alumnos, academicos, hist_indulto 
   WHERE ((hist_reingreso.anio_ing = :ano ) AND  
          (hist_reingreso.periodo_ing = :periodo)) AND
          (hist_reingreso.cuenta = alumnos.cuenta) AND
          (hist_reingreso.cuenta = academicos.cuenta) AND
          (hist_reingreso.cuenta *= hist_indulto.cuenta)
 
    
" arguments=(("ano", number),("periodo", number))  sort="academicos_cve_carrera A alumnos_cuenta A " )
