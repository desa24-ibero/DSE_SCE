 retrieve="SELECT salon_horario.cve_salon                     
FROM  salon_horario,salon                                                    
WHERE ((hora_inicio BETWEEN :horini AND :horfin) AND
      (cve_dia = :dia)) AND 
 		(salon_horario.cve_salon = salon.cve_salon) AND
      (cupo_max = :cupo) AND
      (clase_aula = ~"SALON~") AND 
      (bloqueado = 0 )                                    
GROUP BY salon_horario.cve_salon HAVING SUM(ocupado) = 0


" arguments=(("horini", number),("horfin", number),("dia", number),("cupo", number)) )
