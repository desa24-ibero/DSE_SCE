 retrieve="  SELECT horario.cve_mat,   
         horario.gpo,   
         horario.periodo,   
         horario.anio,   
         horario.cve_dia,   
         horario.cve_salon,   
         horario.hora_inicio,   
         horario.hora_final,   
         horario.clase_aula  
    FROM horario   
	WHERE horario.cve_mat = :Materia AND 
         horario.gpo = :Grupo AND
         horario.periodo = :Periodo AND  
         horario.anio = :Ano" update="horario" updatewhere=1 updatekeyinplace=no arguments=(("Materia", number),("Grupo", string),("Periodo", number),("Ano", number)) )
