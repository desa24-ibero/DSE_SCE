 retrieve="  SELECT dbo.historico.cuenta,   
         dbo.alumnos.nombre_completo,   
         dbo.historico.cve_mat,   
         dbo.materias.materia,   
         dbo.historico.gpo,   
         dbo.historico.periodo,   
         dbo.historico.anio,   
         dbo.historico.calificacion,   
         dbo.historico.observacion,   
         dbo.materias.evaluacion,
	     dbo.periodo.descripcion
    FROM dbo.alumnos,   
         dbo.historico,   
         dbo.materias,
		dbo.periodo
   WHERE ( dbo.alumnos.cuenta = dbo.historico.cuenta ) and  
         ( dbo.historico.cve_mat = dbo.materias.cve_mat ) and  
		( dbo.historico.periodo = dbo.periodo.periodo) AND
         ( dbo.historico.calificacion LIKE ~"IN~" ) AND 
		( dbo.historico.anio <= :anio ) AND
		( dbo.historico.periodo <= :periodo ) AND
		( dbo.periodo.tipo=:tipo_per)
" update="dbo.historico" updatewhere=0 updatekeyinplace=yes filter=" historico_cve_mat NOT IN (8763,3748)"arguments=(("anio", number),("periodo", number),("tipo_per", string)) )
