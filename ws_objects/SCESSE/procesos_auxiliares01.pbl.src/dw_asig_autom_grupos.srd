table(column=(type=long updatewhereclause=yes name=cve_mat dbname="grupos.cve_mat" )
 column=(type=char(2) updatewhereclause=yes name=gpo dbname="grupos.gpo" )
 column=(type=long updatewhereclause=yes name=cond_gpo dbname="grupos.cond_gpo" values="Cancelado	0/Activo	1/" )
 column=(type=long updatewhereclause=yes name=cupo dbname="grupos.cupo" )
 column=(type=long updatewhereclause=yes name=grupos_inscritos dbname="grupos.inscritos" )
 retrieve="  SELECT grupos.cve_mat,   
         grupos.gpo,   
         grupos.cond_gpo,   
         grupos.cupo,
         grupos.inscritos
    FROM grupos, horario, materias   
   WHERE ( grupos.periodo 	= :periodo ) AND  
         ( grupos.anio 		= :ano ) AND
         ( horario.periodo = :periodo ) AND  
         ( horario.anio 	= :ano ) AND
         ( grupos.tipo 		= 0 ) AND  
         ( grupos.cond_gpo	= 1 )	AND
         ( grupos.cve_mat = horario.cve_mat and grupos.gpo = horario.gpo and
           grupos.cve_mat = materias.cve_mat and materias.nivel = :Nivel) and
			  (horario.clase_aula = 0 and cve_salon =null) 
group by grupos.cve_mat,grupos.gpo,cupo,inscritos" arguments=(("periodo", number),("ano", number),("nivel", string))  sort="cupo D " )
