 column=(type=long updatewhereclause=yes key=yes name=hora_inicio dbname="horario.hora_inicio" )
 column=(type=long updatewhereclause=yes name=hora_final dbname="horario.hora_final" )
 column=(type=long updatewhereclause=yes name=clase_aula dbname="horario.clase_aula" )
 retrieve="  SELECT horario.cve_mat,   
         horario.gpo,   
         horario.periodo,   
         horario.anio, 
         horario.cve_dia,   
         horario.cve_salon,   
         horario.hora_inicio,   
         horario.hora_final,
         horario.clase_aula
    FROM horario  
    WHERE (horario.periodo = :Periodo AND horario.anio = :Ano) AND  
          (horario.cve_mat = :Materia AND horario.gpo = :Grupo AND
           horario.cve_salon = NULL)
" update="horario" updatewhere=1 updatekeyinplace=yes arguments=(("Periodo", number),("Ano", number),("Materia", number),("Grupo", string)) )
