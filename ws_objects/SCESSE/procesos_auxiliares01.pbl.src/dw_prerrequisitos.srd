 retrieve="  SELECT prerrequisitos.cve_mat,   
         prerrequisitos.cve_carrera,   
         prerrequisitos.cve_plan,   
         prerrequisitos.cve_prerreq,   
         prerrequisitos.enlace,
			prerrequisitos.orden
    FROM prerrequisitos   
WHERE (prerrequisitos.cve_mat = :Materia) and   
      (prerrequisitos.cve_carrera = :Carrera) and   
      (prerrequisitos.cve_plan = :Plan) 
order by orden
" arguments=(("materia", number),("carrera", number),("plan", number))  sort="orden A " )
