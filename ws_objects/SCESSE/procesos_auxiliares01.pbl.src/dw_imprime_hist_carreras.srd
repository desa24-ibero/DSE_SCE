 retrieve="  	SELECT hist_carreras.cuenta,   
         alumnos.cuenta,   
         alumnos.nombre,   
         alumnos.apaterno,   
         alumnos.amaterno,   
         hist_carreras.cve_formaingreso,   
         hist_carreras.cve_carrera_ant,   
         hist_carreras.cve_carrera_act,   
         hist_carreras.periodo_ing,   
         hist_carreras.anio_ing  
    	FROM alumnos,   hist_carreras  
   	WHERE (hist_carreras.anio_ing    	= :ano)  AND
         			(hist_carreras.periodo_ing = :Periodo) AND         
         			(hist_carreras.cuenta      	= alumnos.cuenta)
 	ORDER BY  hist_carreras.cve_carrera_act,hist_carreras.cuenta
" arguments=(("periodo", number),("ano", number)) )
