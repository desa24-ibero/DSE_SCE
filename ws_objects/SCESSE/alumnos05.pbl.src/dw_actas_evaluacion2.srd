 column=(type=char(42) updatewhereclause=yes name=materias_materia dbname="materias.materia" )
 retrieve="  SELECT examen_extrao_titulo.cve_mat,   
         COUNT(examen_extrao_titulo.cve_mat),   
         alumnos.cuenta,   
         alumnos.nombre_completo,
			1,
			coordinaciones.coordinacion,materias.materia 
    FROM alumnos,   
         coordinaciones,   
         examen_extrao_titulo,   
         materias  
   WHERE ( alumnos.cuenta = examen_extrao_titulo.cuenta ) and  
         ( examen_extrao_titulo.cve_mat = materias.cve_mat ) and  
         ( coordinaciones.cve_coordinacion = materias.cve_coordinacion ) and
			( examen_extrao_titulo.anio = :anio ) and
			( examen_extrao_titulo.periodo = :periodo ) and  
         ( ( examen_extrao_titulo.tipo_examen = :tipo ) )   
GROUP BY examen_extrao_titulo.cve_mat,   
			examen_extrao_titulo.tipo_examen,  
         coordinaciones.cve_coordinacion,
			coordinaciones.coordinacion,materias.materia 
  HAVING ( examen_extrao_titulo.cuenta = alumnos.cuenta  and
			 coordinaciones.cve_coordinacion = materias.cve_coordinacion and
			 examen_extrao_titulo.cve_mat = materias.cve_mat and
			 examen_extrao_titulo.tipo_examen = :tipo)   
ORDER BY coordinaciones.cve_coordinacion ASC,
			examen_extrao_titulo.cve_mat ASC,   
         alumnos.nombre_completo ASC   
" arguments=(("tipo", number),("anio", number),("periodo", number)) )
