end type

on m_comprobante.create
this.Text="Comprobante"
this.m_imrpimir=create m_imrpimir
this.m_configurarimpresora=create m_configurarimpresora
this.Item[]={this.m_imrpimir, &
this.m_configurarimpresora}
end on

on m_comprobante.destroy
destroy(this.m_imrpimir)
destroy(this.m_configurarimpresora)
end on

