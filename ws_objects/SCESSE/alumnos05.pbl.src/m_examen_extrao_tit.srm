global m_examen_extrao_tit m_examen_extrao_tit

on m_examen_extrao_tit.create
m_examen_extrao_tit=this
this.m_comprobante=create m_comprobante
this.m_bajas=create m_bajas
this.m_salir=create m_salir
this.Item[]={this.m_comprobante, &
this.m_bajas, &
this.m_salir}
end on

on m_examen_extrao_tit.destroy
destroy(this.m_comprobante)
destroy(this.m_bajas)
destroy(this.m_salir)
end on

