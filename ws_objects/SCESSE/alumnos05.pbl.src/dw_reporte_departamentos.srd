 column=(type=long updatewhereclause=yes name=mat_inscritas_cve_mat dbname="mat_inscritas.cve_mat" )
 column=(type=char(2) updatewhereclause=yes name=mat_inscritas_gpo dbname="mat_inscritas.gpo" )
 column=(type=long updatewhereclause=yes name=compute_0005 dbname="compute_0005" )
 column=(type=char(42) updatewhereclause=yes name=materias_materia dbname="materias.materia" )
 retrieve="  SELECT coordinaciones.cve_coordinacion,
			coordinaciones.coordinacion,   
         mat_inscritas.cve_mat,
			mat_inscritas.gpo,   
         COUNT(mat_inscritas.cuenta),   
         materias.materia  
    FROM coordinaciones,   
         mat_inscritas,   
         materias  
   WHERE ( mat_inscritas.cve_mat = materias.cve_mat ) and  
         ( coordinaciones.cve_coordinacion = materias.cve_coordinacion ) and 
			( materias .nivel LIKE ~"P~" ) and
			( mat_inscritas.cve_mat NOT IN (SELECT cve_lab FROM teoria_lab WHERE requiere_acta = 0 )) and
         ( ( mat_inscritas.cve_condicion = 0 ) )   
GROUP BY coordinaciones.cve_coordinacion,
			mat_inscritas.cve_mat,
			materias.materia,
			mat_inscritas.gpo,
			coordinaciones.coordinacion  
ORDER BY coordinaciones.cve_coordinacion ASC,
			mat_inscritas.cve_mat ASC,
			mat_inscritas.gpo ASC" )
