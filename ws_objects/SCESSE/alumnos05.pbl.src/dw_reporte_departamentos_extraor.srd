 retrieve="  SELECT  coordinaciones.cve_coordinacion ,
           coordinaciones.coordinacion ,
           examen_extrao_titulo.cve_mat ,
           examen_extrao_titulo.gpo ,
           COUNT(examen_extrao_titulo.cuenta),
           materias.materia     
        FROM coordinaciones ,
           examen_extrao_titulo ,
           materias     
        WHERE ( examen_extrao_titulo.cve_mat = materias.cve_mat ) and
          ( materias.cve_coordinacion = coordinaciones.cve_coordinacion ) and
          ( materias.nivel LIKE :nivel ) and
          ( examen_extrao_titulo.tipo_examen = :tipo ) and
			 ( examen_extrao_titulo.anio = :anio ) and
			 ( examen_extrao_titulo.periodo = :periodo ) 
        GROUP BY coordinaciones.cve_coordinacion ,
           	examen_extrao_titulo.cve_mat ,
				coordinaciones.coordinacion,
				examen_extrao_titulo.cve_mat ,
           	examen_extrao_titulo.gpo,
				materias.materia
        ORDER BY coordinaciones.cve_coordinacion          ASC,
           examen_extrao_titulo.cve_mat          ASC,
           examen_extrao_titulo.gpo          ASC  " arguments=(("nivel", string),("tipo", number),("anio", number),("periodo", number))  sort="coordinaciones_cve_coordinacion A examen_extrao_titulo_cve_mat A " )
