 retrieve="  SELECT dbo.examen_extrao_titulo.cuenta,
			dbo.examen_extrao_titulo.cve_mat,   
         dbo.examen_extrao_titulo.gpo,   
         dbo.examen_extrao_titulo.periodo,   
         dbo.examen_extrao_titulo.anio,   
         dbo.examen_extrao_titulo.tipo_examen,  
         dbo.materias.materia,   
         dbo.materias.creditos
    FROM dbo.examen_extrao_titulo,   
         dbo.materias  
   WHERE ( dbo.examen_extrao_titulo.cve_mat = dbo.materias.cve_mat ) and  
         ( ( dbo.examen_extrao_titulo.cuenta = :cuenta ) and
			( dbo.examen_extrao_titulo.tipo_examen = :tipo ) and
			( dbo.examen_extrao_titulo.periodo = :periodo ) and
			( dbo.examen_extrao_titulo.anio = :anio ) )    
" update="dbo.examen_extrao_titulo" updatewhere=2 updatekeyinplace=yes arguments=(("cuenta", number),("anio", number),("periodo", number),("tipo", number)) )
