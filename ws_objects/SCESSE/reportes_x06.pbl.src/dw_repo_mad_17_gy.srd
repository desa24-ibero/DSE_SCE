 column=(type=char(100) updatewhereclause=yes name=carreras_carrera dbname="carreras.carrera" )
 retrieve="  SELECT sum( materias.creditos),   
         academicos.cuenta,   
         academicos.cve_carrera,   
         academicos.creditos_cursados,   
         plan_estudios.cred_serv_social,   
         alumnos.apaterno,   
         alumnos.amaterno,   
         alumnos.nombre,   
         alumnos.nombre_completo,   
         banderas.cve_flag_serv_social,   
         carreras.carrera  
    FROM mat_inscritas,   
         materias,   
         plan_estudios,   
         banderas,   
         academicos,   
         alumnos,   
         carreras  
   WHERE ( academicos.cuenta = alumnos.cuenta ) and  
         ( academicos.cuenta = mat_inscritas.cuenta ) and  
         ( academicos.cuenta = banderas.cuenta ) and  
         ( academicos.cve_carrera = plan_estudios.cve_carrera ) and  
         ( academicos.cve_plan = plan_estudios.cve_plan ) and  
         ( mat_inscritas.cve_mat = materias.cve_mat ) and  
         ( academicos.cve_carrera = carreras.cve_carrera ) and  
         ( banderas.cve_flag_serv_social = 0 ) AND  
         ( carreras.nivel = ~"L~" )   
GROUP BY academicos.cuenta,   
         academicos.cve_carrera,   
         academicos.creditos_cursados,   
         plan_estudios.cred_serv_social,   
         alumnos.apaterno,   
         alumnos.amaterno,   
         alumnos.nombre,   
         alumnos.nombre_completo,   
         banderas.cve_flag_serv_social,   
         carreras.carrera  
" filter=" academicos_creditos_cursados + compute_0001 >= plan_estudios_cred_serv_social "  sort="academicos_cve_carrera A alumnos_nombre_completo A " )
