 column=(type=long updatewhereclause=yes name=historico_cve_mat dbname="historico.cve_mat" )
 retrieve="  SELECT DISTINCT academicos.cuenta,   
         academicos.cve_carrera,   
         carreras.carrera,   
         alumnos.nombre,   
         alumnos.apaterno,   
         alumnos.amaterno,   
         alumnos.nombre_completo,   
         academicos.creditos_cursados,   
         0,   
         academicos.cve_plan,   
         nombre_plan.nombre_plan,   
         historico.cve_mat  
    FROM academicos,   
         banderas,   
         historico,   
         alumnos,   
         nombre_plan,   
         carreras  
   WHERE ( academicos.cuenta = banderas.cuenta ) and  
         ( academicos.cuenta = historico.cuenta ) and  
         ( academicos.cuenta = alumnos.cuenta ) and  
         ( academicos.cve_plan = nombre_plan.cve_plan ) and  
         ( academicos.cve_carrera = carreras.cve_carrera ) and  
         ( banderas.cve_flag_serv_social = 2 ) AND  
         ( historico.cve_mat in ( :mat_serv ) ) AND  
         ( historico.calificacion <> ~"AC~")    
" arguments=(("mat_serv", numberlist))  sort="academicos_cve_plan A academicos_cve_carrera A alumnos_nombre_completo A " )
