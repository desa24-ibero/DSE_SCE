		  	 WHEN CONVERT(VARCHAR(3), horario_modular.fecha_fin, 107) = 'Apr' THEN 'Abr' + ' ' + CONVERT(VARCHAR(2), horario_modular.fecha_fin, 106)
		  	 WHEN CONVERT(VARCHAR(3), horario_modular.fecha_fin, 107) = 'Aug' THEN 'Ago' + ' ' + CONVERT(VARCHAR(2), horario_modular.fecha_fin, 106)
		  	 WHEN CONVERT(VARCHAR(3), horario_modular.fecha_fin, 107) = 'Dec' THEN 'Dic' + ' ' + CONVERT(VARCHAR(2), horario_modular.fecha_fin, 106)
		  	 ELSE CONVERT(VARCHAR(6), horario_modular.fecha_fin, 107)
	   END) AS fecha_duracion
FROM grupos 
INNER JOIN horario_modular ON (grupos.cve_mat = horario_modular.cve_mat OR grupos.cve_asimilada = horario_modular.cve_mat) 
AND (grupos.gpo = horario_modular.gpo OR grupos.gpo_asimilado = horario_modular.gpo)
AND grupos.periodo = horario_modular.periodo
AND grupos.anio = horario_modular.anio
LEFT JOIN salon ON salon.cve_salon = horario_modular.cve_salon
INNER JOIN dias ON dias.cve_dia = horario_modular.cve_dia
INNER JOIN materias ON grupos.cve_mat = materias.cve_mat
INNER JOIN coordinaciones ON materias.cve_coordinacion = coordinaciones.cve_coordinacion
WHERE grupos.periodo IN (:periodo)
AND grupos.anio IN (:anio)
AND materias.nivel IN (:nivel)
AND grupos.forma_imparte = :tipo_imp
AND grupos.sesionado = 'S'
AND materias.cve_coordinacion IN (:coordinaciones)
ORDER BY 1,2,3,5,6,18,19,20,10
*/

