 retrieve="  SELECT DISTINCT materias.cve_mat,   
         materias.materia,   
         grupos.gpo,   
         grupos.cupo,   
         grupos.inscritos,   
         materias.sigla,   
         profesor.cve_profesor,   
         profesor.nombre,   
         profesor.apaterno,   
         profesor.amaterno,   
         profesor.nombre_completo,   
         grupos.tipo,   
         tipo_grupo.nombre_tipo,   
         grupos.cve_asimilada,   
         grupos.gpo_asimilado,   
         materias.cve_coordinacion,   
         coordinaciones.coordinacion,   
         grupos.cond_gpo,
			materias.horas_reales,
			materias.nivel,
			0,
			profesor.cve_categoria    
    FROM grupos LEFT JOIN horario on (( grupos.cve_mat = horario.cve_mat) and  ( grupos.gpo = horario.gpo) and (grupos.periodo = horario.periodo) and (grupos.anio = horario.anio) AND ( horario.clase_aula in ( :tipo_salon ) )),
		profesor,   
         materias,   
         tipo_grupo,   
         coordinaciones  
   WHERE ( grupos.cve_mat = materias.cve_mat ) and  
         ( grupos.cve_profesor = profesor.cve_profesor ) and  
         ( grupos.tipo = tipo_grupo.tipo ) and  
         ( materias.cve_coordinacion = coordinaciones.cve_coordinacion ) and  
         ( ( materias.cve_coordinacion in ( :coordinaciones ) ) AND  
         ( grupos.periodo = :periodo ) AND  
         ( grupos.anio = :anio ) AND  
         ( materias.nivel in ( :nivel ) ) AND
			( profesor.cve_categoria IN (:categoria) ) )    
UNION
  SELECT DISTINCT materias.cve_mat,   
         materias.materia,   
