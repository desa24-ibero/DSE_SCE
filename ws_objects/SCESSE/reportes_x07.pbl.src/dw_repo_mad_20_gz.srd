    FROM grupos LEFT JOIN horario on (( grupos.cve_asimilada = horario.cve_mat) and ( grupos.gpo_asimilado = horario.gpo) and (grupos.periodo = horario.periodo) and (grupos.anio = horario.anio) AND ( horario.clase_aula in ( :tipo_salon ) )),
