 retrieve="  SELECT dbo.hist_grupos.cve_mat_gpo,
			dbo.hist_grupos.cve_mat,   
         dbo.hist_grupos.gpo,   
         dbo.materias.sigla,   
         dbo.materias.materia,   
         dbo.profesor.nombre_completo,   
         dbo.hist_horario.cve_dia,   
         dbo.hist_horario.hora_inicio,   
         dbo.hist_horario.hora_final,   
         dbo.hist_horario.cve_salon,
         dbo.hist_grupos.cupo,
         dbo.hist_grupos.inscritos,
			dbo.materias.cve_coordinacion,
			dbo.hist_grupos.tipo,
			dbo.tipo_grupo.nombre_tipo,
			dbo.hist_grupos.cve_profesor,
			dbo.profesor.nombre_completo,
			dbo.profesor.cve_categoria		
    FROM dbo.hist_grupos,   
         dbo.hist_horario,   
         dbo.materias,   
         dbo.profesor,
			dbo.tipo_grupo 
	WHERE ((( dbo.hist_grupos.cve_mat *= dbo.hist_horario.cve_mat) and  
         ( dbo.hist_grupos.gpo *= dbo.hist_horario.gpo)) or
			(( dbo.hist_grupos.cve_asimilada *= dbo.hist_horario.cve_mat) and  
         ( dbo.hist_grupos.gpo_asimilado *= dbo.hist_horario.gpo))) and 
			( dbo.hist_grupos.tipo = dbo.tipo_grupo.tipo) and
         ( dbo.hist_grupos.cve_mat = dbo.materias.cve_mat ) and
			( dbo.hist_grupos.cond_gpo = :cond ) and
			( dbo.hist_grupos.cve_profesor = dbo.profesor.cve_profesor) and  
         (( :cve_coordinacion = 9999 OR dbo.materias.cve_coordinacion = :cve_coordinacion ) AND
			( dbo.hist_grupos.anio = :ani  ) AND  
         ( dbo.hist_grupos.periodo = :peri ) AND  
         ( dbo.hist_horario.anio = :ani ) AND  
         ( dbo.hist_horario.periodo = :peri ))" arguments=(("cve_coordinacion", number),("peri", number),("ani", number),("cond", number)) )
