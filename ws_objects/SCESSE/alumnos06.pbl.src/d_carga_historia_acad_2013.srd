 retrieve="  SELECT historico.cuenta,   
         historico.cve_mat,   
         historico.gpo,   
         historico.periodo,   
         historico.anio,   
         historico.cve_carrera,   
         historico.cve_plan,   
         historico.calificacion,   
         historico.observacion,   
         periodo.descripcion  
    FROM historico,   
         periodo  
   WHERE ( historico.periodo = periodo.periodo ) and  
         ( ( historico.cuenta = :cuenta ) AND  
         ( historico.cve_carrera = :carrera ) )    
" update="historico" updatewhere=1 updatekeyinplace=yes arguments=(("cuenta", number),("carrera", number)) )
