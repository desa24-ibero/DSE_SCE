 column=(type=long updatewhereclause=yes key=yes name=anio dbname="historico.anio" )
 column=(type=char(2) update=yes updatewhereclause=yes name=historico_calificacion dbname="historico.calificacion" )
 column=(type=long update=yes updatewhereclause=yes name=historico_observacion dbname="historico.observacion" values="NO ACREDITADA	0/ACREDITADA	1/EXTRAORDINARIO	2/ORDINARIO	3/PRERREQ. INGLES	4/REVALIDADA	5/TIT. SUFICIENCIA	6/" )
 column=(type=long updatewhereclause=yes key=yes name=cve_carrera dbname="historico.cve_carrera" )
 column=(type=long updatewhereclause=yes key=yes name=cve_plan dbname="historico.cve_plan" )
 retrieve="  SELECT historico.cuenta,
			historico.cve_mat,   
         historico.gpo,   
         historico.periodo,    
			historico.anio, 
         historico.calificacion,   
         historico.observacion ,
			historico.cve_carrera,
			historico.cve_plan
    FROM historico
   WHERE (historico.cuenta  = :Cuenta) AND
         (historico.cve_mat = :Materia) AND
         (historico.cve_carrera = :an_cve_carrera) AND
         (historico.cve_plan = :an_cve_plan) 			
   ORDER BY anio DESC, periodo DESC
      

" update="historico" updatewhere=0 updatekeyinplace=yes arguments=(("Cuenta", number),("Materia", number),("an_cve_carrera", number),("an_cve_plan", number)) )
