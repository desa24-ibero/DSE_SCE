 retrieve="  SELECT space(6-char_length(convert(varchar,profesor.cve_profesor)))+ convert(varchar,profesor.cve_profesor),   
         space(4-char_length(convert(varchar,/*deptos_equiv.cve_depto_ant*/materias.cve_coordinacion)))+ convert(varchar,/*deptos_equiv.cve_depto_ant*/materias.cve_coordinacion),   
         space(4-char_length(convert(varchar,materias.cve_mat)))+ convert(varchar,materias.cve_mat),   
         char(ascii(materias.nivel)-((ascii(materias.nivel)/80)*9)),   
         space(2-char_length(convert(varchar,convert(int,profesor_auxiliar.horas))))+ convert(varchar,convert(int,profesor_auxiliar.horas)),   
         convert(char(1),~"O~"),   
         convert(char(8),:fechastring),    
         space(6-char_length(materias.sigla))+ materias.sigla,   
         convert(char(35),materias.materia)+~".~",   
         space(2-char_length(grupos.gpo))+grupos.gpo,   
         categoria = CASE profesor_auxiliar.cve_categoria_auxiliar
                                            WHEN  1 THEN ~"T~"
														  WHEN  2 THEN ~"C~"
														  WHEN  3 THEN ~"L~"
														  WHEN  4 THEN ~"A~"
														  ELSE ~" ~" END,
			~"******~"  
    FROM profesor,   
         materias,   
         grupos,
			profesor_auxiliar,
			bit_profesor_auxiliar bpa1
   WHERE ( materias.cve_mat = grupos.cve_mat ) and
         ( profesor.cve_profesor = profesor_auxiliar.cve_profesor ) and  
			( grupos.cve_mat = profesor_auxiliar.cve_mat) and
			( grupos.gpo = profesor_auxiliar.gpo) and
			( grupos.periodo = profesor_auxiliar.periodo) and
			( grupos.anio = profesor_auxiliar.anio) and
          ( grupos.tipo in ( 0, 1, 2, 3) ) AND  
         ( profesor.cve_profesor >= 9 ) AND  
         ( grupos.cond_gpo = 1 ) AND  
         ( grupos.inscritos > 0 ) AND
			( grupos.anio = :anio ) AND
			( grupos.periodo = :periodo) AND 
		  (( profesor_auxiliar.cve_mat = bpa1.cve_mat) AND
			( profesor_auxiliar.gpo = bpa1.gpo) AND
			( profesor_auxiliar.periodo = bpa1.periodo) AND
			( profesor_auxiliar.anio = bpa1.anio) AND
			( profesor_auxiliar.cve_profesor = bpa1.cve_profesor) AND
			( bpa1.fecha <= :fecha) AND
			( bpa1.movimiento in (~"IN~")))" arguments=(("anio", number),("periodo", number),("fecha", datetime),("fechastring", string)) )
