table(column=(type=long updatewhereclause=yes name=academicos_cuenta dbname="academicos.cuenta" dbalias=".cuenta" )
 column=(type=long updatewhereclause=yes name=academicos_cve_carrera dbname="academicos.cve_carrera" dbalias=".cve_carrera" )
 column=(type=char(100) updatewhereclause=yes name=carreras_carrera dbname="carreras.carrera" dbalias=".carrera" )
 column=(type=char(15) updatewhereclause=yes name=periodo_descripcion dbname="periodo.descripcion" dbalias=".descripcion" )
 column=(type=long updatewhereclause=yes name=mat_inscritas_periodo dbname="mat_inscritas.periodo" dbalias=".periodo" )
 column=(type=long updatewhereclause=yes name=mat_inscritas_anio dbname="mat_inscritas.anio" dbalias=".anio" )
 retrieve="  SELECT DISTINCT academicos.cuenta,   
         academicos.cve_carrera,   
         carreras.carrera,   
         periodo.descripcion,   
         mat_inscritas.periodo,   
         mat_inscritas.anio  
    FROM academicos,   
         banderas_inscrito,   
         plan_estudios,   
         carreras,   
         mat_inscritas,   
         periodo  
   WHERE ( academicos.cve_plan = plan_estudios.cve_plan ) and  
         ( academicos.cuenta = banderas_inscrito.cuenta ) and  
         ( plan_estudios.cve_carrera = carreras.cve_carrera ) and  
         ( carreras.cve_carrera = academicos.cve_carrera ) and  
         ( banderas_inscrito.cuenta = mat_inscritas.cuenta ) and  
         ( mat_inscritas.periodo = periodo.periodo ) and  
         ( ( academicos.sem_cursados > 0 ) AND  
         ( carreras.nivel in ( :Nivel ) ) AND  
         ( plan_estudios.incorporado_sep = 1 ) AND  
         ( banderas_inscrito.cuenta = mat_inscritas.cuenta ) AND  
         ( mat_inscritas.periodo in ( :periodo ) ) AND  
         ( mat_inscritas.periodo = periodo.periodo ) )    
" arguments=(("Nivel", stringlist),("periodo", numberlist))  sort="mat_inscritas_periodo A academicos_cve_carrera A " )
