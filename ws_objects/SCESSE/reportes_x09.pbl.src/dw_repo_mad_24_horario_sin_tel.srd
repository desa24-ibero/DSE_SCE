table(column=(type=char(9) updatewhereclause=yes name=dias_dia dbname="dias.dia" )
 column=(type=long updatewhereclause=yes name=horario_hora_inicio dbname="hora_inicio" )
 column=(type=long updatewhereclause=yes name=horario_hora_final dbname="hora_final" )
 column=(type=char(12) updatewhereclause=yes name=clase_aula_nombre_aula dbname="clase_aula.nombre_aula" )
 column=(type=char(16) updatewhereclause=yes name=horario_cve_salon dbname="cve_salon" )
 column=(type=char(15) updatewhereclause=yes name=fecha_duracion dbname="fecha_duracion" )
 retrieve="SELECT c.dia,
	   CASE WHEN ISNULL(d.sesionado,'N') = 'S' THEN e.hora_inicio
	   		ELSE a.hora_inicio
	   END AS hora_inicio,
	   CASE WHEN ISNULL(d.sesionado,'N') = 'S' THEN e.hora_final
	   		ELSE a.hora_final
	   END AS hora_final,
	   b.nombre_aula,
	   CASE WHEN ISNULL(d.sesionado,'N') = 'S' THEN e.cve_salon
	   		ELSE a.cve_salon
	   END AS cve_salon,
	   CASE WHEN ISNULL(d.sesionado,'N') = 'S' THEN ISNULL((CONVERT(VARCHAR(6), e.fecha_inicio, 107) + ' a ' + CONVERT(VARCHAR(6), e.fecha_fin, 107)),'')
			ELSE ISNULL((CONVERT(VARCHAR(6), d.fecha_inicio, 107) + ' a ' + CONVERT(VARCHAR(6), d.fecha_fin, 107)),'')
	   END AS fecha_duracion
FROM horario a
INNER JOIN clase_aula b ON b.clase = a.clase_aula
INNER JOIN dias c ON c.cve_dia = a.cve_dia
INNER JOIN grupos d ON d.anio = a.anio
AND d.periodo = a.periodo
AND d.gpo = a.gpo
AND	((d.cve_mat = a.cve_mat AND d.gpo = a.gpo) OR (d.cve_asimilada = a.cve_mat AND d.gpo_asimilado = a.gpo))
LEFT JOIN horario_modular e ON e.anio = a.anio
AND e.periodo = a.periodo
AND e.gpo = a.gpo
AND	(e.cve_mat = a.cve_mat AND e.gpo = a.gpo)
WHERE a.cve_mat = :cve_mat
AND a.gpo = :grupo
AND a.periodo = :periodo
AND a.anio = :anio
ORDER BY a.cve_dia" arguments=(("cve_mat", number),("grupo", string),("periodo", number),("anio", number)) )
