$PBExportComments$Datawindow para los Horarios ,recibe como parámetros Cve_mat, grupo,periodo,anio y regresa el Horario
release 5;
datawindow(units=0 timer_interval=0 color=1073741824 processing=0 print.documentname="" print.orientation = 0 print.margin.left = 110 print.margin.right = 110 print.margin.top = 97 print.margin.bottom = 97 print.paper.source = 0 print.paper.size = 0 print.prompt=no rows_per_detail = 3 )
header(height=1 color="536870912" )
summary(height=1 color="536870912" )
footer(height=1 color="536870912" )
detail(height=65 color="536870912" )
table(column=(type=char(9) updatewhereclause=yes name=dias_dia dbname="dias.dia" )
 column=(type=long updatewhereclause=yes name=horario_hora_inicio dbname="horario.hora_inicio" )
 column=(type=long updatewhereclause=yes name=horario_hora_final dbname="horario.hora_final" )
 column=(type=char(12) updatewhereclause=yes name=clase_aula_nombre_aula dbname="clase_aula.nombre_aula" )
 column=(type=char(16) updatewhereclause=yes name=horario_cve_salon dbname="horario.cve_salon" )
 retrieve="SELECT dias.dia,   
         horario.hora_inicio,   
         horario.hora_final,   
         clase_aula.nombre_aula,   
         horario.cve_salon  
    FROM horario,   
         clase_aula,   
         dias  
   WHERE ( horario.cve_dia = dias.cve_dia ) and  
         ( horario.clase_aula = clase_aula.clase ) and  
         ( ( horario.cve_mat = :cve_mat ) AND  
         ( horario.gpo = :grupo ) AND  
         ( horario.periodo = :periodo ) AND  
         ( horario.anio = :anio ) )    

	ORDER BY horario.cve_dia" arguments=(("cve_mat", number),("grupo", string),("periodo", number),("anio", number)) )
