 retrieve="SELECT dias.dia,   
         horario.hora_inicio,   
         horario.hora_final,   
         clase_aula.nombre_aula,   
         horario.cve_salon  
    FROM horario,   
         clase_aula,   
         dias  
   WHERE ( horario.cve_dia = dias.cve_dia ) and  
         ( horario.clase_aula = clase_aula.clase ) and  
         ( ( horario.cve_mat = :cve_mat ) AND  
         ( horario.gpo = :grupo ) AND  
         ( horario.periodo = :periodo ) AND  
         ( horario.anio = :anio ) )    

	ORDER BY horario.cve_dia" arguments=(("cve_mat", number),("grupo", string),("periodo", number),("anio", number)) )
