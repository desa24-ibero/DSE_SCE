 retrieve="	Select		grupo_clase.periodo,
			grupo_clase.anio,
			tipo_grupo_clase.cve_tipo_grupo_clase cve_tipo_grupo_clase,
			tipo_grupo_clase.tipo_grupo_clase,
			clase_aula.clase clase_aula,
			nombre_aula,
			dias.cve_dia cve_dia,
			dia,
			num_horas = sum(num_horas),
			num_grupos = 0,
			total_horas = 0,
			total_grupos = 0,
			porcentaje_horas = 0.000000,
			porcentaje_grupos = 0.000000,
			total_horas_bloque = 0,
			total_grupos_bloque = 0,
			total_horas_sin_aula = 0,
			total_grupos_sin_aula = 0,
			turno = '                  '
From		grupo_clase,
			tipo_grupo_clase,
			clase_aula,
			dias
Where	grupo_clase.cve_tipo_grupo_clase <> 9999 AND
			grupo_clase.anio = :an_anio AND
			grupo_clase.periodo = :an_periodo AND
			grupo_clase.cve_tipo_grupo_clase = tipo_grupo_clase.cve_tipo_grupo_clase AND
			grupo_clase.clase_aula = clase_aula.clase AND
			grupo_clase.cve_dia = dias.cve_dia AND
			( dias.cve_dia = :an_cve_dia OR 9999 = :an_cve_dia ) AND						
			( clase_aula.clase = :an_clase_aula OR 9999 = :an_clase_aula ) AND		
			( tipo_grupo_clase.cve_tipo_grupo_clase in (:an_cve_tipo_grupo_clase ) ) AND
			(	/* Filtro para Horario ( Matutino, Vespertino, o TODOS ) */
				(	/* Filtro para Horario Vespertino ( Se activa si argumento = 0 ) */
					grupo_clase.hora_inicio >= ( Select hora_inicio_vespertino from parametros_grupo_clase where cve_parametros_grupo_clase = 1 AND 0 = :an_horario_matutino ) OR
					:an_horario_matutino = 9999
				) AND
				(	/* Filtro para Horario Matutino ( Se activa si argumento = 0 ) */
	
					grupo_clase.hora_inicio < ( Select hora_inicio_vespertino from parametros_grupo_clase where cve_parametros_grupo_clase = 1 AND 0 = :an_horario_vespertino ) OR
					:an_horario_vespertino = 9999
				)
			) and
			( grupo_clase.num_inscritos <= :an_num_inscritos ) 								/* Filtro para número de Inscritos */
Group by grupo_clase.periodo,
			grupo_clase.anio,
			tipo_grupo_clase.cve_tipo_grupo_clase,
			tipo_grupo_clase.tipo_grupo_clase,
			clase_aula.clase,
			nombre_aula,
			dias.cve_dia,
			dia
Order BY tipo_grupo_clase.cve_tipo_grupo_clase,clase_aula.clase
