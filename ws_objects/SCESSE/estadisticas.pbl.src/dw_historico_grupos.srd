 retrieve="  SELECT hist_grupos.cve_mat,   
         materias.materia,   
         SUM(hist_grupos.inscritos)  
    FROM hist_grupos,   
         materias  
   WHERE ( hist_grupos.cve_mat = materias.cve_mat ) and  
         ( ( hist_grupos.periodo = :periodo ) AND  
         ( hist_grupos.anio = :anio ) AND  
         ( hist_grupos.tipo <> 2 ) AND  
         ( hist_grupos.cond_gpo = 1 ) )   
GROUP BY hist_grupos.cve_mat,   
         materias.materia   
" arguments=(("periodo", number),("anio", number)) )
