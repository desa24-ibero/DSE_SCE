 column=(type=number updatewhereclause=yes name=total_grupos_clase dbname="total_grupos_clase" )
 retrieve="Select		tipo_grupo_clase.cve_tipo_grupo_clase cve_tipo_grupo_clase,
			tipo_grupo_clase.tipo_grupo_clase,
			clase_aula.clase clase_aula,
			nombre_aula,
			dias.cve_dia cve_dia,
			dia,
			Sum ( num_bloques ) as num_bloques,
			( select sum( num_bloques ) from grupo_clase where cve_tipo_grupo_clase = 1 and anio = :an_anio and periodo = :an_periodo ) as total_base_recomendado,
			( select sum( num_bloques ) from grupo_clase where cve_tipo_grupo_clase = 2 and anio = :an_anio and periodo = :an_periodo ) as total_base_extendido,
			( select sum( num_bloques ) from grupo_clase where cve_tipo_grupo_clase = 3 and anio = :an_anio and periodo = :an_periodo ) as total_transgrede_inicio,
			( select sum( num_bloques ) from grupo_clase where cve_tipo_grupo_clase = 4 and anio = :an_anio and periodo = :an_periodo ) as total_transgrede_fin,
			( select sum( num_bloques ) from grupo_clase where cve_tipo_grupo_clase = 5 and anio = :an_anio and periodo = :an_periodo ) as total_transgrede_inicio_y_fin,
			( select sum( num_bloques ) from grupo_clase where cve_tipo_grupo_clase = 6 and anio = :an_anio and periodo = :an_periodo ) as total_ext_pero_transgrede,
			( select sum( num_bloques ) from grupo_clase where cve_tipo_grupo_clase = 7 and anio = :an_anio and periodo = :an_periodo ) as total_inician_13_o_14,
			( select sum( num_bloques ) from grupo_clase where cve_tipo_grupo_clase <> 9999 and anio = :an_anio and periodo = :an_periodo ) as total_grupos_clase
From		grupo_clase,
			tipo_grupo_clase,
			clase_aula,
			dias
Where	grupo_clase.cve_tipo_grupo_clase <> 9999 AND
			grupo_clase.anio = :an_anio AND
			grupo_clase.periodo = :an_periodo AND
			grupo_clase.cve_tipo_grupo_clase = tipo_grupo_clase.cve_tipo_grupo_clase AND
			grupo_clase.clase_aula = clase_aula.clase AND
			grupo_clase.cve_dia = dias.cve_dia AND
			( dias.cve_dia = :an_cve_dia OR :an_cve_dia = 9999 ) AND						/* Filtro para Día de la semana */
			( clase_aula.clase = :an_clase_aula OR :an_clase_aula = 9999 ) AND		/* Filtro para Clase de Aula */
			( tipo_grupo_clase.cve_tipo_grupo_clase in ( :an_cve_tipo_grupo_clase ) )
Group by tipo_grupo_clase.cve_tipo_grupo_clase,
			tipo_grupo_clase.tipo_grupo_clase,
			clase_aula.clase,
			nombre_aula,
			dias.cve_dia,
			dia
Order BY tipo_grupo_clase.cve_tipo_grupo_clase,clase_aula.clase" arguments=(("an_anio", number),("an_periodo", number),("an_cve_dia", number),("an_clase_aula", number),("an_cve_tipo_grupo_clase", numberlist)) )
