 retrieve="  SELECT dbo.examen_extrao_titulo.cuenta,   
         dbo.examen_extrao_titulo.cve_mat,   
         dbo.examen_extrao_titulo.gpo,   
         dbo.examen_extrao_titulo.periodo,   
         dbo.examen_extrao_titulo.anio,   
         dbo.examen_extrao_titulo.calificacion,   
         dbo.examen_extrao_titulo.tipo_examen,   
         dbo.examen_extrao_titulo.cve_condicion,   
         dbo.v_sce_alumno_digito.digito,   
         dbo.academicos.cve_carrera,   
         dbo.carreras.carrera,   
         dbo.tipo_examen.tipo_examen,   
         dbo.alumnos.nombre_completo,   
         dbo.materias.materia,   
         dbo.mat_prerreq_pos.cursativa  
    FROM dbo.examen_extrao_titulo,   
         dbo.academicos,   
         dbo.alumnos,   
         dbo.carreras,   
         dbo.v_sce_alumno_digito,   
         dbo.tipo_examen,   
         dbo.condicion_insc,   
         dbo.materias,   
         dbo.mat_prerreq_pos  
   WHERE ( dbo.examen_extrao_titulo.cuenta = dbo.academicos.cuenta ) and  
         ( dbo.academicos.cuenta = dbo.alumnos.cuenta ) and  
         ( dbo.academicos.cve_carrera = dbo.carreras.cve_carrera ) and  
         ( dbo.alumnos.cuenta = dbo.v_sce_alumno_digito.cuenta ) and  
         ( dbo.examen_extrao_titulo.tipo_examen = dbo.tipo_examen.cve_tipo_examen ) and  
         ( dbo.examen_extrao_titulo.cve_condicion = dbo.condicion_insc.cond_insc ) and  
         ( dbo.examen_extrao_titulo.cve_mat = dbo.materias.cve_mat ) and  
         ( dbo.materias.cve_mat = dbo.mat_prerreq_pos.cve_mat ) and  
         ( dbo.academicos.cve_carrera = dbo.mat_prerreq_pos.cve_carrera ) and  
         ( dbo.academicos.cve_plan = dbo.mat_prerreq_pos.cve_plan ) and  
         (dbo.materias.cve_coordinacion = :an_cve_coordinacion OR  
         :an_cve_coordinacion = 9999) AND  
         dbo.examen_extrao_titulo.periodo = :an_periodo AND  
         dbo.examen_extrao_titulo.anio = :an_anio    
" arguments=(("an_cve_coordinacion", number),("an_periodo", number),("an_anio", number)) )
