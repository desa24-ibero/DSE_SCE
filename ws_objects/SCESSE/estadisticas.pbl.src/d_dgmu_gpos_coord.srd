 retrieve="SELECT   coordinaciones.cve_coordinacion, 
          coordinaciones.coordinacion,   
			 materias.cve_mat,
			 materias.materia,
          numero= COUNT(*)  
    FROM  coordinaciones,   
			 materias ,
			 grupos   
   WHERE (  coordinaciones.cve_coordinacion =  materias.cve_coordinacion ) and  
			(  coordinaciones.cve_coordinacion = :an_cve_coordinacion or
            :an_cve_coordinacion = 9999 ) and
 			(  grupos.cve_mat =  materias.cve_mat ) and  
			(  grupos.periodo = :an_periodo) and
			(  grupos.anio = :an_anio) and
			(  grupos.cond_gpo in (1))
	GROUP BY 
			 coordinaciones.cve_coordinacion,   
          coordinaciones.coordinacion,
			 materias.cve_mat,
			 materias.materia" arguments=(("an_cve_coordinacion", number),("an_periodo", number),("an_anio", number)) )
