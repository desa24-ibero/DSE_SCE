table(column=(type=long updatewhereclause=yes name=hist_grupos_cve_mat dbname="hist_grupos.cve_mat" )
 column=(type=char(42) updatewhereclause=yes name=materias_materia dbname="materias.materia" )
 column=(type=long updatewhereclause=yes name=hist_grupos_periodo dbname="hist_grupos.periodo" values="PRIMAVERA	0/VERANO	1/OTOÑO	2/" )
 column=(type=long updatewhereclause=yes name=hist_grupos_anio dbname="hist_grupos.anio" )
 column=(type=long updatewhereclause=yes name=compute_0005 dbname="compute_0005" )
 retrieve="  SELECT hist_grupos.cve_mat,   
         materias.materia,   
         hist_grupos.periodo,   
         hist_grupos.anio,   
         ~"Tot. Abiertos~"=count(hist_grupos.cve_mat)  
    FROM hist_grupos,   
         materias  
   WHERE ( hist_grupos.cve_mat = materias.cve_mat ) and  
         ( hist_grupos.anio = :ano AND hist_grupos.periodo = :periodo) AND  
         (hist_grupos.cond_gpo = 1) AND  
         (hist_grupos.tipo <> 2)   
GROUP BY hist_grupos.cve_mat,   
         materias.materia,   
         hist_grupos.periodo,   
         hist_grupos.anio   
" arguments=(("ano", number),("periodo", number)) )
