table(column=(type=char(100) updatewhereclause=yes name=carreras_carrera dbname="carreras.carrera" dbalias=".carrera" )
 column=(type=long updatewhereclause=yes name=academicos_cve_carrera dbname="academicos.cve_carrera" dbalias=".cve_carrera" )
 column=(type=long updatewhereclause=yes name=tot_inscritos dbname="tot_inscritos" )
 retrieve="SELECT carreras.carrera,   academicos.cve_carrera,   count(banderas_inscrito.cuenta)  as tot_inscritos  
FROM academicos,    banderas_inscrito,   carreras    
WHERE ( banderas_inscrito.cuenta = academicos.cuenta ) 
and    ( academicos.cve_carrera = carreras.cve_carrera ) 
and    (academicos.nivel IN('T' ))   
and  EXISTS(SELECT * FROM mat_inscritas  
WHERE mat_inscritas.cve_condicion IN (0,1,3)  
AND mat_inscritas.periodo IN(2)  
AND mat_inscritas.cuenta =  banderas_inscrito.cuenta)  
GROUP BY carreras.carrera,     academicos.cve_carrera    

