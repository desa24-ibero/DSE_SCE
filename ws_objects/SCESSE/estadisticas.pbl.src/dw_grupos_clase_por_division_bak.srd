 column=(type=long updatewhereclause=yes name=num_inscritos dbname="grupo_clase.num_inscritos" )
 retrieve="Select		divisiones.cve_division cve_division,
			division,
			departamentos.cve_depto cve_depto,
			departamento,
			coordinaciones.cve_coordinacion cve_coordinacion,
			coordinacion,
			tipo_grupo_clase.cve_tipo_grupo_clase cve_tipo_grupo_clase,
			tipo_grupo_clase.tipo_grupo_clase,
			clase_aula.clase clase_aula,
			nombre_aula,
			dias.cve_dia cve_dia,
			dia,
			hora_inicio,
			hora_final,
			num_horas,
			num_bloques,
			num_inscritos
From		grupo_clase,
			coordinaciones,
			departamentos,
			divisiones,
			tipo_grupo_clase,
			clase_aula,
			dias
Where	grupo_clase.cve_tipo_grupo_clase <> 9999 AND
			grupo_clase.anio = :an_anio AND
			grupo_clase.periodo = :an_periodo AND
			grupo_clase.cve_coordinacion = coordinaciones.cve_coordinacion AND
			coordinaciones.cve_depto = departamentos.cve_depto AND
			departamentos.cve_division = divisiones.cve_division AND
			grupo_clase.cve_tipo_grupo_clase = tipo_grupo_clase.cve_tipo_grupo_clase AND
			grupo_clase.clase_aula = clase_aula.clase AND
			grupo_clase.cve_dia = dias.cve_dia AND
			( coordinaciones.cve_coordinacion = 9999 OR 9999 = 9999 ) AND						/* Filtro para Coordinación */
			( departamentos.cve_depto = 9999 OR 9999 = 9999 ) AND								/* Filtro para Departamento */
			( divisiones.cve_division = :an_cve_division OR :an_cve_division = 9999 ) AND		/* Filtro para Division */
			( dias.cve_dia = :an_cve_dia OR :an_cve_dia = 9999 ) AND								/* Filtro para Día de la semana */
			( grupo_clase.num_inscritos <= :an_num_inscritos ) AND								/* Filtro para número de Inscritos */
			( clase_aula.clase = :an_clase_aula OR :an_clase_aula = 9999 ) AND				/* Filtro para Clase de Aula */
			(	/* Filtro para Horario ( Maturino, Vespertino, o TODOS ) */
				(	/* Filtro para Horario Vespertino ( Se activa si argumento = 0 ) */
					grupo_clase.hora_inicio >= ( Select hora_inicio_vespertino from parametros_grupo_clase where cve_parametros_grupo_clase = 1 AND 0 = :an_horario_matutino ) OR
					:an_horario_matutino = 9999
				) AND
				(	/* Filtro para Horario Matutino ( Se activa si argumento = 0 ) */
	
					grupo_clase.hora_inicio < ( Select hora_inicio_vespertino from parametros_grupo_clase where cve_parametros_grupo_clase = 1 AND 0 = :an_horario_vespertino ) OR
					:an_horario_vespertino = 9999
				)
			)
Order by	division asc, departamento asc, coordinacion asc" arguments=(("an_anio", number),("an_periodo", number),("an_cve_dia", number),("an_num_inscritos", number),("an_clase_aula", number),("an_horario_matutino", number),("an_horario_vespertino", number),("an_cve_division", number)) )
