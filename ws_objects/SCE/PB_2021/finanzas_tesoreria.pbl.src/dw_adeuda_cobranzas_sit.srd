 column=(type=number updatewhereclause=yes name=saldos_importe dbname="saldos_importe" )
 retrieve="SELECT v_saldos_mov_alumnos.cuenta,   
       saldos_periodo = v_saldos_mov_alumnos.periodo,   
       saldos_anio = v_saldos_mov_alumnos.anio,   
       conceptos_descripcion = conceptos.concepto,   
       v_saldos_mov_alumnos.fecha_vencimiento,
       saldos_importe = v_saldos_mov_alumnos.saldo  
    FROM conceptos,   
         v_saldos_mov_alumnos		
   WHERE (v_saldos_mov_alumnos.cuenta =:cuenta) And
         (v_saldos_mov_alumnos.saldo > 0 ) And 
			(v_saldos_mov_alumnos.fecha_vencimiento < :Fechaven) And
         (v_saldos_mov_alumnos.cve_concepto = conceptos.cve_concepto) AND
			( conceptos.pago_servicio = 0 )
" retrieve.asneeded=yes arguments=(("Cuenta", number),("FechaVen", datetime)) )
