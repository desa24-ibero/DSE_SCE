 retrieve="  SELECT dbo.saldos.cuenta, 
			sum( dbo.saldos.importe)    
        FROM dbo.saldos      
        WHERE ( dbo.saldos.cuenta = :cuenta ) and          ( dbo.saldos.status = 0 ) and          ( dbo.saldos.fecha_ven < getdate() )  
        GROUP BY dbo.saldos.cuenta    
        HAVING ( sum( dbo.saldos.importe) > 0 )   " arguments=(("cuenta", number)) )
