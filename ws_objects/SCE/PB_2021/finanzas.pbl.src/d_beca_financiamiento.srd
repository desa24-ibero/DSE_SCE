table(column=(type=long updatewhereclause=yes name=apoyos_porcentaje dbname="apoyos.porcentaje" )
 retrieve="SELECT 	dbo.apoyos.porcentaje
		FROM 	dbo.becas_fin,
				dbo.apoyos
		WHERE ( dbo.becas_fin.cve_apoyo = dbo.apoyos.cve_apoyo ) AND
				( dbo.becas_fin.tipo_apoyo = dbo.apoyos.tipo_apoyo ) AND
				( ( dbo.becas_fin.cuenta = :cuenta ) AND
				( dbo.becas_fin.anio = :anio ) AND
				( dbo.becas_fin.periodo = :periodo ) AND
				(( ( dbo.becas_fin.cve_status_apoyo IN (0,1,2) ) AND
				( dbo.becas_fin.tipo_apoyo LIKE ~"B~" ) ) OR
				( ( dbo.becas_fin.cve_status_apoyo IN (0,1,21) ) AND
				( dbo.becas_fin.tipo_apoyo LIKE ~"F~" ) ) ) )" arguments=(("cuenta", number),("anio", number),("periodo", number)) )
