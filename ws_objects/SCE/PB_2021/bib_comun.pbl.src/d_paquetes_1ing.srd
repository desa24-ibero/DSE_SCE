 retrieve="  SELECT paquetes.num_paq,   
         paquetes.clv_carr,   
         paquetes.clv_plan,   
         paquetes.cupo,   
         paquetes.inscritos, 
  		paquetes.periodo, 
		paquetes.anio 
    FROM paquetes    
	WHERE paquetes.periodo = :a_periodo
AND paquetes.anio = :a_anio
AND (paquetes.clv_carr = :a_cve_carrera
   OR    :a_cve_carrera = 9999)


" update="paquetes" updatewhere=1 updatekeyinplace=yes arguments=(("a_cve_carrera", number),("a_periodo", number),("a_anio", number))  sort="clv_carr A num_paq A " )
