 retrieve="  SELECT area_mat.cve_mat  
    FROM area_mat,   
         plan_estudios  
   WHERE (( plan_estudios.cve_area_basica = area_mat.cve_area ) or  
         ( plan_estudios.cve_area_mayor_oblig = area_mat.cve_area )) and  
         ( ( area_mat.cve_mat = :cve_mat ) ) AND  
         plan_estudios.cve_carrera = :cve_carrera AND  
         plan_estudios.cve_plan = 4    
" arguments=(("cve_mat", number),("cve_carrera", number)) )
