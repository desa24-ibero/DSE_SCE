table(column=(type=long updatewhereclause=yes name=historico_cuenta dbname="historico.cuenta" dbalias=".cuenta" )
 column=(type=char(6) updatewhereclause=yes name=materias_sigla dbname="materias.sigla" dbalias=".sigla" )
 column=(type=char(42) updatewhereclause=yes name=materias_materia dbname="materias.materia" dbalias=".materia" )
 column=(type=long updatewhereclause=yes name=historico_anio dbname="historico.anio" dbalias=".anio" )
 column=(type=long updatewhereclause=yes name=historico_periodo dbname="historico.periodo" dbalias=".periodo" )
 column=(type=long updatewhereclause=yes name=materias_creditos dbname="materias.creditos" dbalias=".creditos" )
 column=(type=char(2) updatewhereclause=yes name=historico_calificacion dbname="historico.calificacion" dbalias=".calificacion" )
 column=(type=long updatewhereclause=yes name=historico_observacion dbname="historico.observacion" dbalias=".observacion" )
 column=(type=long updatewhereclause=yes name=materias_cve_mat dbname="materias.cve_mat" dbalias=".cve_mat" )
 column=(type=char(6) updatewhereclause=yes name=materias_sigla_anterior dbname="materias.sigla_anterior" dbalias=".sigla_anterior" )
 retrieve="  SELECT historico.cuenta,   
         materias.sigla,   
         materias.materia,   
         historico.anio,   
         historico.periodo,   
         materias.creditos,   
         historico.calificacion,   
         historico.observacion,   
         materias.cve_mat,   
         materias.sigla_anterior  
    FROM area_mat,   
         historico,   
         materias  
   WHERE ( area_mat.cve_mat = historico.cve_mat ) and  
         ( historico.cve_mat = materias.cve_mat ) and  
         ( ( historico.cuenta = :cuen ) AND  
         ( area_mat.cve_area = :area ) AND  
         ( materias.creditos > 0 ) AND  
         ( historico.calificacion in ('6','7','8','9','10','AC','MB','B','S','RE','E') ) AND  
         ( historico.cve_carrera = :carr ) AND  
         ( historico.cve_plan = :pla ) AND  
         ( historico.cve_mat not in (  SELECT area_mat.cve_mat  
                                         FROM area_mat  
                                        WHERE area_mat.cve_area = :area2  )
		AND  historico.cve_mat not in (  SELECT area_mat.cve_mat  
                                         FROM area_mat  
                                        WHERE area_mat.cve_area = :area3  )) AND  
         ( historico.cuenta not in ( select cuenta from historico_temp_intercambio 
               where historico_temp_intercambio.cuenta  = historico.cuenta 
               and	historico_temp_intercambio.cve_mat = historico.cve_mat 
               and	historico_temp_intercambio.gpo = historico.gpo 
               and	historico_temp_intercambio.periodo = historico.periodo 
               and	historico_temp_intercambio.anio    = historico.anio  ) ) )   
ORDER BY materias.creditos DESC   
" arguments=(("cuen", number),("area", number),("carr", number),("pla", number),("area2", number),("area3", number)) )
