 column=(type=long updatewhereclause=yes name=historico_cve_plan dbname="historico.cve_plan" )
 column=(type=char(2) updatewhereclause=yes name=historico_calificacion dbname="historico.calificacion" )
 column=(type=long updatewhereclause=yes name=historico_periodo dbname="historico.periodo" values="PRIMAVERA	0/VERANO	1/OTOÑO	2/" )
 column=(type=long updatewhereclause=yes name=historico_anio dbname="historico.anio" )
 retrieve="  SELECT historico.cuenta,   
         historico.cve_mat,   
			historico.cve_carrera,
			historico.cve_plan,
         historico.calificacion,   
         historico.periodo,   
         historico.anio  
    FROM banderas,               
         historico,
			academicos
   WHERE ( academicos.cuenta = historico.cuenta ) and 
			( academicos.cve_carrera = historico.cve_carrera ) and 
			( academicos.cve_plan = historico.cve_plan ) and 
			( academicos.cuenta = banderas.cuenta ) and  
			( academicos.cuenta = :al_cuenta)  and  
         ( ( banderas.cve_flag_promedio <> 3 ) )   
GROUP BY historico.cuenta,   
         historico.cve_mat,
			historico.cve_carrera,
			historico.cve_plan 
  HAVING ( count(*) >= 3 )   
" arguments=(("al_cuenta", number))  sort="historico_cuenta A historico_cve_mat A historico_anio A historico_periodo A " )
