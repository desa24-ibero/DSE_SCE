 retrieve="PBSELECT( VERSION(400) TABLE(NAME=~"area_mat~" )  TABLE(NAME=~"historico~" )  TABLE(NAME=~"materias~" ) COLUMN(NAME=~"historico.cuenta~") COLUMN(NAME=~"materias.sigla~") COLUMN(NAME=~"materias.materia~") COLUMN(NAME=~"historico.anio~") COLUMN(NAME=~"historico.periodo~") COLUMN(NAME=~"materias.creditos~") COLUMN(NAME=~"historico.calificacion~") COLUMN(NAME=~"historico.observacion~") COLUMN(NAME=~"materias.cve_mat~") COLUMN(NAME=~"materias.sigla_anterior~")    JOIN (LEFT=~"area_mat.cve_mat~"    OP =~"=~"RIGHT=~"historico.cve_mat~" )    JOIN (LEFT=~"historico.cve_mat~"    OP =~"=~"RIGHT=~"materias.cve_mat~" )WHERE(    EXP1 =~"historico.cuenta~"   OP =~"=~"    EXP2 =~":cuen~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"area_mat.cve_area~"   OP =~"=~"    EXP2 =~":area~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"materias.creditos~"   OP =~">~"    EXP2 =~"0~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"historico.calificacion~"   OP =~"in~"    EXP2 =~"('6','7','8','9','10','AC','MB','B','S','RE','E')~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"historico.cve_carrera~"   OP =~"=~"    EXP2 =~":carr~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"historico.cve_plan~"   OP =~"=~"    EXP2 =~":pla~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"historico.cve_mat~"   OP =~"not in~" NEST = PBSELECT( VERSION(400) TABLE(NAME=~"area_mat~" ) COLUMN(NAME=~"area_mat.cve_mat~")WHERE(    EXP1 =~"area_mat.cve_area~"   OP =~"=~"    EXP2 =~":area2~" ) )   LOGIC =~"and~" ) WHERE(    EXP1 =~"historico.cuenta~"   OP =~"not in~"    EXP2 =~"select cuenta from historico_temp_intercambio 
               where historico_temp_intercambio.cuenta  = historico.cuenta 
               and	historico_temp_intercambio.cve_mat = historico.cve_mat 
               and	historico_temp_intercambio.gpo = historico.gpo 
               and	historico_temp_intercambio.periodo = historico.periodo 
               and	historico_temp_intercambio.anio    = historico.anio ~" ) ) ORDER(NAME=~"materias.creditos~" ASC=no) ARG(NAME = ~"cuen~" TYPE = number)  ARG(NAME = ~"area~" TYPE = number)  ARG(NAME = ~"carr~" TYPE = number)  ARG(NAME = ~"pla~" TYPE = number)  ARG(NAME = ~"area2~" TYPE = number) " arguments=(("cuen", number),("area", number),("carr", number),("pla", number),("area2", number)) )
