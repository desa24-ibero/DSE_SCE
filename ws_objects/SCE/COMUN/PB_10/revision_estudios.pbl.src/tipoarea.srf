global type tipoarea from function_object
end type

forward prototypes
global function integer tipoarea (integer area, integer cve_plan)
end prototypes

global function integer tipoarea (integer area, integer cve_plan);CHOOSE CASE area
	CASE 1 to 2
		return 1
	CASE 3
		if cve_plan = 2 then
			return 2
		else
			return 1
		end if
	CASE 4
		return 1
	CASE 5
		return 2
	CASE 6 to 10
		return 3
	CASE 11 to 12
		return 2
END CHOOSE
end function

