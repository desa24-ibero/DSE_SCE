 column=(type=long updatewhereclause=yes name=materias_cve_coordinacion dbname="materias.cve_coordinacion" )
 retrieve="  SELECT dbo.grupos.periodo,   
         dbo.grupos.anio,   
         dbo.v_paquetes_materias.num_paq,   
         dbo.v_paquetes_materias.clv_mat,   
         dbo.materias.materia,   
         dbo.materias.sigla,   
         dbo.v_paquetes_materias.grupo,   
         dbo.horario.cve_dia,   
         dbo.dias.dia,   
         dbo.horario.cve_salon,   
         dbo.horario.hora_inicio,   
         dbo.horario.hora_final,
         dbo.materias.cve_coordinacion
    FROM dbo.grupos,   
         dbo.horario,   
         dbo.materias,   
         dbo.v_paquetes,   
         dbo.v_paquetes_materias,   
         dbo.dias  
   WHERE ( dbo.grupos.cve_mat = dbo.horario.cve_mat ) and  
         ( dbo.horario.cve_mat = dbo.materias.cve_mat ) and  
         ( dbo.v_paquetes.num_paq = dbo.v_paquetes_materias.num_paq ) and  
         ( dbo.horario.cve_dia = dbo.dias.cve_dia ) and  
         ( ( dbo.v_paquetes_materias.clv_per = :a_periodo ) AND  
         ( dbo.v_paquetes_materias.anio = :a_anio ) AND  
         ( dbo.v_paquetes_materias.num_paq = dbo.v_paquetes.num_paq ) AND  
         ( dbo.v_paquetes_materias.clv_mat = dbo.materias.cve_mat ) AND  
         ( dbo.v_paquetes_materias.grupo = dbo.grupos.gpo ) AND  
         ( dbo.v_paquetes_materias.clv_mat = dbo.grupos.cve_mat ) AND  
         ( dbo.v_paquetes_materias.clv_per = dbo.grupos.periodo ) AND  
         ( dbo.v_paquetes_materias.anio = dbo.grupos.anio ) AND  
         ((dbo.grupos.tipo <>  2 AND  
         dbo.horario.cve_mat = dbo.grupos.cve_mat AND  
         dbo.horario.gpo = dbo.grupos.gpo) OR  
         (dbo.grupos.tipo = 2 AND  
         dbo.horario.cve_mat = dbo.grupos.cve_asimilada AND  
         dbo.horario.gpo = dbo.grupos.gpo_asimilado)) AND  
         (dbo.v_paquetes_materias.clv_mat = :a_materia OR  
         :a_materia = 9999) AND  
         (dbo.v_paquetes_materias.grupo = :a_gpo OR  
         :a_gpo = ~"Z~") )    
" arguments=(("a_periodo", number),("a_anio", number),("a_materia", number),("a_gpo", string)) )
