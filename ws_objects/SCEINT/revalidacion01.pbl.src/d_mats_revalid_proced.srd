 retrieve="  SELECT materias_revalidacion.folio,   
         materias_revalidacion.nombre_materia,   
         materias_revalidacion.calificacion,   
         materias_revalidacion.cve_coordinacion,   
         materias_revalidacion.periodo,   
         materias_revalidacion.anio,   
         materias_revalidacion.cve_mat,   
         materias_revalidacion.gpo,   
         materias_revalidacion.procede,   
         materias_revalidacion.pase_realizado,  
         materias_revalidacion.status 
    FROM materias_revalidacion  
   WHERE ( materias_revalidacion.procede = 1 ) AND  
         ( materias_revalidacion.folio = :al_folio ) AND
			( isnull(materias_revalidacion.status,0) = 0)   

" update="materias_revalidacion" updatewhere=1 updatekeyinplace=yes arguments=(("al_folio", number)) )
