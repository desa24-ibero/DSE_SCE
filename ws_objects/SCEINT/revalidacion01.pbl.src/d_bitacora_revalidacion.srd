 column=(type=char(2) updatewhereclause=yes name=movimiento dbname="bit_revalidacion.movimiento" dbalias=".movimiento" )
 column=(type=char(30) updatewhereclause=yes name=columna dbname="bit_revalidacion.columna" dbalias=".columna" )
 column=(type=char(255) updatewhereclause=yes name=valor dbname="bit_revalidacion.valor" dbalias=".valor" )
 column=(type=char(30) updatewhereclause=yes name=usuario dbname="bit_revalidacion.usuario" dbalias=".usuario" )
 column=(type=datetime updatewhereclause=yes name=fecha dbname="bit_revalidacion.fecha" dbalias=".fecha" )
 retrieve="  SELECT bit_revalidacion.folio,   
         bit_revalidacion.movimiento,   
         bit_revalidacion.columna,   
         bit_revalidacion.valor,   
         bit_revalidacion.usuario,   
         bit_revalidacion.fecha  
    FROM bit_revalidacion, aspirantes_revalidacion  
   WHERE ( bit_revalidacion.fecha > :fecha_inicial ) AND  
         ( bit_revalidacion.fecha < :fecha_final ) AND  
         ( bit_revalidacion.movimiento in ('IN','DE') )    
AND bit_revalidacion.folio = aspirantes_revalidacion.folio 
AND aspirantes_revalidacion.tipo_periodo = :tipo_periodo 


" arguments=(("fecha_inicial", datetime),("fecha_final", datetime),("tipo_periodo", string))  sort="folio A fecha A " )
