 column=(type=char(2) updatewhereclause=yes name=historico_calificacion dbname="historico.calificacion" )
 retrieve="  SELECT dbo.academicos.cuenta,   
         dbo.academicos.cve_carrera,   
         dbo.academicos.cve_plan, 
			dbo.area_mat.cve_mat,  
         dbo.aux_opcion_terminal.cve_area,   
         dbo.aux_opcion_terminal.cve_proyecto_opc_term,   
         dbo.aux_opcion_terminal.cve_seminario_tit,   
         dbo.historico.cve_mat,   
         dbo.historico.calificacion  
    FROM dbo.academicos,   
         dbo.aux_opcion_terminal,   
         dbo.historico,   
         dbo.plan_estudios,
			dbo.area_mat 
   WHERE ( dbo.academicos.cuenta = dbo.historico.cuenta ) and  
         ( dbo.plan_estudios.cve_area_opcion_terminal = dbo.aux_opcion_terminal.cve_area ) and  
         ( dbo.academicos.cve_carrera = dbo.plan_estudios.cve_carrera ) and  
         ( dbo.academicos.cve_plan = dbo.plan_estudios.cve_plan ) and  
         ( dbo.academicos.cve_carrera = dbo.historico.cve_carrera ) and  
         ( dbo.academicos.cve_plan = dbo.historico.cve_plan ) and  
         ( dbo.aux_opcion_terminal.cve_seminario_tit = dbo.historico.cve_mat ) and
			( dbo.area_mat.cve_area = dbo.plan_estudios.cve_area_opcion_terminal ) and
         ( ( dbo.historico.anio = :anio ) AND  
         ( dbo.historico.periodo = :periodo ) AND  
         ( dbo.historico.calificacion like ~"AC~" ) )    
" arguments=(("anio", number),("periodo", number))  sort="academicos_cuenta A " )
