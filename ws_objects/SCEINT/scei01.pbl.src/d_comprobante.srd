 column=(type=char(12) updatewhereclause=yes name=tipo_grupo_nombre_tipo dbname="tipo_grupo.nombre_tipo" )
 column=(type=char(12) updatewhereclause=yes name=tipo_grupo_nombre_tipo dbname="tipo_grupo.nombre_tipo" )
 retrieve="  SELECT dbo.mat_inscritas.periodo,   
         dbo.mat_inscritas.anio,   
         dbo.alumnos.cuenta,   
         dbo.alumnos.nombre_completo,   
         dbo.carreras.carrera,   
         dbo.nombre_plan.nombre_plan,   
         dbo.mat_inscritas.cve_mat,   
         dbo.mat_inscritas.gpo,   
         dbo.materias.materia,   
         dbo.materias.creditos,   
         dbo.materias.horas_teoria, 
			dbo.materias.horas_practica,  
         dbo.horario.cve_salon,
			dbo.horario.cve_dia,
			CASE dbo.horario.cve_dia
				WHEN 0 THEN ~"DOMINGO~"
				WHEN 1 THEN ~"LUNES~"
				WHEN 2 THEN ~"MARTES~"
				WHEN 3 THEN ~"MIERCOLES~"
				WHEN 4 THEN ~"JUEVES~"
				WHEN 5 THEN ~"VIERNES~"
				WHEN 6 THEN ~"SABADO~"
			END,
			dbo.horario.hora_inicio,
			dbo.horario.hora_final,
			dbo.tipo_grupo.nombre_tipo, 
			dbo.tipo_grupo.nombre_tipo  
    FROM dbo.academicos,   
         dbo.alumnos,   
         dbo.mat_inscritas,   
         dbo.carreras,   
         dbo.nombre_plan,
			dbo.horario,   
         dbo.materias,
			dbo.grupos,
			dbo.tipo_grupo
   WHERE ( dbo.alumnos.cuenta = dbo.academicos.cuenta ) and  
			( dbo.alumnos.cuenta = dbo.mat_inscritas.cuenta ) and  
         ( dbo.academicos.cve_carrera = dbo.carreras.cve_carrera ) and  
         ( dbo.academicos.cve_plan = dbo.nombre_plan.cve_plan ) and
			( dbo.mat_inscritas.cve_mat = dbo.materias.cve_mat ) and 
			( dbo.grupos.tipo = dbo.tipo_grupo.tipo ) and
			( dbo.grupos.cve_mat = dbo.mat_inscritas.cve_mat ) and
			( dbo.grupos.gpo = dbo.mat_inscritas.gpo ) and
			( dbo.grupos.anio = dbo.mat_inscritas.anio ) and 
			( dbo.grupos.periodo = dbo.mat_inscritas.periodo ) and
			( ( ( dbo.grupos.cve_mat *= dbo.horario.cve_mat) and  
         ( dbo.grupos.gpo *= dbo.horario.gpo) ) or
			( ( dbo.grupos.cve_asimilada *= dbo.horario.cve_mat) and  
         ( dbo.grupos.gpo_asimilado *= dbo.horario.gpo) ) ) and
         ( dbo.horario.anio = :anio ) and
			( dbo.horario.periodo = :periodo ) and
			( ( dbo.alumnos.cuenta = :cuenta ) and 
			( dbo.mat_inscritas.anio= :anio ) and
			( dbo.mat_inscritas.periodo = :periodo ) )
           
" arguments=(("cuenta", number),("anio", number),("periodo", number))  sort="mat_inscritas_cve_mat A horario_cve_dia A " )
