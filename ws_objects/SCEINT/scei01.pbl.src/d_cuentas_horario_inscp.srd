 retrieve="  SELECT horario_insc.cuenta
    FROM horario_insc,   
         alumnos,
			academicos
   WHERE ( horario_insc.cuenta = alumnos.cuenta ) and  
			( academicos.cuenta = alumnos.cuenta ) and
			( academicos.cve_carrera BETWEEN :carreraini AND :carrerafin ) and
         ( ( alumnos.nombre_completo between :NombreIni And :NombreFin ) ) and
         ( academicos.nivel = 'P' )
	ORDER BY academicos.cve_carrera,
				alumnos.nombre_completo   
" arguments=(("NombreIni", string),("NombreFin", string),("carreraini", number),("carrerafin", number)) )
