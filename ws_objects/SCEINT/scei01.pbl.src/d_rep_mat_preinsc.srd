 retrieve="  SELECT  coordinaciones.cve_coordinacion ,
           coordinaciones.coordinacion ,
           mat_preinsc.cve_mat ,
           materias.materia ,
           mat_preinsc.gpo ,
           count(mat_preinsc.cuenta),
			  grupos.cupo    
        FROM mat_preinsc ,
           materias ,
           coordinaciones, 
			  grupos     
        WHERE ( mat_preinsc.cve_mat = materias.cve_mat ) and
          ( materias.cve_coordinacion = coordinaciones.cve_coordinacion ) and
          ( ( :status = 99 ) or          ( mat_preinsc.status = :status ) and
          ( mat_preinsc.periodo = :periodo ) and
          ( mat_preinsc.anio = :anio ) ) and
			 ( mat_preinsc.cve_mat = grupos.cve_mat) and
			 ( mat_preinsc.gpo = grupos.gpo) and
			 ( mat_preinsc.periodo = grupos.periodo) and
			 ( mat_preinsc.anio = grupos.anio) 
        GROUP BY coordinaciones.cve_coordinacion ,
           coordinaciones.coordinacion ,
           mat_preinsc.cve_mat ,
           materias.materia ,
           mat_preinsc.gpo,  
			  grupos.cupo    
        ORDER BY coordinaciones.cve_coordinacion          ASC,
           mat_preinsc.cve_mat          ASC,
           mat_preinsc.gpo          ASC  " arguments=(("status", number),("periodo", number),("anio", number)) )
