 column=(type=char(1) updatewhereclause=yes name=saldos_periodo dbname="saldos.periodo" values="PRIMAVERA	P/VERANO	V/OTONO	O/" )
 column=(type=long updatewhereclause=yes name=saldos_anio dbname="saldos.anio" )
 column=(type=char(30) updatewhereclause=yes name=conceptos_descripcion dbname="conceptos.descripcion" )
 column=(type=datetime updatewhereclause=yes name=saldos_fecha_ven dbname="saldos.fecha_ven" )
 column=(type=decimal(4) updatewhereclause=yes name=saldos_importe dbname="saldos.importe" )
 column=(type=long updatewhereclause=yes name=saldos_status dbname="saldos.status" )
 retrieve="  SELECT dbo.saldos.cuenta,   
         dbo.saldos.periodo,   
         dbo.saldos.anio,   
         dbo.conceptos.descripcion,   
         dbo.saldos.fecha_ven,
    		dbo.saldos.importe,
         dbo.saldos.status  
    FROM dbo.conceptos,   
         dbo.saldos  
   WHERE (dbo.saldos.cuenta = :Cuenta) And
         (dbo.saldos.status = 0 ) And /* 0 = Adeuda*/  
			(dbo.saldos.fecha_ven < :FechaVen) And
         (dbo.saldos.cve_con = dbo.conceptos.cve_con)    
" retrieve.asneeded=yes arguments=(("Cuenta", number),("FechaVen", datetime)) )
