 retrieve="  SELECT  escuelas.cve_escuela ,
           escuelas.escuela ,
           escuelas.cve_sep ,
           escuelas.cene_1 ,
           escuelas.cene_2 ,
           escuelas.cene_3 ,
           escuelas.cve_mun_loc ,
			  escuelas.ceneval ,
           estados.estado ,
           municipio_localidad.municipio ,
           municipio_localidad.localidad     
        FROM escuelas ,
           estados ,
           municipio_localidad     
        WHERE ( escuelas.cve_sep *= estados.cve_sep) and
          ( escuelas.cve_mun_loc *= municipio_localidad.cve_mun_loc)
   " update="escuelas" updatewhere=1 updatekeyinplace=no )
