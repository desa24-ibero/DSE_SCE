table(column=(type=long update=yes updatewhereclause=yes name=cuenta dbname="horario_insc.cuenta" )
 retrieve="  SELECT horario_insc.cuenta 
    FROM horario_insc,   
         alumnos,
			academicos, 
			preinsc 
   WHERE ( horario_insc.cuenta = alumnos.cuenta ) and 
			( horario_insc.cuenta = preinsc.cuenta ) and 
			( alumnos.cuenta = academicos.cuenta ) and
			( academicos.nivel LIKE ~"P~" ) and 
			( academicos.cve_carrera BETWEEN :carreraini AND :carrerafin ) and
         ( ( alumnos.nombre_completo between :NombreIni And :NombreFin ) ) 
	ORDER BY academicos.cve_carrera,
				alumnos.nombre_completo   
" arguments=(("NombreIni", string),("NombreFin", string),("carreraini", number),("carrerafin", number)) )
