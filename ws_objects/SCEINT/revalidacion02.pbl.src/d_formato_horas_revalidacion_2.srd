 retrieve="  SELECT DISTINCT materias_revalidacion.folio,   
         materias.cve_mat,   
         materias.sigla,   
         materias.materia,   
         materias.horas_reales,   
         aspirantes_revalidacion.apaterno,   
         aspirantes_revalidacion.amaterno,   
         aspirantes_revalidacion.nombre,   
         carreras.carrera,   
         aspirantes_revalidacion.cuenta,  
         aspirantes_revalidacion.anio_ing,
         aspirantes_revalidacion.periodo_ing,  
         materias.sigla_anterior,
         academicos.cve_plan
    FROM materias_revalidacion,   
         materias,   
         aspirantes_revalidacion,   
         carreras,
         academicos  
   WHERE ( aspirantes_revalidacion.cve_carrera *= carreras.cve_carrera) and  
         ( materias.cve_mat = materias_revalidacion.cve_mat ) and  
         ( materias_revalidacion.folio = aspirantes_revalidacion.folio ) and  
         ( ( materias_revalidacion.folio = :al_folio ) )  and  
			( materias_revalidacion.aceptada_sep = 1) and
			( materias_revalidacion.procede = 1) and
         ( academicos.cuenta =  aspirantes_revalidacion.cuenta)" arguments=(("al_folio", number),("as_prefijo_destinatario", string),("as_saludo_destinatario", string),("as_destinatario", string),("as_responsable_dse", string),("as_puesto_responsable_dse", string)) )
