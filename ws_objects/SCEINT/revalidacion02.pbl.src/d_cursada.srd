 retrieve="  SELECT dbo.historico.cve_mat,   
         dbo.historico.calificacion  
    FROM dbo.historico 
   WHERE ( dbo.historico.cuenta = :cuenta ) AND  
         ( dbo.historico.cve_mat = :cve_mat ) AND  
         ( dbo.historico.cve_carrera = :cve_carrera ) AND  
         ( dbo.historico.cve_plan = :cve_plan )    

" arguments=(("cuenta", number),("cve_mat", number),("cve_carrera", number),("cve_plan", number)) )
