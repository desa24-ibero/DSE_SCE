 retrieve="SELECT documentos.cuenta,
       documentos.cve_documento,
       documentos.cve_flag_documento,
		 flag_documentos.desc_flag_documento,
		 tipo_documento.documento
	FROM documentos, flag_documentos, tipo_documento
	WHERE documentos.cuenta = :al_cuenta
	AND 	documentos.cve_documento in (:ai_documentos )
	AND	documentos.cve_documento = tipo_documento.cve_documento
	AND	documentos.cve_flag_documento = flag_documentos.cve_flag_documento" arguments=(("al_cuenta", number),("ai_documentos", numberlist)) )
