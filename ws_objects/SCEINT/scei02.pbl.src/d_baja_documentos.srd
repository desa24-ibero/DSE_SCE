table(column=(type=long updatewhereclause=yes name=academicos_cuenta dbname="academicos.cuenta" dbalias=".cuenta" )
 column=(type=long updatewhereclause=yes name=academicos_cve_carrera dbname="academicos.cve_carrera" dbalias=".cve_carrera" )
 column=(type=long updatewhereclause=yes name=academicos_cve_plan dbname="academicos.cve_plan" dbalias=".cve_plan" )
 column=(type=char(50) updatewhereclause=yes name=alumnos_apaterno dbname="alumnos.apaterno" dbalias=".apaterno" )
 column=(type=char(50) updatewhereclause=yes name=alumnos_amaterno dbname="alumnos.amaterno" dbalias=".amaterno" )
 column=(type=char(50) updatewhereclause=yes name=alumnos_nombre dbname="alumnos.nombre" dbalias=".nombre" )
 column=(type=long updatewhereclause=yes name=academicos_sem_cursados dbname="academicos.sem_cursados" dbalias=".sem_cursados" )
 column=(type=long updatewhereclause=yes name=banderas_baja_documentos dbname="banderas.baja_documentos" dbalias=".baja_documentos" )
 column=(type=long updatewhereclause=yes name=banderas_exten_cred dbname="banderas.exten_cred" dbalias=".exten_cred" )
 column=(type=long updatewhereclause=yes name=inscrito dbname="inscrito" values="SI	1/NO	0/" )
 column=(type=char(100) updatewhereclause=yes name=carreras_carrera dbname="carreras.carrera" dbalias=".carrera" )
 column=(type=char(20) updatewhereclause=yes name=nombre_plan_nombre_plan dbname="nombre_plan.nombre_plan" dbalias=".nombre_plan" )
 column=(type=char(1) updatewhereclause=yes name=academicos_nivel dbname="academicos.nivel" dbalias=".nivel" )
 retrieve="SELECT academicos.cuenta,   
			academicos.cve_carrera,
			 academicos.cve_plan,
			 alumnos.apaterno,
			 alumnos.amaterno,
			 alumnos.nombre,
          academicos.sem_cursados,   
          banderas.baja_documentos,
          banderas.exten_cred,
			inscrito = (SELECT CASE cuenta WHEN null THEN 0 ELSE 1 END 
							FROM banderas_inscrito where cuenta = academicos.cuenta),
			carreras.carrera,
			nombre_plan.nombre_plan,
			academicos.nivel
    FROM  academicos,
			 alumnos,   
          banderas,
			 carreras,
			nombre_plan
   WHERE (  academicos.cuenta =  banderas.cuenta ) and  
		   (  academicos.cuenta =  alumnos.cuenta ) and  
         (  academicos.cuenta not in (Select cuenta from historico where cve_mat IN(SELECT cve_mat 
																											FROM materias_requisito 
																											WHERE id_prerrequisito = 'ING') and calificacion='AC') ) AND  
			( academicos.cve_carrera = carreras.cve_carrera) AND
			( academicos.cve_plan = nombre_plan.cve_plan) AND
			( banderas.baja_documentos = 1 ) AND
		  (( ~"P~" <> :as_nivel AND academicos.nivel = :as_nivel) OR
			( ~"P~" = :as_nivel AND academicos.nivel = :as_nivel AND
			  carreras.carrera LIKE :as_grado OR
           carreras.carrera LIKE :as_grado2 )
		  )

/*Linea modificada (( ~"L~" = :as_nivel AND academicos.nivel = :as_nivel) OR*/

" arguments=(("as_nivel", string),("as_grado", string),("as_grado2", string)) )
