 retrieve="  SELECT solicitud_certificado.cuenta,   
         v_sce_alumno_digito.digito,   
         alumnos.nombre_completo,   
         solicitud_certificado.cve_carrera,   
         carreras.carrera,   
         solicitud_certificado.cve_plan,   
         nombre_plan.nombre_plan,   
         doc_control_sep.doc_control_sep,   
         doc_control_sep.cve_doc_control_sep,   
         solicitud_certificado.fecha_solicitud,   
         carreras.nivel,   
         solicitud_certificado.legalizado  
    FROM alumnos,   
         carreras,   
         solicitud_certificado,   
         v_sce_alumno_digito,   
         nombre_plan,   
         doc_control_sep, e_documento_generado dg   
   WHERE ( carreras.cve_carrera = solicitud_certificado.cve_carrera ) and  
         ( solicitud_certificado.cuenta = v_sce_alumno_digito.cuenta ) and  
         ( alumnos.cuenta = solicitud_certificado.cuenta ) and  
         ( solicitud_certificado.cve_plan = nombre_plan.cve_plan ) and  
         ( solicitud_certificado.cve_doc_control_sep = doc_control_sep.cve_doc_control_sep ) and  
         ( ( dg.fecha_registro >= :adttm_fecha_inicial ) AND 
		  ( dg.fecha_registro <= :adttm_fecha_final ) AND  
         (solicitud_certificado.cve_doc_control_sep = :an_cve_doc_control_sep OR :an_cve_doc_control_sep = 9999) AND  
		(solicitud_certificado.legalizado = :an_legalizado OR  :an_legalizado = 9999) AND
         (carreras.nivel IN(:as_nivel)) AND  
         (isnull(solicitud_certificado.cancelado,0) = 0) )     
		AND ( solicitud_certificado.cuenta = :al_cuenta OR :al_cuenta = 9999) 
		AND ( solicitud_certificado.cuenta = dg.cuenta )


