table(column=(type=long updatewhereclause=yes name=cuenta dbname="titulacion.cuenta" dbalias="t.cuenta" )
 column=(type=char(30) updatewhereclause=yes name=digito dbname="v_sce_alumno_digito.digito" dbalias="d.digito" )
 column=(type=char(152) updatewhereclause=yes name=nombre dbname="nombre" )
 column=(type=long updatewhereclause=yes name=cve_carrera dbname="carreras.cve_carrera" dbalias="c.cve_carrera" )
