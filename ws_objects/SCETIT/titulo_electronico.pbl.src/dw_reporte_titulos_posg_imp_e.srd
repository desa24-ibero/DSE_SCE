 column=(type=char(30) updatewhereclause=yes name=numero_titulo dbname="diseño_titulacion.numero_titulo" dbalias="dt.numero_titulo" )
 column=(type=long updatewhereclause=yes name=folio dbname="hoja_libro_posgrado.folio" dbalias="h.folio" )
 column=(type=long updatewhereclause=yes name=libro dbname="hoja_libro_posgrado.libro" dbalias="h.libro" )
 column=(type=datetime updatewhereclause=yes name=fecha_envio dbname="diseño_titulacion.fecha_envio" dbalias="dt.fecha_envio" )
 column=(type=char(1) updatewhereclause=yes name=rec_mencion dbname="rec_mencion" )
 column=(type=datetime update=yes updatewhereclause=yes name=fecha_expedicion dbname="diseño_titulacion.fecha_expedicion" dbalias="dt.fecha_expedicion" )
 column=(type=long updatewhereclause=yes name=acta_tit dbname="titulacion.acta_tit" dbalias="t.acta_tit" )
 column=(type=long updatewhereclause=yes name=libro_tit dbname="titulacion.libro_tit" dbalias="t.libro_tit" )
 column=(type=long updatewhereclause=yes name=hoja_tit dbname="titulacion.hoja_tit" dbalias="t.hoja_tit" )
 column=(type=char(70) updatewhereclause=yes name=nom_rector dbname="datos_certificado.nom_rector" dbalias="dc.nom_rector" )
 column=(type=char(70) updatewhereclause=yes name=puesto_rector dbname="datos_certificado.puesto_rector" dbalias="dc.puesto_rector" )
 column=(type=char(25) updatewhereclause=yes name=plantel dbname="planteles.plantel" dbalias="p.plantel" )
 column=(type=char(8) updatewhereclause=yes name=fecha_emision dbname="fecha_emision" )
 column=(type=long updatewhereclause=yes name=cve_plan dbname="diseño_titulacion.cve_plan" dbalias="dt.cve_plan" )
 column=(type=long updatewhereclause=yes name=num_holograma dbname="control_sep.num_holograma" dbalias="s.num_holograma" )
 column=(type=char(4) updatewhereclause=yes name=folio_acta dbname="folio_acta" )
 column=(type=char(152) updatewhereclause=yes name=nombre_2 dbname="nombre_2" )
 retrieve="SELECT DISTINCT t.cuenta, d. digito, a.nombre +''+ a.apaterno +''+ a.amaterno as nombre, c.cve_carrera, c.carrera_sin_prefijo, 
		    t.fecha_examen, dt.numero_titulo, h.folio, h. libro, dt.fecha_envio, 
		   Case When t.mencion = 1 and t.reconocimiento = 2 then 'M' When t.reconocimiento = 1 and t.mencion = 2 then 'R' else '' end as rec_mencion, 
		   dt.fecha_expedicion, t.acta as acta_tit, t.libro as libro_tit,t.hoja as hoja_tit, dc.nombre as nom_rector, dc.puesto as puesto_rector, p.plantel, CONVERT(varchar(08), dt.fecha_expedicion,112) as fecha_emision, dt.cve_plan,
		   s.num_holograma, Case when s.folio_acta > 0 then Right('0000' + Convert(varchar(04), s.folio_acta), 4) else ' ' end as folio_acta, 
		   a.apaterno +''+ a.amaterno + '' + a.nombre as nombre_2
	FROM titulacion t LEFT OUTER JOIN hoja_libro_posgrado h ON t.cuenta = h.cuenta AND t.cve_plan = h.cve_plan AND t.cve_carrera = h.cve_carrera
					  	   LEFT OUTER JOIN control_sep s ON t.cuenta = s.cuenta 
									AND t.cve_plan = s.cve_plan 
									AND t.cve_carrera = s.cve_carrera  
									AND s.cve_doc_control_sep = CASE WHEN(SELECT car.cve_grado 
																					  FROM carreras car 
																					  WHERE car.cve_carrera = t.cve_carrera)    
												 							in ('M', 'D') then 6 
												 							WHEN(SELECT car2.cve_grado 
																					  FROM carreras car2 
																					  WHERE car2.cve_carrera = t.cve_carrera) in ('E') then 5  
												 							ELSE 0 END ,
		 diseño_titulacion dt, carreras c, alumnos a, v_sce_alumno_digito d, datos_certificado dc, planteles p, plan_estudios pe
		, e_documento_generado dg 
   WHERE t.cuenta = dt.cuenta
	 AND t.cve_plan = dt.cve_plan
	 AND t.cve_carrera = dt.cve_carrera
	 AND t.cve_carrera = c. cve_carrera 
	 AND a.cuenta = t.cuenta
	 AND a.cuenta = d.cuenta
	 AND dg.fecha_registro >= :ar_fecha_examen1
	 AND dg.fecha_registro <= :ar_fecha_examen2
	 AND c.cve_grado IN(:ar_grado) 
	 AND t.aprobado = 1
	 AND dc.id  = 6
	 AND p.actual = 1
	 AND t.opcion_titulacion <> 5
	 AND t.cve_plan = pe.cve_plan
	 AND t.cve_carrera = pe.cve_carrera
	 AND pe.incorporado_sep = 1
	AND (t.cuenta = :al_cuenta OR :al_cuenta = 9999)
	AND t.cuenta = dg.cuenta  

/*	AND s.cve_doc_control_sep = case when :ar_grado in ('M', 'D') then 6 when :ar_grado = 'E' then 5 else 0 end,
 AND convert(varchar(08), t.fecha_examen, 112) >= :ar_fecha_examen1
	 AND convert(varchar(08), t.fecha_examen, 112) <= :ar_fecha_examen2*/" arguments=(("ar_fecha_examen1", datetime),("ar_fecha_examen2", datetime),("ar_grado", stringlist),("ar_cve_relacion", number),("al_cuenta", number))  sort="cve_carrera A nombre_2 A " )
