 retrieve="SELECT materias_certificado.cve_mat, historico.cuenta,   
		historico.calificacion,   
		historico.observacion,   
		historico.cve_carrera,   
		historico.cve_plan, 
	 materias_certificado.creditos as creditos	
 FROM historico, materias_certificado    
 WHERE historico.cuenta = materias_certificado.cuenta 
 AND historico.cve_mat = materias_certificado.cve_mat
 AND historico.periodo = materias_certificado.periodo 
 AND historico.anio = materias_certificado.anio 



" )
