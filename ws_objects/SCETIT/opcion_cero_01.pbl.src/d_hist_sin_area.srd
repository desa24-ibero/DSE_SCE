 column=(type=char(42) updatewhereclause=yes name=materias_materia dbname="materias.materia" )
 column=(type=long updatewhereclause=yes name=historico_anio dbname="historico.anio" )
 column=(type=long updatewhereclause=yes name=historico_periodo dbname="historico.periodo" )
 column=(type=long updatewhereclause=yes name=materias_creditos dbname="materias.creditos" )
 column=(type=char(2) updatewhereclause=yes name=historico_calificacion dbname="historico.calificacion" )
 column=(type=long updatewhereclause=yes name=historico_observacion dbname="historico.observacion" )
 column=(type=long updatewhereclause=yes name=materias_cve_mat dbname="materias.cve_mat" )
 retrieve="  SELECT historico.cuenta,   
         materias.sigla,   
         materias.materia,   
         historico.anio,   
         historico.periodo,   
         materias.creditos,   
         historico.calificacion,   
         historico.observacion,   
         materias.cve_mat  
    FROM historico,   
         materias  
   WHERE ( historico.cve_mat = materias.cve_mat ) and  
         ( ( historico.cuenta = :cuen ) AND  
         ( materias.creditos > 0 ) AND  
         (( historico.calificacion in ('6','7','8','9','10','AC','MB','B','S','RE','E') )OR
				(historico.calificacion LIKE ~"IN~" AND historico.cve_mat IN(3748,8763))) AND  
         ( historico.cve_carrera = :carr ) AND  
         ( historico.cve_plan = :pla ) )    
" arguments=(("cuen", number),("carr", number),("pla", number)) )
