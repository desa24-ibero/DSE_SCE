table(column=(type=long updatewhereclause=yes name=areas_cve_area dbname="areas.cve_area" )
 column=(type=long updatewhereclause=yes name=areas_total_materias dbname="areas.total_materias" )
 column=(type=long updatewhereclause=yes name=materias_creditos dbname="creditos" )
 column=(type=long updatewhereclause=yes name=areas_creditos_min dbname="areas.creditos_min" )
 retrieve="  SELECT  areas.cve_area ,           areas.total_materias ,          sum( materias.creditos) as creditos  ,           areas.creditos_min     
        FROM area_mat ,           areas ,           materias     
        WHERE ( area_mat.cve_area = areas.cve_area ) and          ( area_mat.cve_mat = materias.cve_mat ) and       
   ( ( area_mat.cve_area = :BASICA ) or          ( area_mat.cve_area = :OBLIG ) or          ( area_mat.cve_area = :OPT ) or        
  ( area_mat.cve_area = :SERV_SOC ) or          ( area_mat.cve_area = :TERMINAL ) or          ( area_mat.cve_area = :SISOBLIG ) or          ( area_mat.cve_area = :SISOPT ) )  
group by  areas.cve_area ,           areas.total_materias ,  areas.creditos_min  " arguments=(("BASICA", number),("OBLIG", number),("OPT", number),("SERV_SOC", number),("TERMINAL", number),("SISOBLIG", number),("SISOPT", number))  sort="areas_cve_area A " )
