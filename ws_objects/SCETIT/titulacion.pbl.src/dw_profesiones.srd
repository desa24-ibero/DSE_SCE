 column=(type=long update=yes updatewhereclause=yes name=sin_tramite dbname="profesiones.sin_tramite" values="NORMAL	0/SIN TRAMITE	1/" )
 retrieve="  SELECT  profesiones.cuenta ,
           profesiones.cve_plan ,
           profesiones.cve_carrera ,
           profesiones.fecha_envio_prof ,
           profesiones.fecha_regreso_prof ,
           profesiones.num_relacion ,
           profesiones.observaciones,     
           profesiones.sin_tramite
        FROM profesiones      
        WHERE ( :cuenta = 0 ) or
          (( profesiones.cuenta = :cuenta ) and
          ( profesiones.cve_carrera = :cve_carrera ) and
          ( profesiones.cve_plan = :cve_plan ) )   " update="profesiones" updatewhere=1 updatekeyinplace=yes arguments=(("cuenta", number),("cve_carrera", number),("cve_plan", number))  sort="cuenta A " )
