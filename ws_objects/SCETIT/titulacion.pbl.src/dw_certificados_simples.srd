 retrieve="  SELECT dbo.certificado.parcial,   
         dbo.certificado.legalizado,   
         dbo.certificado.num_reg,   
         dbo.certificado.fecha_salida_sep,   
         dbo.certificado.checado_sep,   
         dbo.certificado.entrada_arch,   
         dbo.certificado.num_veces_sep,   
         dbo.certificado.fecha_entrada_sep,   
         dbo.certificado.fecha_inicio,   
         dbo.certificado.fecha_checado_sep,   
         dbo.certificado.fecha_salida_tentat_sep,   
         dbo.certificado.fecha_envio_plantel,   
         dbo.certificado.observaciones,   
         dbo.certificado.cve_carrera,   
         dbo.certificado.cuenta,   
         dbo.certificado.cve_plan,   
         dbo.certificado.fecha_elaboracion  
    FROM dbo.certificado  
   WHERE ( :cta = 0 ) OR  
         (( dbo.certificado.cve_carrera = :carrera ) AND  
         ( dbo.certificado.cuenta = :cta ) AND  
         ( dbo.certificado.cve_plan = :ai_cve_plan )) 
ORDER BY dbo.certificado.cuenta ASC,   
         dbo.certificado.cve_carrera ASC   
" update="certificado" updatewhere=1 updatekeyinplace=yes arguments=(("cta", number),("carrera", number),("ai_cve_plan", number)) )
