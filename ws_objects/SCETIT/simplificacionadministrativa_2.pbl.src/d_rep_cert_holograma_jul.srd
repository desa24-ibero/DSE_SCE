 retrieve="  SELECT control_sep.orden_cobro,   
         control_sep.cuenta,   
         control_sep.cve_carrera,   
         control_sep.cve_plan,   
         control_sep.numero,   
         control_sep.fecha_emision,   
         control_sep.ciclos_cursados,   
         control_sep.cancelado,   
         control_sep.cve_doc_control_sep,   
         control_sep.num_holograma,   
         control_sep.folio_acta,   
         doc_control_sep.doc_control_sep,   
         alumnos.nombre_completo,   
         alumnos.sexo,   
         alumnos.cve_nacion,   
         nacionalidad.nacionalidad,   
         digito= v_sce_alumno_digito.digito,   
         carreras.num_simpl_admva_sep,   
         plantel = ~"UNIVERSIDAD IBEROAMERICANA CIUDAD DE MÉXICO~"  
    FROM control_sep,   
         doc_control_sep,   
         alumnos,   
         nacionalidad,   
         carreras,
			v_sce_alumno_digito  
   WHERE ( control_sep.cve_doc_control_sep = doc_control_sep.cve_doc_control_sep ) and  
         ( control_sep.cuenta = alumnos.cuenta ) and  
         ( alumnos.cve_nacion = nacionalidad.cve_nacion ) and  
         ( control_sep.cve_carrera = carreras.cve_carrera ) and  
         ( ( control_sep.orden_cobro = :as_orden_cobro ) AND  
         ( control_sep.cve_doc_control_sep in (1,2,3) ) AND  
         ( control_sep.cancelado = 0 ) AND  
         ( datepart(yy,fecha_emision)  =  datepart(yy,getdate()) OR folio_acta = 1) )   AND
			( alumnos.cuenta = v_sce_alumno_digito.cuenta)
 
