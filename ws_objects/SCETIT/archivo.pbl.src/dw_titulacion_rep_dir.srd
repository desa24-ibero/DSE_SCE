 retrieve="  SELECT titulacion.nombre_tesis,   
         titulacion.fecha_examen,   
         titulacion.mencion,   
         titulacion.reconocimiento,   
         titulacion.presidente,   
         titulacion.vocal,   
         titulacion.secretario,   
         domicilio.calle,   
         domicilio.colonia,   
         domicilio.telefono,   
         domicilio.cve_estado,   
         domicilio.cod_postal,   
         titulacion.opcion_titulacion,   
         titulacion.cuenta,   
         titulacion.cve_plan,   
         titulacion.cve_carrera,   
         titulacion.aprobado,   
         alumnos.apaterno,   
         alumnos.amaterno,   
         alumnos.nombre,   
         carreras.carrera,   
         titulacion.observaciones  
    FROM titulacion,   
         domicilio,   
         alumnos,   
         carreras  
   WHERE ( titulacion.cuenta = domicilio.cuenta ) and  
         ( domicilio.cuenta = alumnos.cuenta ) and  
         ( titulacion.cve_carrera = carreras.cve_carrera ) and  
         ( titulacion.fecha_examen >= :fecha_inicial ) AND  
         ( titulacion.fecha_examen <= :fecha_final ) AND  
         ( titulacion.aprobado = 1 ) AND  
         (( titulacion.cve_carrera = :cve_carrera ) OR  
         ( :cve_carrera = 0 ) ) AND
			(( carreras.nivel in (:as_nivel) AND (:as_nivel = 'P')) OR 
			 ( carreras.nivel in (:as_nivel) AND (:as_nivel = 'L')) OR 
			 ( carreras.nivel in ('P', 'L' ) AND :as_nivel not in ('P','L'))) 



" arguments=(("fecha_inicial", date),("fecha_final", date),("cve_carrera", number),("as_nivel", string))  sort="carreras_carrera A titulacion_fecha_examen A alumnos_apaterno A alumnos_amaterno A alumnos_nombre A " )
