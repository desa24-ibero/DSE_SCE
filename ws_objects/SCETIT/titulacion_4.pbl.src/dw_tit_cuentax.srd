table(column=(type=long updatewhereclause=yes name=cve_carrera dbname="cve_carrera" )
 column=(type=char(100) updatewhereclause=yes name=carreras_carrera dbname="carrera" )
 column=(type=long updatewhereclause=yes name=titulacion_cuenta dbname="cuenta" )
 retrieve="  SELECT titulacion.cve_carrera,   
         carreras.carrera,   
         titulacion.cuenta  
    FROM titulacion,   
         carreras  
   WHERE ( titulacion.cve_carrera = carreras.cve_carrera ) and  
         ( titulacion.cuenta in ( :cuenta ) ) AND           
         ( titulacion.aprobado = 1 ) AND
			(( carreras.nivel in (:as_nivel) AND (:as_nivel = 'P')) OR 
			 ( carreras.nivel in (:as_nivel) AND (:as_nivel = 'L')) OR 
			 ( carreras.nivel in ('P', 'L' ) AND :as_nivel not in ('P','L'))) 

         " arguments=(("cuenta", numberlist),("as_nivel", string))  sort="cve_carrera A " )
