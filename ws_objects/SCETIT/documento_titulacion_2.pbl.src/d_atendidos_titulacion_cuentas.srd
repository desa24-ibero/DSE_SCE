 column=(type=long updatewhereclause=yes name=cve_carrera dbname="cve_carrera" )
 column=(type=long updatewhereclause=yes name=cve_plan dbname="cve_plan" )
 retrieve="SELECT  cuenta =titulacion.cuenta,   
        cve_carrera =titulacion.cve_carrera,   
        cve_plan =titulacion.cve_plan
    FROM titulacion,   
         relacion_documentos_alum,
			alumnos
   WHERE ( titulacion.cuenta = alumnos.cuenta)
	and   ( titulacion.cuenta = relacion_documentos_alum.cuenta)
	and   ( titulacion.cve_carrera = relacion_documentos_alum.cve_carrera)
	and   ( titulacion.cve_plan =  relacion_documentos_alum.cve_plan)
	and	( relacion_documentos_alum.cve_relacion = :an_cve_relacion)
	and	( relacion_documentos_alum.cve_tipo_relacion = :an_cve_tipo_relacion )
	ORDER BY titulacion.cve_carrera, alumnos.nombre_completo" arguments=(("an_cve_relacion", number),("an_cve_tipo_relacion", number)) )
