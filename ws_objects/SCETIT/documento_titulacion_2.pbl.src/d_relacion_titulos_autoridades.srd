table(column=(type=long updatewhereclause=yes key=yes name=id dbname="datos_certificado.id" )
 column=(type=char(70) updatewhereclause=yes name=nombre dbname="datos_certificado.nombre" )
 column=(type=char(70) updatewhereclause=yes name=puesto dbname="datos_certificado.puesto" )
 column=(type=char(24) updatewhereclause=yes name=fecha_expedicion dbname="fecha_expedicion" )
 retrieve="  SELECT datos_certificado.id,   
         datos_certificado.nombre,   
         datos_certificado.puesto,
			fecha_expedicion = CASE datepart(mm, :adttm_fecha_expedicion) 
                             WHEN 1 THEN convert(varchar(2),datepart(dd, :adttm_fecha_expedicion)) +~" de Enero de ~"+convert(varchar(4),datepart(yy, :adttm_fecha_expedicion))
                             WHEN 2 THEN convert(varchar(2),datepart(dd, :adttm_fecha_expedicion)) +~" de Febrero de ~"+convert(varchar(4),datepart(yy, :adttm_fecha_expedicion))
                             WHEN 3 THEN convert(varchar(2),datepart(dd, :adttm_fecha_expedicion)) +~" de Marzo de ~"+convert(varchar(4),datepart(yy, :adttm_fecha_expedicion))
                             WHEN 4 THEN convert(varchar(2),datepart(dd, :adttm_fecha_expedicion)) +~" de Abril de ~"+convert(varchar(4),datepart(yy, :adttm_fecha_expedicion))
                             WHEN 5 THEN convert(varchar(2),datepart(dd, :adttm_fecha_expedicion)) +~" de Mayo de ~"+convert(varchar(4),datepart(yy, :adttm_fecha_expedicion))
                             WHEN 6 THEN convert(varchar(2),datepart(dd, :adttm_fecha_expedicion)) +~" de Junio de ~"+convert(varchar(4),datepart(yy, :adttm_fecha_expedicion))
                             WHEN 7 THEN convert(varchar(2),datepart(dd, :adttm_fecha_expedicion)) +~" de Julio de ~"+convert(varchar(4),datepart(yy, :adttm_fecha_expedicion))
                             WHEN 8 THEN convert(varchar(2),datepart(dd, :adttm_fecha_expedicion)) +~" de Agosto de ~"+convert(varchar(4),datepart(yy, :adttm_fecha_expedicion))
                             WHEN 9 THEN convert(varchar(2),datepart(dd, :adttm_fecha_expedicion)) +~" de Septiembre de ~"+convert(varchar(4),datepart(yy, :adttm_fecha_expedicion))
                             WHEN 10 THEN convert(varchar(2),datepart(dd, :adttm_fecha_expedicion)) +~" de Octubre de ~"+convert(varchar(4),datepart(yy, :adttm_fecha_expedicion))
                             WHEN 11 THEN convert(varchar(2),datepart(dd, :adttm_fecha_expedicion)) +~" de Noviembre de ~"+convert(varchar(4),datepart(yy, :adttm_fecha_expedicion))
                             WHEN 12 THEN convert(varchar(2),datepart(dd, :adttm_fecha_expedicion)) +~" de Diciembre de ~"+convert(varchar(4),datepart(yy, :adttm_fecha_expedicion))
                             ELSE ~"~" END
    FROM datos_certificado  
   WHERE datos_certificado.id in (6,1)   
ORDER BY datos_certificado.id DESC   
" update="datos_certificado" updatewhere=0 updatekeyinplace=no arguments=(("adttm_fecha_expedicion", datetime)) )
