 retrieve="SELECT 
carrera = CASE 
						WHEN ac.nivel=~"L~" THEN c.carrera
						WHEN ac.nivel=~"P~" and al.sexo=~"M~" and charindex(~"MAESTR~",c.carrera)>0 THEN c.carrera
						WHEN ac.nivel=~"P~" and al.sexo=~"F~" and charindex(~"MAESTR~",c.carrera)>0 THEN c.carrera
						WHEN ac.nivel=~"P~" and al.sexo=~"M~" and charindex(~"DOCT~",c.carrera)>0 THEN c.carrera
						WHEN ac.nivel=~"P~" and al.sexo=~"F~" and charindex(~"DOCT~",c.carrera)>0 THEN c.carrera
						WHEN ac.nivel=~"P~" and al.sexo=~"M~" and charindex(~"ESPECIAL~",c.carrera)>0 AND charindex(~".~", c.carrera) >0 THEN ~"ESPECIALIDAD ~"+substring(c.carrera, charindex(~".~", c.carrera) + 1, char_length(c.carrera))
						WHEN ac.nivel=~"P~" and al.sexo=~"F~" and charindex(~"ESPECIAL~",c.carrera)>0 AND charindex(~".~", c.carrera) >0 THEN ~"ESPECIALIDAD ~"+substring(c.carrera, charindex(~".~", c.carrera) + 1, char_length(c.carrera))
						WHEN ac.nivel=~"P~" and al.sexo=~"M~" and charindex(~"ESPECIAL~",c.carrera)>0 AND charindex(~" ~", c.carrera) >0 THEN ~"ESPECIALIDAD ~"+substring(c.carrera, charindex(~" ~", c.carrera) + 1, char_length(c.carrera))
						WHEN ac.nivel=~"P~" and al.sexo=~"F~" and charindex(~"ESPECIAL~",c.carrera)>0 AND charindex(~" ~", c.carrera) >0 THEN ~"ESPECIALIDAD ~"+substring(c.carrera, charindex(~" ~", c.carrera) + 1, char_length(c.carrera))
						ELSE ~"~" 
					END,

cuenta_digito = convert(varchar(6),al.cuenta)+~"-~"+ ad.digito, 
nombre_completo = al.nombre +~" ~"+al.apaterno+~" ~"+al.amaterno,
fecha_examen = CASE datepart(mm, t.fecha_examen) 
                             WHEN 1 THEN convert(varchar(2),datepart(dd,t.fecha_examen))+~" DE ENERO DE ~"+convert(varchar(6),datepart(yy,t.fecha_examen))
                             WHEN 2 THEN convert(varchar(2),datepart(dd,t.fecha_examen))+~" DE FEBRERO DE ~"+convert(varchar(6),datepart(yy,t.fecha_examen))
                             WHEN 3 THEN convert(varchar(2),datepart(dd,t.fecha_examen))+~" DE MARZO DE ~"+convert(varchar(6),datepart(yy,t.fecha_examen)) 
                             WHEN 4 THEN convert(varchar(2),datepart(dd,t.fecha_examen))+~" DE ABRIL DE ~"+convert(varchar(6),datepart(yy,t.fecha_examen))
                             WHEN 5 THEN convert(varchar(2),datepart(dd,t.fecha_examen))+~" DE MAYO DE ~"+convert(varchar(6),datepart(yy,t.fecha_examen))
                             WHEN 6 THEN convert(varchar(2),datepart(dd,t.fecha_examen))+~" DE JUNIO DE ~"+convert(varchar(6),datepart(yy,t.fecha_examen))
                             WHEN 7 THEN convert(varchar(2),datepart(dd,t.fecha_examen))+~" DE JULIO DE ~"+convert(varchar(6),datepart(yy,t.fecha_examen))
                             WHEN 8 THEN convert(varchar(2),datepart(dd,t.fecha_examen))+~" DE AGOSTO DE ~"+convert(varchar(6),datepart(yy,t.fecha_examen))
                             WHEN 9 THEN convert(varchar(2),datepart(dd,t.fecha_examen))+~" DE SEPTIEMBRE DE ~"+convert(varchar(6),datepart(yy,t.fecha_examen))
                             WHEN 10 THEN convert(varchar(2),datepart(dd,t.fecha_examen))+~" DE OCTUBRE DE ~"+convert(varchar(6),datepart(yy,t.fecha_examen))
                             WHEN 11 THEN convert(varchar(2),datepart(dd,t.fecha_examen))+~" DE NOVIEMBRE DE ~"+convert(varchar(6),datepart(yy,t.fecha_examen))
                             WHEN 12 THEN convert(varchar(2),datepart(dd,t.fecha_examen))+~" DE DICIEMBRE DE ~"+convert(varchar(6),datepart(yy,t.fecha_examen))
                             ELSE ~"~" END,
horas = CASE 	WHEN datepart(hh,t.fecha_examen)< 10 AND datepart(mm, t.fecha_examen) <10 
						THEN ~"0~"+convert(varchar(2),datepart(hh,t.fecha_examen)) + ~":0~"+ convert(varchar(2),datepart(mi,t.fecha_examen)) 
					WHEN datepart(hh,t.fecha_examen)< 10 AND datepart(mm, t.fecha_examen) >=10 
						THEN ~"0~"+convert(varchar(2),datepart(hh,t.fecha_examen)) + ~":~"+ convert(varchar(2),datepart(mi,t.fecha_examen)) 
					WHEN datepart(hh,t.fecha_examen)>= 10 AND datepart(mm, t.fecha_examen) <10 
						THEN convert(varchar(2),datepart(hh,t.fecha_examen)) + ~":0~"+ convert(varchar(2),datepart(mi,t.fecha_examen)) 
					WHEN datepart(hh,t.fecha_examen)>= 10 AND datepart(mm, t.fecha_examen) >=10 
						THEN convert(varchar(2),datepart(hh,t.fecha_examen)) + ~":~"+ convert(varchar(2),datepart(mi,t.fecha_examen)) 
					ELSE ~"~" END, 
fecha_inicial = CASE datepart(mm, :adttm_fecha_inicial) 
                             WHEN 1 THEN convert(varchar(2),datepart(dd,:adttm_fecha_inicial))+~" DE ENERO DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_inicial))
                             WHEN 2 THEN convert(varchar(2),datepart(dd,:adttm_fecha_inicial))+~" DE FEBRERO DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_inicial))
                             WHEN 3 THEN convert(varchar(2),datepart(dd,:adttm_fecha_inicial))+~" DE MARZO DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_inicial)) 
                             WHEN 4 THEN convert(varchar(2),datepart(dd,:adttm_fecha_inicial))+~" DE ABRIL DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_inicial))
                             WHEN 5 THEN convert(varchar(2),datepart(dd,:adttm_fecha_inicial))+~" DE MAYO DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_inicial))
                             WHEN 6 THEN convert(varchar(2),datepart(dd,:adttm_fecha_inicial))+~" DE JUNIO DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_inicial))
                             WHEN 7 THEN convert(varchar(2),datepart(dd,:adttm_fecha_inicial))+~" DE JULIO DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_inicial))
                             WHEN 8 THEN convert(varchar(2),datepart(dd,:adttm_fecha_inicial))+~" DE AGOSTO DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_inicial))
                             WHEN 9 THEN convert(varchar(2),datepart(dd,:adttm_fecha_inicial))+~" DE SEPTIEMBRE DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_inicial))
                             WHEN 10 THEN convert(varchar(2),datepart(dd,:adttm_fecha_inicial))+~" DE OCTUBRE DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_inicial))
                             WHEN 11 THEN convert(varchar(2),datepart(dd,:adttm_fecha_inicial))+~" DE NOVIEMBRE DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_inicial))
                             WHEN 12 THEN convert(varchar(2),datepart(dd,:adttm_fecha_inicial))+~" DE DICIEMBRE DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_inicial))
                             ELSE ~"~" END,
fecha_final = CASE datepart(mm, :adttm_fecha_final) 
                             WHEN 1 THEN convert(varchar(2),datepart(dd,:adttm_fecha_final))+~" DE ENERO DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_final))
                             WHEN 2 THEN convert(varchar(2),datepart(dd,:adttm_fecha_final))+~" DE FEBRERO DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_final))
                             WHEN 3 THEN convert(varchar(2),datepart(dd,:adttm_fecha_final))+~" DE MARZO DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_final)) 
                             WHEN 4 THEN convert(varchar(2),datepart(dd,:adttm_fecha_final))+~" DE ABRIL DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_final))
                             WHEN 5 THEN convert(varchar(2),datepart(dd,:adttm_fecha_final))+~" DE MAYO DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_final))
                             WHEN 6 THEN convert(varchar(2),datepart(dd,:adttm_fecha_final))+~" DE JUNIO DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_final))
                             WHEN 7 THEN convert(varchar(2),datepart(dd,:adttm_fecha_final))+~" DE JULIO DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_final))
                             WHEN 8 THEN convert(varchar(2),datepart(dd,:adttm_fecha_final))+~" DE AGOSTO DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_final))
                             WHEN 9 THEN convert(varchar(2),datepart(dd,:adttm_fecha_final))+~" DE SEPTIEMBRE DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_final))
                             WHEN 10 THEN convert(varchar(2),datepart(dd,:adttm_fecha_final))+~" DE OCTUBRE DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_final))
                             WHEN 11 THEN convert(varchar(2),datepart(dd,:adttm_fecha_final))+~" DE NOVIEMBRE DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_final))
                             WHEN 12 THEN convert(varchar(2),datepart(dd,:adttm_fecha_final))+~" DE DICIEMBRE DE ~"+convert(varchar(6),datepart(yy,:adttm_fecha_final))
                             ELSE ~"~" END,
monedas = 3
FROM alumnos al, academicos ac, carreras c, titulacion t, v_sce_alumno_digito ad, opc_titulacion ot
WHERE al.cuenta = ac.cuenta
AND al.cuenta = t.cuenta
AND al.cuenta = ad.cuenta
AND ac.cve_carrera = c.cve_carrera
AND t.opcion_titulacion=ot.cve_titulacion
AND t.fecha_examen >= :adttm_fecha_inicial
AND t.fecha_examen <= :adttm_fecha_final
ORDER BY t.fecha_examen" arguments=(("adttm_fecha_inicial", datetime),("adttm_fecha_final", datetime))  sort="fecha_examen A carrera A " )
