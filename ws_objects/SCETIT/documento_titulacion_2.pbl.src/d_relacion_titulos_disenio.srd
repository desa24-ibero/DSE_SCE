 retrieve="  SELECT titulacion.cuenta,   
         v_sce_alumno_digitoverificador.digito_verificador,   
         titulacion.fecha_examen,   
         titulacion.acta,   
         titulacion.libro,   
         titulacion.hoja,   
         titulacion.reconocimiento,   
         titulacion.mencion,   
         carreras.cve_carrera,   
         carreras.carrera,
			carrera_equiv.cve_carrera_ant,
  			nombre_completo = alumnos.nombre+~" ~"+alumnos.apaterno+~" ~"+alumnos.amaterno,
			fecha_examen2 = CASE datepart(mm, titulacion.fecha_examen) 
                             WHEN 1 THEN convert(varchar(2),datepart(dd, titulacion.fecha_examen)) +~" de Enero de ~"+convert(varchar(4),datepart(yy, titulacion.fecha_examen))
                             WHEN 2 THEN convert(varchar(2),datepart(dd, titulacion.fecha_examen)) +~" de Febrero de ~"+convert(varchar(4),datepart(yy, titulacion.fecha_examen))
                             WHEN 3 THEN convert(varchar(2),datepart(dd, titulacion.fecha_examen)) +~" de Marzo de ~"+convert(varchar(4),datepart(yy, titulacion.fecha_examen))
                             WHEN 4 THEN convert(varchar(2),datepart(dd, titulacion.fecha_examen)) +~" de Abril de ~"+convert(varchar(4),datepart(yy, titulacion.fecha_examen))
                             WHEN 5 THEN convert(varchar(2),datepart(dd, titulacion.fecha_examen)) +~" de Mayo de ~"+convert(varchar(4),datepart(yy, titulacion.fecha_examen))
                             WHEN 6 THEN convert(varchar(2),datepart(dd, titulacion.fecha_examen)) +~" de Junio de ~"+convert(varchar(4),datepart(yy, titulacion.fecha_examen))
                             WHEN 7 THEN convert(varchar(2),datepart(dd, titulacion.fecha_examen)) +~" de Julio de ~"+convert(varchar(4),datepart(yy, titulacion.fecha_examen))
                             WHEN 8 THEN convert(varchar(2),datepart(dd, titulacion.fecha_examen)) +~" de Agosto de ~"+convert(varchar(4),datepart(yy, titulacion.fecha_examen))
                             WHEN 9 THEN convert(varchar(2),datepart(dd, titulacion.fecha_examen)) +~" de Septiembre de ~"+convert(varchar(4),datepart(yy, titulacion.fecha_examen))
                             WHEN 10 THEN convert(varchar(2),datepart(dd, titulacion.fecha_examen)) +~" de Octubre de ~"+convert(varchar(4),datepart(yy, titulacion.fecha_examen))
                             WHEN 11 THEN convert(varchar(2),datepart(dd, titulacion.fecha_examen)) +~" de Noviembre de ~"+convert(varchar(4),datepart(yy, titulacion.fecha_examen))
                             WHEN 12 THEN convert(varchar(2),datepart(dd, titulacion.fecha_examen)) +~" de Diciembre de ~"+convert(varchar(4),datepart(yy, titulacion.fecha_examen))
                             ELSE ~"~" END,
			titulacion.num_titulo
    FROM diseño_titulacion,   
         titulacion,   
         carreras,   
         carrera_equiv,   
         v_sce_alumno_digitoverificador,
			alumnos  
   WHERE ( diseño_titulacion.cuenta = titulacion.cuenta ) and  
         ( diseño_titulacion.cve_plan = titulacion.cve_plan ) and  
         ( diseño_titulacion.cve_carrera = titulacion.cve_carrera ) and  
         ( titulacion.cve_carrera = carreras.cve_carrera ) and  
         ( titulacion.cuenta = v_sce_alumno_digitoverificador.cuenta ) and  
         ( titulacion.cuenta = alumnos.cuenta ) and  
         ( carrera_equiv.cve_carrera_act = carreras.cve_carrera ) and  
         ( ( titulacion.aprobado = 1 ) )   and 
			( diseño_titulacion.fecha_envio >= :adttm_fecha_inicial) and
			( diseño_titulacion.fecha_envio <= :adttm_fecha_final)
ORDER BY carreras.carrera ASC,   
         titulacion.cuenta ASC   
" arguments=(("adttm_fecha_inicial", datetime),("adttm_fecha_final", datetime))  sort="cve_carrera A carrera A cve_carrera_ant A fecha_examen A " )
