 retrieve="select distinct cuenta = dt.cuenta, 
cve_carrera = dt.cve_carrera, 
cve_plan = dt.cve_plan, 
codigo = dt.codigo, 
numero_titulo = dt.numero_titulo, 
id_titulo = dt.id_titulo ,
nombre_completo = a.nombre_completo,
dt.codigo_completo,
dt.fecha_expedicion,
N_ACTA = t.acta,
N_LIBRO = t.libro,
N_HOJA = t.hoja,
F_EXAMEN = t.fecha_examen,
F_EXPED	  = dt.fecha_expedicion,
c.cve_grado,
NOMBRES = a.nombre,
A_PAT	= a.apaterno,
A_MAT	= a.amaterno,
ID_UNIV = 10,	
ID_PLANTEL = 1
from diseño_titulacion dt, bit_documento_titulacion b, alumnos a, titulacion t, carreras c
where dt.cuenta = b.cuenta
and	dt.cve_carrera = b.cve_carrera
and	dt.cve_plan = b.cve_plan
and 	t.cuenta = dt.cuenta
and 	t.cve_plan = dt.cve_plan
and 	t.cve_carrera = dt.cve_carrera
and 	t.cve_carrera = c.cve_carrera 
and 	b.cve_documento = :ai_cve_documento
and 	b.cve_carrera  not in (null, 0)
and 	isnull(b.cancelado,0) not in (1)
--and 	b.fecha >= :adt_fecha_inicial
--and 	b.fecha < :adt_fecha_final
--and convert(datetime, b.fecha) >= :adt_fecha_inicial
--and convert(datetime, b.fecha) <= :adt_fecha_final
and convert(varchar(08), convert(date, b.fecha), 112) >=  :adt_fecha_inicial
and convert(varchar(08), convert(date, b.fecha), 112) <=  :adt_fecha_final
--and 	dt.codigo_completo in (null)
and   dt.cuenta = a.cuenta
ORDER BY dt.cve_carrera, a.nombre_completo 
" update="diseño_titulacion" updatewhere=0 updatekeyinplace=no arguments=(("adt_fecha_inicial", string),("adt_fecha_final", string),("ai_cve_documento", number)) )
