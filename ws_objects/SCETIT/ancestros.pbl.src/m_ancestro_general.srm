on m_ancestro_general.create
m_ancestro_general=this
call super::create
this.m_tipodecuota=create m_tipodecuota
this.m_registro=create m_registro
this.m_procesos=create m_procesos
this.m_pasos=create m_pasos
this.m_ver=create m_ver
this.m_animacion=create m_animacion
this.m_ventana=create m_ventana
this.m_ayuda=create m_ayuda
this.Item[UpperBound(this.item)+1]=this.m_tipodecuota
this.Item[UpperBound(this.item)+1]=this.m_registro
this.Item[UpperBound(this.item)+1]=this.m_procesos
this.Item[UpperBound(this.item)+1]=this.m_pasos
this.Item[UpperBound(this.item)+1]=this.m_ver
this.Item[UpperBound(this.item)+1]=this.m_animacion
this.Item[UpperBound(this.item)+1]=this.m_ventana
this.Item[UpperBound(this.item)+1]=this.m_ayuda
end on

on m_ancestro_general.destroy
call super::destroy
destroy(this.m_tipodecuota)
destroy(this.m_registro)
destroy(this.m_procesos)
destroy(this.m_pasos)
destroy(this.m_ver)
destroy(this.m_animacion)
destroy(this.m_ventana)
destroy(this.m_ayuda)
end on

