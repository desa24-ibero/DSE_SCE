 column=(type=datetime updatewhereclause=yes name=legalizacion_fecha_regreso_sep dbname="legalizacion.fecha_regreso_sep" )
 column=(type=char(20) updatewhereclause=yes name=legalizacion_num_pago_sep dbname="legalizacion.num_pago_sep" )
 column=(type=char(50) updatewhereclause=yes name=legalizacion_observaciones dbname="legalizacion.observaciones" )
 retrieve="  SELECT titulacion.cuenta,   
         titulacion.cve_plan,   
         titulacion.cve_carrera,   
         carreras.carrera,   
         titulacion.aprobado,   
         legalizacion.fecha_envio_sep,   
         legalizacion.fecha_regreso_sep,   
         legalizacion.num_pago_sep,   
         legalizacion.observaciones  
    FROM titulacion,   
         carreras,   
         legalizacion  
   WHERE ( titulacion.cve_carrera = carreras.cve_carrera ) and  
         ( legalizacion.cuenta = titulacion.cuenta ) and  
         ( legalizacion.cve_plan = titulacion.cve_plan ) and  
         ( legalizacion.cve_carrera = titulacion.cve_carrera ) and  
         ( titulacion.cuenta in ( :cuenta ) ) AND 
         (titulacion.aprobado = 1 ) AND  
         (legalizacion.fecha_regreso_sep != null  )" arguments=(("cuenta", numberlist))  sort="cve_carrera A " )
