table(column=(type=char(255) updatewhereclause=yes name=dgmu_materias_materia dbname="dgmu_materias.materia" )
 column=(type=long updatewhereclause=yes name=dgmu_mat_inscritas_anio dbname="dgmu_mat_inscritas.anio" )
 column=(type=long updatewhereclause=yes name=dgmu_mat_inscritas_periodo dbname="dgmu_mat_inscritas.periodo" )
 column=(type=char(70) updatewhereclause=yes name=dgmu_coordinaciones_coordinacion dbname="dgmu_coordinaciones.coordinacion" )
 column=(type=long updatewhereclause=yes name=dgmu_coordinaciones_cve_coordinacion dbname="dgmu_coordinaciones.cve_coordinacion" )
 retrieve="SELECT	distinct mat.materia,
			mi.anio,
			mi.periodo,
			c.coordinacion,
			c.cve_coordinacion
FROM		dgmu_mat_inscritas mi 
			JOIN
			(
				SELECT	CASE
							WHEN ( SELECT count(*) FROM hist_carreras WHERE cuenta = :cuenta AND cve_carrera_ant = :cve_carrera AND egresado_ant = 1 ) > 0
								THEN ( SELECT anio_ing FROM hist_carreras WHERE cuenta = :cuenta AND cve_carrera_ant = :cve_carrera AND egresado_ant = 1 )
							ELSE
								anio_ing
						END AS anio_ingreso,
						CASE
							WHEN ( SELECT count(*) FROM hist_carreras WHERE cuenta = :cuenta AND cve_carrera_ant = :cve_carrera AND egresado_ant = 1 ) > 0
								THEN ( SELECT periodo_ing FROM hist_carreras WHERE cuenta = :cuenta AND cve_carrera_ant = :cve_carrera AND egresado_ant = 1 )
							ELSE
								periodo_ing
						END AS periodo_ingreso,
						CASE
							WHEN ( SELECT count(*) FROM hist_carreras WHERE cuenta = :cuenta AND cve_carrera_ant = :cve_carrera AND egresado_ant = 1 ) > 0
								THEN ( SELECT anio_egre_ant FROM hist_carreras WHERE cuenta = :cuenta AND cve_carrera_ant = :cve_carrera AND egresado_ant = 1 )
							ELSE
								anio_egre
						END AS anio_egreso,
						CASE
							WHEN ( SELECT count(*) FROM hist_carreras WHERE cuenta = :cuenta AND cve_carrera_ant = :cve_carrera AND egresado_ant = 1 ) > 0
								THEN ( SELECT periodo_egre_ant FROM hist_carreras WHERE cuenta = :cuenta AND cve_carrera_ant = :cve_carrera AND egresado_ant = 1 )
							ELSE
								periodo_egre
						END AS periodo_egreso
				FROM	academicos
				WHERE	cuenta = :cuenta
			) AS A
			ON
			(
				mi.anio = A.anio_ingreso AND mi.periodo >= A.periodo_ingreso 
			) 
			OR
			(
				mi.anio > A.anio_ingreso AND mi.anio < A.anio_egreso
			)
			OR
			(
				mi.anio = A.anio_egreso AND mi.periodo <= A. periodo_egreso
			),
			dgmu_materias mat,
			dgmu_coordinaciones c
WHERE	mi.cuenta = :cuenta AND
			mi.cve_mat = mat.cve_mat AND
			mat.cve_coordinacion = c.cve_coordinacion
ORDER BY mi.anio, mi.periodo" arguments=(("cuenta", number),("cve_carrera", number)) )
