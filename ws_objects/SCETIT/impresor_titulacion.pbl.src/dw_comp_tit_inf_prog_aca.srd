table(column=(type=long updatewhereclause=yes name=cuenta dbname="alumnos.cuenta" dbalias="a.cuenta" )
 column=(type=long updatewhereclause=yes name=cve_carrera dbname="titulacion.cve_carrera" dbalias="t.cve_carrera" )
 column=(type=long updatewhereclause=yes name=cve_plan dbname="titulacion.cve_plan" dbalias="t.cve_plan" )
 column=(type=long updatewhereclause=yes name=semestre_ideal dbname="semestre_ideal" )
 column=(type=char(1) updatewhereclause=yes name=nivel dbname="carreras.nivel" dbalias="c.nivel" )
 column=(type=char(1) updatewhereclause=yes name=cve_grado dbname="carreras.cve_grado" dbalias="c.cve_grado" )
 column=(type=long updatewhereclause=yes name=creditos_asig_troncales dbname="creditos_asig_troncales" )
 column=(type=long updatewhereclause=yes name=horas_asig_troncales dbname="horas_asig_troncales" )
 column=(type=long updatewhereclause=yes name=creditos_asig_optativas dbname="creditos_asig_optativas" )
 column=(type=long updatewhereclause=yes name=horas_asig_optativas dbname="horas_asig_optativas" )
 column=(type=long updatewhereclause=yes name=creditos_servicio_social dbname="creditos_servicio_social" )
 column=(type=long updatewhereclause=yes name=horas_servicio_social dbname="horas_servicio_social" )
 retrieve="SELECT	a.cuenta, 
			t.cve_carrera, 
			t.cve_plan,
			(
				SELECT max( mp.semestre_ideal ) FROM mat_prerrequisito mp WHERE t.cve_carrera = mp.cve_carrera AND t.cve_plan = mp.cve_plan 
			) as semestre_ideal,
			c.nivel, 
			c.cve_grado,
			(
				SELECT	sum(m.creditos)
				FROM	area_mat am,
						materias m
				WHERE	am.cve_mat = m.cve_mat AND
						am.cve_area =
						(
							SELECT	cve_area_basica
							FROM	plan_estudios
							WHERE	cve_plan = t.cve_plan AND cve_carrera = t.cve_carrera
						)

			)
			+
			(
				SELECT	sum(m.creditos)
				FROM	area_mat am,
						materias m
				WHERE	am.cve_mat = m.cve_mat AND
						am.cve_area =
						(
							SELECT	cve_area_mayor_oblig
							FROM	plan_estudios
							WHERE	cve_plan = t.cve_plan AND cve_carrera = t.cve_carrera
						)
			) creditos_asig_troncales,
			(
				SELECT	sum(m.horas_reales)
				FROM	area_mat am,
						materias m
				WHERE	am.cve_mat = m.cve_mat AND
						am.cve_area =
						(
							SELECT	cve_area_basica
							FROM	plan_estudios
							WHERE	cve_plan = t.cve_plan AND cve_carrera = t.cve_carrera
						)

			)
			+
			(
				SELECT	sum(m.horas_reales)
				FROM	area_mat am,
						materias m
				WHERE	am.cve_mat = m.cve_mat AND
						am.cve_area =
						(
							SELECT	cve_area_mayor_oblig
							FROM	plan_estudios
							WHERE	cve_plan = t.cve_plan AND cve_carrera = t.cve_carrera
						)
			) horas_asig_troncales,
			(
				SELECT	sum(m.creditos)
				FROM	area_mat am,
						materias m
				WHERE	am.cve_mat = m.cve_mat AND
						am.cve_area =
						(
							SELECT	cve_area_mayor_opt
							FROM	plan_estudios
							WHERE	cve_plan = t.cve_plan AND cve_carrera = t.cve_carrera
						)
			) creditos_asig_optativas,
			(
				SELECT	sum(m.horas_reales)
				FROM	area_mat am,
						materias m
				WHERE	am.cve_mat = m.cve_mat AND
						am.cve_area =
						(
							SELECT	cve_area_mayor_opt
							FROM	plan_estudios
							WHERE	cve_plan = t.cve_plan AND cve_carrera = t.cve_carrera
						)
			) horas_asig_optativas,
			(
				SELECT	sum(m.creditos)
				FROM	area_mat am,
						materias m
				WHERE	am.cve_mat = m.cve_mat AND
						am.cve_area =
						(
							SELECT	cve_area_servicio_social
							FROM	plan_estudios
							WHERE	cve_plan = t.cve_plan AND cve_carrera = t.cve_carrera
						)
			) creditos_servicio_social,
			(
				SELECT	sum(m.horas_reales)
				FROM	area_mat am,
						materias m
				WHERE	am.cve_mat = m.cve_mat AND
						am.cve_area =
						(
							SELECT	cve_area_servicio_social
							FROM	plan_estudios
							WHERE	cve_plan = t.cve_plan AND cve_carrera = t.cve_carrera
						)
			) horas_servicio_social
FROM		titulacion t, 
			diseño_titulacion dt, 
			alumnos a,
			carreras c
WHERE	t.cuenta = dt.cuenta AND 
			t.cve_plan = dt.cve_plan AND 
			t.cve_carrera = dt.cve_carrera AND 
			t.cve_carrera = c.cve_carrera AND 
			a.cuenta = t.cuenta AND 
			a.cuenta = :cuenta AND
			t.cve_carrera = :cve_carrera" arguments=(("cve_idioma", number),("cuenta", number),("cve_carrera", number)) )
