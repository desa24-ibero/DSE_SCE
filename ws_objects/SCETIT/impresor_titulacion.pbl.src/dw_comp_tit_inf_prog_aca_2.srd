table(column=(type=long updatewhereclause=yes name=cuenta dbname="academicos.cuenta" )
 column=(type=long updatewhereclause=yes name=cve_carrera dbname="titulacion.cve_carrera" )
 column=(type=long updatewhereclause=yes name=cve_plan dbname="titulacion.cve_plan" )
 column=(type=char(1) updatewhereclause=yes name=nivel dbname="carreras.nivel" )
 column=(type=char(1) updatewhereclause=yes name=cve_grado dbname="carreras.cve_grado" )
 column=(type=real updatewhereclause=yes name=promedio dbname="academicos.promedio" )
 column=(type=real updatewhereclause=yes name=puntaje_min dbname="puntaje_min" )
 column=(type=char(255) updatewhereclause=yes name=nombre_tesis dbname="titulacion.nombre_tesis" )
 column=(type=datetime updatewhereclause=yes name=dia_hoy dbname="dia_hoy" )
 column=(type=char(70) updatewhereclause=yes name=direc_se dbname="direc_se" )
 column=(type=char(70) updatewhereclause=yes name=jefe_tramitacion dbname="jefe_tramitacion" )
 retrieve="SELECT	ac.cuenta, 
			t.cve_carrera, 
			t.cve_plan, 
			c.nivel, 
			c.cve_grado, 
			ac.promedio, 
			(
				SELECT pe.puntaje_min FROM plan_estudios pe WHERE pe.cve_carrera = t.cve_carrera AND pe.cve_plan = t.cve_plan 
			) as puntaje_min,
			t.nombre_tesis, 
			getdate() as dia_hoy, 
			(
				SELECT r.nombre FROM datos_certificado r WHERE r.id = 1
			) as direc_se,
			(
				SELECT r.nombre FROM datos_certificado r WHERE r.id = 2
			) as jefe_tramitacion
FROM		titulacion t, 
			diseño_titulacion dt, 
			alumnos a,
			carreras c, 
			academicos ac
WHERE	t.cuenta = dt.cuenta AND 
			t.cve_plan = dt.cve_plan AND 
			t.cve_carrera = dt.cve_carrera AND 
			t.cve_carrera = c.cve_carrera AND 
			a.cuenta = t.cuenta AND 
			a.cuenta = ac.cuenta AND 
			a.cuenta = :cuenta AND
			t.cve_carrera = :cve_carrera" arguments=(("cve_idioma", number),("cuenta", number),("cve_carrera", number)) )
