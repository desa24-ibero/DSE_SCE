table(column=(type=long update=yes updatewhereclause=yes name=cuenta dbname="titulacion.cuenta" dbalias="t.cuenta" )
 column=(type=char(30) updatewhereclause=yes name=digito dbname="v_sce_alumno_digito.digito" dbalias="d.digito" )
 column=(type=long update=yes updatewhereclause=yes name=cve_carrera dbname="carreras.cve_carrera" dbalias="c.cve_carrera" )
 column=(type=char(100) updatewhereclause=yes name=carrera dbname="carreras.carrera" dbalias="c.carrera" )
 column=(type=char(152) updatewhereclause=yes name=nombre_2 dbname="nombre_2" )
 column=(type=datetime updatewhereclause=yes name=fecha_hoy dbname="fecha_hoy" )
 retrieve="  SELECT DISTINCT t.cuenta, d. digito, c.cve_carrera, c.carrera, a.apaterno +''+ a.amaterno + '' + a.nombre as nombre_2,
			getdate() as fecha_hoy
	FROM titulacion t LEFT OUTER JOIN hoja_libro_carrera h ON t.cuenta = h.cuenta AND t.cve_plan = h.cve_plan AND t.cve_carrera = h.cve_carrera
					  	   LEFT OUTER JOIN control_sep s ON t.cuenta = s.cuenta AND t.cve_plan = s.cve_plan AND t.cve_carrera = s.cve_carrera AND s.cve_doc_control_sep = 4,
		 diseño_titulacion dt, carreras c, alumnos a, v_sce_alumno_digito d
   WHERE t.cuenta = dt.cuenta
	 AND t.cve_plan = dt.cve_plan
	 AND t.cve_carrera = dt.cve_carrera
	 AND t.cve_carrera = c. cve_carrera 
	 AND a.cuenta = t.cuenta
	 AND a.cuenta = d.cuenta
	 AND convert(varchar(08), t.fecha_examen, 112) >= :ar_fecha_inicio
	 AND convert(varchar(08), t.fecha_examen, 112) <= :ar_fecha_fin
	 AND c.cve_grado = 'L'
	 AND t.aprobado = 1
ORDER BY t.cuenta" arguments=(("ar_fecha_inicio", string),("ar_fecha_fin", string),("ar_cve_relacion", number))  sort="cuenta A " )
