table(column=(type=long updatewhereclause=yes name=cuenta dbname="alumnos.cuenta" dbalias="a.cuenta" )
 column=(type=long updatewhereclause=yes name=cve_carrera dbname="titulacion.cve_carrera" dbalias="t.cve_carrera" )
 column=(type=long updatewhereclause=yes name=cve_plan dbname="titulacion.cve_plan" dbalias="t.cve_plan" )
 column=(type=long updatewhereclause=yes name=semestre_ideal dbname="semestre_ideal" )
 column=(type=char(1) updatewhereclause=yes name=nivel dbname="carreras.nivel" dbalias="c.nivel" )
 column=(type=char(1) updatewhereclause=yes name=cve_grado dbname="carreras.cve_grado" dbalias="c.cve_grado" )
 retrieve="SELECT	a.cuenta, 
			t.cve_carrera, 
			t.cve_plan,
			(
				SELECT max( mp.semestre_ideal ) FROM mat_prerrequisito mp WHERE t.cve_carrera = mp.cve_carrera AND t.cve_plan = mp.cve_plan 
			) as semestre_ideal,
			c.nivel, 
			c.cve_grado
FROM		titulacion t, 
			diseño_titulacion dt, 
			alumnos a,
			carreras c
WHERE	t.cuenta = dt.cuenta AND 
			t.cve_plan = dt.cve_plan AND 
			t.cve_carrera = dt.cve_carrera AND 
			t.cve_carrera = c.cve_carrera AND 
			a.cuenta = t.cuenta AND 
			a.cuenta = :cuenta and
			t.cve_carrera = :cve_carrera" arguments=(("cve_idioma", number),("cuenta", number),("cve_carrera", number)) )
