 column=(type=char(100) updatewhereclause=yes name=carrera_sin_prefijo dbname="carreras.carrera_sin_prefijo" )
 column=(type=datetime updatewhereclause=yes name=fecha_examen dbname="titulacion.fecha_examen" )
 column=(type=long updatewhereclause=yes name=monedas dbname="monedas" )
 column=(type=char(1) updatewhereclause=yes name=rec_mencion dbname="rec_mencion" )
 column=(type=long updatewhereclause=yes name=hr_exa dbname="hr_exa" )
 column=(type=long updatewhereclause=yes name=min_exa dbname="min_exa" )
 column=(type=char(8) updatewhereclause=yes name=fecha_exam_fmt dbname="fecha_exam_fmt" )
 column=(type=datetime updatewhereclause=yes name=fecha_hoy dbname="fecha_hoy" )
 column=(type=char(1) updatewhereclause=yes name=cve_grado dbname="carreras.cve_grado" )
 column=(type=long updatewhereclause=yes name=opcion_titulacion dbname="titulacion.opcion_titulacion" )
 column=(type=long updatewhereclause=yes name=total_alumnos dbname="total_alumnos" )
 retrieve="SELECT 	t.cuenta, d. digito, a.nombre +''+ a.apaterno +''+ a.amaterno as nombre, c.cve_carrera, c.carrera_sin_prefijo, 
			t.fecha_examen, 3 as monedas,
	   		Case When t.mencion = 1 and t.reconocimiento = 2 then 'M' When t.reconocimiento = 1 and t.mencion = 2 then 'R' else '' end as rec_mencion, 
	   		datepart(hh,t.fecha_examen) hr_exa, datepart(mi,t.fecha_examen) min_exa, convert(varchar(08), t.fecha_examen , 112) as fecha_exam_fmt, getdate() as fecha_hoy, c.cve_grado, t.opcion_titulacion, 0 as total_alumnos
  FROM	titulacion t LEFT OUTER JOIN hoja_libro_posgrado h ON t.cuenta = h.cuenta AND t.cve_plan = h.cve_plan AND t.cve_carrera = h.cve_carrera,
	   		carreras c, alumnos a, v_sce_alumno_digito d
 WHERE 	t.cve_carrera = c.cve_carrera 
	 AND  a.cuenta = t.cuenta
	 AND  a.cuenta = d.cuenta
	 AND	convert(varchar(08), t.fecha_examen , 112) >=  :ar_fecha_inicio
	 AND	convert(varchar(08), t.fecha_examen , 112) <=  :ar_fecha_final
	 AND	c.cve_grado in ('M', 'D')
	 AND t.opcion_titulacion <> 5
  UNION
SELECT 99999 as cuenta, '0' as digito, min(nombre_tesis)  as nombre,t1.cve_carrera,  c.carrera_sin_prefijo,
		   t1.fecha_examen, 3 as monedas, '' as mencion, datepart(hh,t1.fecha_examen) hr_exa, datepart(mi,t1.fecha_examen) min_exa, convert(varchar(08), t1.fecha_examen , 112) as fecha_exam_fmt, getdate() as fecha_hoy, min(c.cve_grado) as cve_grado, MIN(t1.opcion_titulacion), COUNT(*) as total_alumnos
   FROM titulacion t1, carreras c
  WHERE t1.opcion_titulacion = 5
	AND t1.cve_carrera = c.cve_carrera
	AND	convert(varchar(08), t1.fecha_examen , 112) >=  :ar_fecha_inicio
	AND	convert(varchar(08), t1.fecha_examen , 112) <=  :ar_fecha_final
GROUP BY t1.fecha_examen, t1.cve_carrera, c.carrera_sin_prefijo" arguments=(("ar_fecha_inicio", string),("ar_fecha_final", string))  sort="fecha_examen A " )
