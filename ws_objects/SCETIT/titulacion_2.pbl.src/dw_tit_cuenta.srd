table(column=(type=char(100) updatewhereclause=yes name=carreras_carrera dbname="carrera" )
 column=(type=long updatewhereclause=yes name=titulacion_cuenta dbname="cuenta" )
 column=(type=long updatewhereclause=yes name=titulacion_cve_carrera dbname="cve_carrera" )
 retrieve="  SELECT carreras.carrera,   
         titulacion.cuenta,   
         titulacion.cve_carrera  
    FROM titulacion,   
         carreras  
   WHERE ( titulacion.cve_carrera = carreras.cve_carrera ) and  
         ( titulacion.fecha_examen >= :fecha_inicial ) AND  
         ( titulacion.fecha_examen <= :fecha_final ) AND  
         ( titulacion.aprobado = 1 ) AND
         (( titulacion.cve_carrera = :cve_carrera ) OR  
         ( :cve_carrera = 0 ) ) AND
			(( carreras.nivel in (:as_nivel) AND (:as_nivel = 'P')) OR 
			 ( carreras.nivel in (:as_nivel) AND (:as_nivel = 'L')) OR 
			 ( carreras.nivel in ('P', 'L' ) AND :as_nivel not in ('P','L'))) 
    
" arguments=(("fecha_inicial", date),("fecha_final", date),("cve_carrera", number),("as_nivel", string))  sort="titulacion_cve_carrera A " )
