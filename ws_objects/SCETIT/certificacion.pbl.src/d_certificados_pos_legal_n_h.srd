 column=(type=long updatewhereclause=yes name=cuerpo_reporte_pagina dbname="cuerpo_reporte.pagina" )
 retrieve="  SELECT alumnos.cuenta,   
         alumnos.nombre,   
         alumnos.apaterno,   
         alumnos.amaterno,   
         datos_certificado.nombre,   
         carreras.carrera,   
         carreras.nivel,   
         cuerpo_reporte.num_renglon,   
         cuerpo_reporte.texto_renglon,   
         cuerpo_reporte.pagina  
    FROM alumnos,   
         datos_certificado,   
         hist_carreras,   
         carreras,   
         cuerpo_reporte  
   WHERE ( hist_carreras.cuenta = alumnos.cuenta ) and  
         ( hist_carreras.cve_carrera_ant = carreras.cve_carrera ) and  
         ( hist_carreras.cve_carrera_ant = cuerpo_reporte.cve_carrera ) and  
         ( hist_carreras.cuenta = cuerpo_reporte.cuenta ) and  
         ( ( alumnos.cuenta = :an_cuenta ) AND  
         ( cuerpo_reporte.cve_tipo_doc_rep = :an_cve_tipo_doc_rep ) AND  
         ( datos_certificado.id = :id_certificado ) AND  
         ((cuerpo_reporte.pagina = :an_pagina) OR  
         (9999 = :an_pagina AND  
         cuerpo_reporte.pagina > 1)) )    AND
         ( hist_carreras.cve_carrera_ant = :an_cve_carrera)

