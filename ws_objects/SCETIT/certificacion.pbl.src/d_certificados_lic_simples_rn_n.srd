 retrieve="
  SELECT DISTINCT alumnos.cuenta,   
         alumnos.nombre,   
         alumnos.apaterno,   
         alumnos.amaterno,   
         datos_certificado_a.nombre,   
         carreras.carrera,   
         carreras.nivel,   
         materias_certificado.sigla,   
         materias_certificado.materia,   
         materias_certificado.periodo_str,   
         materias_certificado.creditos,   
         materias_certificado.calificacion_str,   
         materias_certificado.obs,   
         alumnos.sexo,   
         carreras.cve_grado,   
         grado.grado,   
         materias_certificado.calificacion,   
         plan_estudios.incorporado_sep,   
         materias_certificado.cve_mat,   
         materias_certificado.tipo,   
         materias_certificado.cve_idioma,   
         materias_certificado.numero_pagina,   
         grado.cve_grado,   
         datos_certificado_b.nombre,   
         materias_certificado.sigla_anterior,   
         historico.cve_plan,   
         datos_certificado_a.puesto  
    FROM alumnos,   
         datos_certificado datos_certificado_a,   
         historico,   
         carreras,   
         datos_certificado datos_certificado_b,   
         materias_certificado,   
         grado,   
         plan_estudios  
   WHERE ( historico.cuenta = alumnos.cuenta ) and  
         ( historico.cve_carrera = carreras.cve_carrera ) and  
         ( historico.cve_carrera = materias_certificado.cve_carrera ) and  
         ( historico.cuenta = materias_certificado.cuenta ) and  
         ( carreras.cve_grado = grado.cve_grado ) and  
         ( historico.cve_carrera = plan_estudios.cve_carrera ) and  
         ( historico.cve_plan = plan_estudios.cve_plan ) and  
         historico.cve_plan = :cve_plan AND
         ( ( alumnos.cuenta = :an_cuenta ) AND  
         ( materias_certificado.cve_tipo_doc_rep = :an_cve_tipo_doc_rep ) AND  
         ( datos_certificado_a.id = 9 ) AND  
         ((materias_certificado.numero_pagina = :an_pagina) OR  
         (9999 = :an_pagina AND  
         materias_certificado.numero_pagina > 1)) AND  
         (historico.cve_carrera = :an_cve_carrera) AND  
         datos_certificado_b.id = 10 )   
ORDER BY materias_certificado.numero_pagina ASC,   
         materias_certificado.renglon_relativo ASC   



