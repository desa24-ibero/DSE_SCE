table(column=(type=long updatewhereclause=yes name=cuenta dbname="cuenta" )
 column=(type=long updatewhereclause=yes name=cve_carrera dbname="cve_carrera" )
 column=(type=long updatewhereclause=yes name=cve_plan dbname="cve_plan" )
 column=(type=long updatewhereclause=yes name=cve_documento dbname="cve_documento" )
 column=(type=char(30) updatewhereclause=yes name=descripcion dbname="descripcion" )
 retrieve="SELECT DISTINCT cuenta =solicitud_certificado.cuenta,   
         cve_carrera =solicitud_certificado.cve_carrera,   
         cve_plan =solicitud_certificado.cve_plan,   
         cve_documento =doc_control_sep.cve_doc_control_sep,   
         descripcion =doc_control_sep.doc_control_sep  
    FROM solicitud_certificado,   
         certificado,
         doc_control_sep,
			alumnos  
   WHERE ( solicitud_certificado.cve_doc_control_sep = doc_control_sep.cve_doc_control_sep )
	and   ( solicitud_certificado.fecha_solicitud > :adttm_fecha_inicial AND  
           solicitud_certificado.fecha_solicitud < :adttm_fecha_final) 
	and   ( solicitud_certificado.cuenta = alumnos.cuenta)
	and   ( solicitud_certificado.cve_carrera not in (null) )
	and   ( solicitud_certificado.cve_plan not in (null) )
	and   ( solicitud_certificado.cuenta = certificado.cuenta )
	and   ( solicitud_certificado.cve_carrera = certificado.cve_carrera )
	and   ( solicitud_certificado.cve_plan= certificado.cve_plan )
   and   ((certificado.legalizado = 1 and solicitud_certificado.cve_doc_control_sep in (1))
   or     (certificado.legalizado = 0 and solicitud_certificado.cve_doc_control_sep in (2)))
	and   ( solicitud_certificado.fecha_solicitud = certificado.fecha_inicio )
	ORDER BY solicitud_certificado.cve_carrera, alumnos.nombre_completo" arguments=(("adttm_fecha_inicial", datetime),("adttm_fecha_final", datetime)) )
