 retrieve="SELECT cuenta = hist_carreras.cuenta,   
       nombre_completo = alumnos.nombre_completo,   
       cve_carrera = carreras.cve_carrera,   
       carrera = carreras.carrera,   
       cve_plan = nombre_plan.cve_plan,   
       nombre_plan = nombre_plan.nombre_plan,
		 cve_subsistema = hist_carreras.cve_subsistema_ant	
    FROM hist_carreras,
			carreras,   
         nombre_plan,   
         alumnos  
   WHERE ( hist_carreras.cve_carrera_ant = carreras.cve_carrera ) and  
         ( hist_carreras.cve_plan_ant = nombre_plan.cve_plan ) and  
         ( hist_carreras.cuenta = alumnos.cuenta ) and  
         ( ( hist_carreras.cuenta = :al_cuenta ) AND  
         ( hist_carreras.cve_carrera_ant = :al_cve_carrera ) AND  
         ( hist_carreras.cve_plan_ant = :al_cve_plan ) )    
UNION all
SELECT cuenta = academicos.cuenta,   
       nombre_completo = alumnos.nombre_completo,   
       cve_carrera = carreras.cve_carrera,   
       carrera = carreras.carrera,   
       cve_plan = nombre_plan.cve_plan,   
       nombre_plan = nombre_plan.nombre_plan,
		 cve_subsistema = academicos.cve_subsistema
    FROM academicos,
			carreras,   
         nombre_plan,   
         alumnos  
   WHERE ( academicos.cve_carrera = carreras.cve_carrera ) and  
         ( academicos.cve_plan = nombre_plan.cve_plan ) and  
         ( academicos.cuenta = alumnos.cuenta ) and  
         ( ( academicos.cuenta = :al_cuenta ) AND  
         ( academicos.cve_carrera = :al_cve_carrera ) AND  
         ( academicos.cve_plan = :al_cve_plan ) )    
" arguments=(("al_cuenta", number),("al_cve_carrera", number),("al_cve_plan", number)) )
