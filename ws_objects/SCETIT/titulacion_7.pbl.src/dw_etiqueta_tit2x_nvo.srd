 retrieve="  SELECT titulacion.cve_carrera,   
         titulacion.mencion,   
         alumnos.nombre,   
         alumnos.apaterno,   
         alumnos.amaterno,   
         titulacion.cuenta,   
         titulacion.reconocimiento,  
         carreras.carrera,
         titulacion.fecha_examen
    FROM titulacion,   
         alumnos,
         carreras  
   WHERE ( titulacion.cuenta = alumnos.cuenta ) AND  
         ( titulacion.cuenta in ( :cuenta ) ) AND 
         (titulacion.reconocimiento = 1 ) AND
         (titulacion.aprobado = 1 ) AND
         (titulacion.cve_carrera = carreras.cve_carrera)" arguments=(("cuenta", numberlist)) )
