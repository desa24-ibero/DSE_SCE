 column=(type=long updatewhereclause=yes key=yes name=plan_estudios_cve_carrera dbname="plan_estudios.cve_carrera" )
 column=(type=long updatewhereclause=yes key=yes name=plan_estudios_cve_plan dbname="plan_estudios.cve_plan" )
 column=(type=datetime update=yes updatewhereclause=yes name=plan_estudios_fecha_acuerdo dbname="plan_estudios.fecha_acuerdo" )
 column=(type=long update=yes updatewhereclause=yes name=plan_estudios_num_acuerdo dbname="plan_estudios.num_acuerdo" )
 column=(type=char(9) update=yes updatewhereclause=yes name=plan_estudios_vigencia dbname="plan_estudios.vigencia" )
 column=(type=decimal(0) update=yes updatewhereclause=yes name=num_registro dbname="plan_estudios.num_registro" )
 retrieve="  SELECT convert(varchar(5),plan_estudios.cve_carrera)+
			convert(varchar(2),plan_estudios.cve_plan),
			plan_estudios.cve_carrera,   
         plan_estudios.cve_plan,   
         plan_estudios.fecha_acuerdo,   
         plan_estudios.num_acuerdo,   
         plan_estudios.vigencia,
			plan_estudios.num_registro  
    FROM plan_estudios  
   WHERE ( plan_estudios.cve_carrera = :carrera ) AND  
         ( plan_estudios.cve_plan = :plan )    
" update="plan_estudios" updatewhere=0 updatekeyinplace=yes arguments=(("carrera", number),("plan", number)) )
