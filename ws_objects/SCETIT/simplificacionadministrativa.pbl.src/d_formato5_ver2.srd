 column=(type=char(70) updatewhereclause=yes name=datos_certificado_nombre dbname="datos_certificado.nombre" )
 retrieve="  SELECT control_sep.cuenta,   
         control_sep.numero,   
         control_sep.fecha_emision,   
         alumnos.nombre_completo,   
         alumnos.sexo,   
         nacionalidad.nacionalidad,   
         carreras.carrera,   
         plan_estudios.fecha_acuerdo,   
         plan_estudios.num_acuerdo,   
         carreras.nivel,   
         planteles.cve_plantel,   
         control_sep.cve_carrera,   
         control_sep.cve_plan,
			control_sep.ciclos_cursados,
			control_sep.cve_doc_control_sep,
			datos_certificado.nombre
    FROM control_sep,   
         alumnos,   
         carreras,   
         nacionalidad,   
         plan_estudios,   
         planteles,
			datos_certificado  
   WHERE ( alumnos.cuenta = control_sep.cuenta ) and  
         ( control_sep.cve_carrera = carreras.cve_carrera ) and  
         ( alumnos.cve_nacion *= nacionalidad.cve_nacion ) and  
         ( control_sep.cve_carrera = plan_estudios.cve_carrera ) and  
         ( control_sep.cve_plan = plan_estudios.cve_plan ) and  
         ( ( control_sep.numero = :numero ) AND  
         ( planteles.actual = 1 ) AND
			( datos_certificado.id = 2 ))    
" update="control_sep" updatewhere=1 updatekeyinplace=yes arguments=(("numero", number),("certificado", number)) )
