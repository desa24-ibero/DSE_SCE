 column=(type=long updatewhereclause=yes name=horario_hora_final dbname="horario.hora_final" )
 column=(type=long updatewhereclause=yes name=grupos_cupo dbname="grupos.cupo" )
 column=(type=long updatewhereclause=yes name=grupos_inscritos dbname="grupos.inscritos" )
 column=(type=long updatewhereclause=yes name=grupos_forma_imparte dbname="grupos.forma_imparte" )
 column=(type=datetime updatewhereclause=yes name=grupos_fecha_inicio dbname="grupos.fecha_inicio" )
 column=(type=datetime updatewhereclause=yes name=grupos_fecha_fin dbname="grupos.fecha_fin" )
 column=(type=char(30) updatewhereclause=yes name=grupo_modalidad_descripcion dbname="grupo_modalidad.descripcion" )
 retrieve="SELECT	grupos.cve_mat,   
			materias.materia,   
			grupos.gpo,   
			horario.cve_dia,   
			horario.hora_inicio,   
			horario.hora_final,   
			grupos.cupo,   
			grupos.inscritos,
			grupos.forma_imparte,
			grupos.fecha_inicio,
			grupos.fecha_fin, 
			grupo_modalidad.descripcion 
FROM		grupos LEFT JOIN horario on	(
														( grupos.cve_mat = horario.cve_mat ) and
														( grupos.gpo = horario.gpo ) and 
														( grupos.periodo = horario.periodo ) and 
														( grupos.anio = horario.anio )
													) or
													(
														( grupos.cve_asimilada = horario.cve_mat ) and 
														( grupos.gpo_asimilado = horario.gpo ) and 
														( grupos.periodo = horario.periodo ) and 
														( grupos.anio = horario.anio )
													),
         materias, grupo_modalidad   
WHERE	( grupos.cupo > grupos.inscritos ) and  
			( grupos.cve_mat = materias.cve_mat ) and  
			( 
				( grupos.anio = :year  ) AND  
				( grupos.periodo = :Periodo ) AND  
				( grupos.cve_mat = :mat ) AND  
				( grupos.cond_gpo = 1 )
			)
AND grupo_modalidad.id_modalidad = grupos.modalidad " arguments=(("mat", number),("Periodo", number),("year", number))  sort="grupos_gpo A horario_cve_dia A horario_hora_inicio A horario_hora_final A " )
