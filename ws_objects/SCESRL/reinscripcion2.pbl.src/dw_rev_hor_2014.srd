table(column=(type=long updatewhereclause=yes name=cve_dia dbname="cve_dia" )
 column=(type=long updatewhereclause=yes name=hora_inicio dbname="hora_inicio" )
 column=(type=long updatewhereclause=yes name=hora_final dbname="hora_final" )
 column=(type=char(16) updatewhereclause=yes name=horario_cve_salon dbname="horario_cve_salon" )
 column=(type=long updatewhereclause=yes name=grupos_forma_imparte dbname="forma_imparte" )
 column=(type=datetime updatewhereclause=yes name=fecha_inicio dbname="fecha_inicio" )
 column=(type=datetime updatewhereclause=yes name=fecha_fin dbname="fecha_fin" )
 retrieve="
SELECT	cve_dia				= horario.cve_dia,   
			hora_inicio			= horario.hora_inicio,   
			hora_final			= horario.hora_final  ,
			horario_cve_salon	= horario.cve_salon,
			forma_imparte		= grupos.forma_imparte,
			fecha_inicio			= grupos.fecha_inicio,
			fecha_fin				= grupos.fecha_fin
FROM		horario,
			grupos  
WHERE	(
			(
				( grupos.cve_mat = horario.cve_mat)
				and
				( grupos.gpo = horario.gpo )
			)
			or
			(
				( grupos.cve_asimilada = horario.cve_mat )
				and
				( grupos.gpo_asimilado = horario.gpo )
			)
		)
		and
		( grupos.cve_mat		= :cvmat		) AND  
		( grupos.gpo			= :gpo		) AND  
		( horario.periodo		= :per		) AND  
		( horario.anio			= :year		) AND
		( grupos.periodo		= :per		) AND
		( grupos.anio			= :year		) AND 
		( grupos.sesionado	= 'N' )

UNION ALL

 SELECT	cve_dia				= horario_modular.cve_dia,   
			hora_inicio			= horario_modular.hora_inicio,   
			hora_final			= horario_modular.hora_final  ,
			horario_cve_salon	= horario_modular.cve_salon,
			forma_imparte		= grupos.forma_imparte,
			fecha_inicio			= horario_modular.fecha_inicio,
			fecha_fin				= horario_modular.fecha_fin
FROM		horario_modular,
			grupos  
WHERE	(
			(
				( grupos.cve_mat = horario_modular.cve_mat)
				and
				( grupos.gpo = horario_modular.gpo )
			)
			or
			(
				( grupos.cve_asimilada = horario_modular.cve_mat )
				and
				( grupos.gpo_asimilado = horario_modular.gpo )
			)
		)
		and
		( grupos.cve_mat				= :cvmat		) AND  
		( grupos.gpo					= :gpo		) AND  
		( horario_modular.periodo	= :per		) AND  
		( horario_modular.anio		= :year		) AND
		( grupos.periodo				= :per		) AND
		( grupos.anio					= :year		) AND 
		( grupos.sesionado			= 'S' )" arguments=(("cvmat", number),("gpo", string),("per", number),("year", number)) )
