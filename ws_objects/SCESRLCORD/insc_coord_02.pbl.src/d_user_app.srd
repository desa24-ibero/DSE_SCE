footer(height=1 color="536870912" )
detail(height=89 color="536870912" )
table(column=(type=long update=yes updatewhereclause=yes key=yes name=ss_user_app_app_skey dbname="ss_user_app.app_skey" )
 column=(type=char(40) updatewhereclause=yes name=pc_app_def_app_name dbname="pc_app_def.app_name" )
 column=(type=char(100) updatewhereclause=yes name=pc_app_def_description dbname="pc_app_def.description" )
 column=(type=long update=yes updatewhereclause=yes key=yes name=ss_user_app_user_skey dbname="ss_user_app.user_skey" )
 column=(type=char(10) updatewhereclause=yes name=pc_user_def_user_id dbname="pc_user_def.user_id" )
 column=(type=char(20) updatewhereclause=yes name=pc_user_def_first_name dbname="pc_user_def.first_name" )
 column=(type=char(20) updatewhereclause=yes name=pc_user_def_last_name dbname="pc_user_def.last_name" )
 column=(type=long update=yes updatewhereclause=yes name=ss_user_app_permiso_modificar dbname="ss_user_app.permiso_modificar" initial="0" )
 retrieve="  SELECT dbo.ss_user_app.app_skey,
			dbo.pc_app_def.app_name,   
         dbo.pc_app_def.description,   
         dbo.ss_user_app.user_skey,   
         dbo.pc_user_def.user_id,   
         dbo.pc_user_def.first_name,   
         dbo.pc_user_def.last_name,
			dbo.ss_user_app.permiso_modificar  
    FROM dbo.pc_app_def,   
         dbo.pc_user_def,   
         dbo.ss_user_app  
   WHERE ( dbo.ss_user_app.user_skey = dbo.pc_user_def.user_skey ) and  
         ( dbo.ss_user_app.app_skey = dbo.pc_app_def.app_skey ) and  
         ( ( ( dbo.pc_user_def.user_id like :user_id ) OR
			  ( :user_id = ~".~" ) ) AND
  			( ( dbo.pc_app_def.app_name like :app_name ) OR
			  ( :app_name = ~".~" ) ) AND
			( ( dbo.ss_user_app.permiso_modificar = :permiso_modificar ) OR
			  ( :permiso_modificar = 2 ) ) )    
" update="dbo.ss_user_app" updatewhere=0 updatekeyinplace=no arguments=(("user_id", string),("app_name", string),("permiso_modificar", number)) )
