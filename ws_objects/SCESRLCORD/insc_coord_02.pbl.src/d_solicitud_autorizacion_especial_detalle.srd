 retrieve="  SELECT alumnos.cuenta,   
         v_sce_alumno_digito.digito,   
         alumnos.nombre_completo,   
         academicos.cve_plan,   
         nombre_plan.nombre_plan,   
         academicos.cve_carrera,   
         carreras.carrera,   
         grupos.cve_mat,   
         grupos.gpo,   
         grupos.periodo,   
         grupos.anio,   
         grupos.cupo,   
         grupos.inscritos,   
         grupos.tipo,   
         tipo_grupo.nombre_tipo,   
         mat_prerrequisito.cursativa,   
         mat_prerrequisito.semestre_ideal,   
         mat_prerrequisito.clase_area,   
         mat_prerrequisito.optativa,   
         academicos.promedio,   
         academicos.sem_cursados,   
         academicos.creditos_cursados,
		cve_dia = 0,		
		horario = ' ',
		cve_mat_ligada = 0,
		gpo_ligado = '--',
		materia_ligada =  space(75),
		num_seriaciones = 0
       FROM   academicos inner join  alumnos on academicos.cuenta = alumnos.cuenta 
							  inner join v_sce_alumno_digito on alumnos.cuenta = v_sce_alumno_digito.cuenta
					           inner join carreras on academicos.cve_carrera = carreras.cve_carrera
							  inner join mat_prerrequisito on academicos.cve_carrera = mat_prerrequisito.cve_carrera 
									and  mat_prerrequisito.cve_plan =  academicos.cve_plan										
					           inner join nombre_plan on  academicos.cve_plan = nombre_plan.cve_plan
							  inner join grupos on mat_prerrequisito.cve_mat = grupos.cve_mat
						       inner join tipo_grupo on grupos.tipo = tipo_grupo.tipo

 	
   WHERE 
         ( ( academicos.cuenta = :an_cuenta ) AND  
         ( grupos.cve_mat = :an_cve_mat ) AND  
         ( grupos.gpo = :as_gpo ) AND  
         ( grupos.periodo = :an_periodo ) AND  
         ( grupos.anio = :an_anio ) )
order by grupos.cve_mat, grupos.gpo
" arguments=(("an_cuenta", number),("an_cve_mat", number),("as_gpo", string),("an_periodo", number),("an_anio", number)) )
