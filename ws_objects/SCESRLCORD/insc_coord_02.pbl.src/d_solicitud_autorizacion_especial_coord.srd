 retrieve="SELECT  	cuenta					= solicitud_autorizacion_espec.cuenta,
		digito 						= digito.digito,
		cve_mat					= solicitud_autorizacion_espec.cve_mat,
		materia					= materias.materia,
		gpo						= solicitud_autorizacion_espec.gpo,
		creditos			    = materias.creditos,
		horas					= materias.horas_reales,
		cve_estatus_autorizacion = estatus.cve_estatus_autorizacion,
		estatus_autorizacion	= estatus.estatus_autorizacion,
		remedial				= (select COUNT(cve_mat) from controlescolar_bd.dbo.mat_no_eliminar remedial WHERE remedial.cve_mat = solicitud_autorizacion_espec.cve_mat),
		texto_solicitud			= solicitud_autorizacion_espec.texto_solicitud,
		texto_respuesta			= solicitud_autorizacion_espec.texto_respuesta,
		periodo				 = solicitud_autorizacion_espec.periodo,
		anio					= solicitud_autorizacion_espec.anio,
		nombre_completo = alumnos.nombre_completo,
		cve_coordinacion  = materias.cve_coordinacion
	FROM controlescolar_bd.dbo.solicitud_autorizacion_espec solicitud_autorizacion_espec,
		controlescolar_bd.dbo.materias materias,
		controlescolar_bd.dbo.grupos grupos,
		web_bd.dbo.v_www_periodo_mat_inscritas periodo_mi,
		controlescolar_bd.dbo.estatus_autorizacion_especial estatus,
		controlescolar_bd.dbo.v_sce_alumno_digito digito,
		controlescolar_bd.dbo.alumnos alumnos
	WHERE ( solicitud_autorizacion_espec.cuenta =  digito.cuenta )
		AND ( solicitud_autorizacion_espec.cuenta =  alumnos.cuenta )
		AND ( solicitud_autorizacion_espec.cve_mat =  materias.cve_mat )
		AND ( solicitud_autorizacion_espec.anio = periodo_mi.anio)
		AND ( solicitud_autorizacion_espec.periodo = periodo_mi.periodo)
		AND ( solicitud_autorizacion_espec.cve_mat =  grupos.cve_mat)
		AND ( solicitud_autorizacion_espec.gpo =  grupos.gpo collate SQL_Latin1_General_CP1_CI_AS)
		AND ( grupos.periodo = periodo_mi.periodo) 
		AND ( grupos.anio = periodo_mi.anio)
		AND ( solicitud_autorizacion_espec.cve_estatus_autorizacion = estatus.cve_estatus_autorizacion)
		AND ( materias.cve_coordinacion = :al_cve_coordinacion OR :al_cve_coordinacion = 9999)
		ORDER BY solicitud_autorizacion_espec.cuenta,
				solicitud_autorizacion_espec.cve_mat" arguments=(("al_cve_coordinacion", number)) )
