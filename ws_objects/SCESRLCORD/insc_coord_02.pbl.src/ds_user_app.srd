 column=(type=char(16) update=yes updatewhereclause=yes name=security_user_app_user_name dbname="security_user_app.user_name" dbalias=".user_name" )
 column=(type=char(3) updatewhereclause=yes name=security_user_app_periodo_default dbname="security_user_app.periodo_default" dbalias=".periodo_default" )
 retrieve="  SELECT security_users.name,   
         security_apps.application,   
         security_user_app.permiso_modificar,   
         security_apps.description,   
         security_users.description,   
         security_user_app.app_name,   
         security_user_app.user_name,   
         security_user_app.periodo_default  
    FROM security_apps,   
         security_user_app,   
         security_users  
   WHERE ( security_apps.application = security_user_app.app_name ) and  
         ( security_users.name = security_user_app.user_name ) and  
         (security_user_app.app_name = :app_name OR :app_name = '.') AND  
         (security_user_app.user_name = :user_name OR :user_name = '.') AND  
         (security_user_app.permiso_modificar = :permiso_modificar OR :permiso_modificar = (2))    


" arguments=(("user_name", string),("app_name", string),("permiso_modificar", number)) )
