table(column=(type=long updatewhereclause=yes name=grupos_cve_mat dbname="grupos.cve_mat" )
 column=(type=char(2) updatewhereclause=yes name=grupos_gpo dbname="grupos.gpo" )
 column=(type=long updatewhereclause=yes name=grupos_cupo dbname="grupos.cupo" )
 column=(type=long updatewhereclause=yes name=grupos_inscritos dbname="grupos.inscritos" )
 column=(type=long updatewhereclause=yes name=profesor_cve_profesor dbname="profesor.cve_profesor" )
 column=(type=char(70) updatewhereclause=yes name=profesor_nombre_completo dbname="profesor.nombre_completo" )
 column=(type=long updatewhereclause=yes name=horario_cve_dia dbname="horario.cve_dia" )
 column=(type=char(9) updatewhereclause=no name=dia dbname="dia" )
 column=(type=char(65) updatewhereclause=no name=horario dbname="horario" )
 retrieve="  SELECT grupos.cve_mat,   
         grupos.gpo,   
         grupos.cupo,   
         grupos.inscritos,   
         profesor.cve_profesor,   
         profesor.nombre_completo,
		hr.cve_dia as cve_dia,
		
		 CASE ISNULL(hr.cve_dia,0) WHEN 1 THEN 'Lunes' WHEN 2 THEN 'Martes' WHEN 3 THEN 'Miércoles'
						WHEN 4 THEN 'Jueves' WHEN 5 THEN 'Viernes' WHEN 6 THEN 'Sábado'
						WHEN 7 THEN 'Domingo' ELSE 'Sin Día' END as dia,
		
		case len(cast(ISNULL(hora_inicio,0) as varchar)) 
		 when 1 then '0'+cast(ISNULL(hora_inicio,0) as varchar) 
		 else cast(ISNULL(hora_inicio,0) as varchar) end
		+' - '+ 
		case len(cast(ISNULL(hora_final,0) as varchar))
		 when 1 then '0' +cast(ISNULL(hora_final,0) as varchar) 
		 else cast(ISNULL(hora_final,0) as varchar) end  as horario    
    FROM grupos inner join profesor  on grupos.cve_profesor = profesor.cve_profesor
	 left join controlescolar_bd.dbo.horario as hr on		
								grupos.anio	= hr.anio
							and	grupos.periodo	= hr.periodo
							and	((grupos.cve_mat = hr.cve_mat and grupos.gpo = hr.gpo) 
									or 
			 					(grupos.cve_asimilada = hr.cve_mat and grupos.gpo_asimilado = hr.gpo))
   WHERE   ( ( grupos.cve_mat = :a_cve_mat ) AND  
		(grupos.gpo = :a_gpo ) AND
         ( grupos.periodo = :a_periodo ) AND  
         ( grupos.anio = :a_anio ) )   
ORDER BY grupos.cve_mat ASC,   
         grupos.gpo ASC, hr.cve_dia, hr.hora_inicio, hr.hora_final   " arguments=(("a_cve_mat", number),("a_gpo", string),("a_periodo", number),("a_anio", number)) )
